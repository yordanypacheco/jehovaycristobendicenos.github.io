                if (cTree.categoryList[path].length == 0) {
                    cTree.sendReq("getLastedFileList");
                } else {
                    cTree.openCloseLeaf(path, "showList", fileC);
                }
                fileC.currentPathFiles = cTree.categoryList[path];
                module.clearSelected();

                return;
            case fileC.rootCategory[1].path:
                if (cTree.categoryList[path].length == 0) {
                    cTree.sendReq("getBigFileList");
                } else {
                    cTree.openCloseLeaf(path, "showList", fileC);
                }
                fileC.currentPathFiles = cTree.categoryList[path];
                module.clearSelected();

                return;
            case fileC.rootCategory[2].path:
                if (cTree.categoryList[path].length == 0) {
                    cTree.sendReq("getZipFileList");
                } else {
                    cTree.openCloseLeaf(path, "showList", fileC);
                }
                fileC.currentPathFiles = cTree.categoryList[path];
                module.clearSelected();

                return;
            case fileC.rootCategory[3].path:
                if (cTree.categoryList[path].length == 0) {
                    cTree.sendReq("getAPKFileList");
                } else {
                    cTree.openCloseLeaf(path, "showList", fileC);
                }
                fileC.currentPathFiles = cTree.categoryList[path];
                module.clearSelected();

                return;
            default:
                if (eType == "click") {
                    fileC.currentPathFiles = [];
                    var croot = document.getElementById(path);
                    var $c = $(croot);
                    if ($($c.find("div").get(0)).hasClass("category_close")) cTree.openCloseLeaf(path, "showTree", fileC);
