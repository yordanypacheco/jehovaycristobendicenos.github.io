define('drag',["avalon",
        "jquery","tools"],
function(avalon,
		$ ,tools){
		function drag(id , mousedown , mouseup , mousemove , mousewheel){
			//if(id != "string") return;
			var dom = document.getElementById(id);
			this._end_x = 0;
			this._end_y = 0;
			this.flag = false;
			this.el = dom;
			this.mousemove =mousemove ? mousemove : this.mousemove;
		//	this.mouseup = mouseup;
			this.mouseup = mouseup ? mouseup : this.mouseup;
		
			dom.addEventListener("mousedown" , mousedown ? mousedown.bind(this) : this.mousedown.bind(this));
			dom.addEventListener("DOMMouseScroll" , mousewheel ? mousewheel.bind(this) : this.mousewheel.bind(this));
			dom.addEventListener("mousewheel" , mousewheel ? mousewheel.bind(this) : this.mousewheel.bind(this));
			dom.addEventListener("mousemove" , mousemove ? mousemove.bind(this) : this.mousemove.bind(this));
			document.addEventListener("mouseup" , mouseup ? mouseup.bind(this) : this.mouseup.bind(this));
		//	document.addEventListener("mouseup" , this.mouseup1.bind(this));
		}
		
		drag.prototype.mousewheel = function(){
			//this.flag = false;
		};
		drag.prototype.keypress = function(e){
			tools.logs("e:" , keypress);
		};
		drag.prototype.mousedown = function(e){
			//document.addEventListener("mousemove" , this.mousemove.bind(this));
			//document.addEventListener("mouseup" , this.mouseup.bind(this));
			if(e.button == 2)
				return;
			
			this.flag = true;
			this.el.style.cursor ="url(srcimg/ic_1.gif),auto";
			this._start_x = parseInt(this.el.style.left , 10);
			this._start_y = parseInt(this.el.style.top , 10);
			//tools.logs("mousedown:" ,this._start_x  , this._start_y , this)
		}
		
		drag.prototype.mouseup1 = function(e){
			console.log("mouseup1")
		}
		
		drag.prototype.mouseup = function(e){
			this.flag = false;
			this.el.style.cursor ="url(srcimg/ic_hand2.gif),auto";
		}
		
		drag.prototype.mousemove = function(e){
			
			if(e.button == 2)
				return;
			
			if(this.flag){
				var x = e.pageX -this._end_x;
				var y = e.pageY - this._end_y ;
				this.el.style.left = (parseInt(this.el.style.left,10) + x) + "px";
				this.el.style.top = (parseInt(this.el.style.top,10) + y) + "px";
				this.el.style.cursor ="url(srcimg/ic_1.gif),auto";
			}else{
				this.el.style.cursor ="url(srcimg/ic_hand2.gif),auto";
			}
			this._end_x = e.pageX;
			this._end_y = e.pageY;
			
		}
	return drag;	
});
