define('swipe',['tools'], function(tools){
	var swipe = {
		touchNames: [],
		touchPos: {
			'start' : {							// start coordinate
				x: 0,
				y: 0
			},
			'end': {								// end coordinate
				x: 0,
				y: 0
			}
		},
		startTime: null,
		elapsedTime: null,
		allowedTime: 300,
		touchSupported: false			// 
	};
	
	swipe.bindEvent = function(){
		tools.addEvt(swipe.container, swipe.touchNames[0], swipe.handleTouchStart);
		tools.addEvt(swipe.container, swipe.touchNames[1], swipe.handleTouchMove);
		tools.addEvt(swipe.container, swipe.touchNames[2], swipe.handleTouchEnd);
		if(swipe.touchNames[3]){
			tools.addEvt(swipe.container, swipe.touchNames[3], swipe.handleTouchCancel);
		}
	};
	
	swipe.swipeDirection = function(){
		var sx = swipe.touchPos.start.x;
		var sy = swipe.touchPos.start.y;
		var ex = swipe.touchPos.end.x;
		var ey = swipe.touchPos.end.y;
		
		return Math.abs(sx - ex) >= Math.abs(sy - ey) ? (sx - ex > 0 ? 'left' : 'right') : (sy - ey > 0 ? 'up' : 'down');
	};
	
	swipe.getCoordinates = function(evt){
		evt.stopPropagation();
		
		var touches = evt.targetTouches && evt.targetTouches.length ? evt.targetTouches[0] : [evt];
		var e = evt.changedTouches ? evt.changedTouches[0] : touches[0];
		
		return {
			x: e.clientX,
			y: e.clientY
		}
	};
	
	swipe.handleTouchStart = function(evt){
		evt.stopPropagation();
		evt.preventDefault();
		
		swipe.touchPos.start = swipe.getCoordinates(evt);
		
		swipe.startTime = new Date().getTime();
		
		// console.log('swipe.touchStart ... ' + 'x = ' + swipe.touchPos.start.x, 'y = ' + swipe.touchPos.start.y);
	};
	
	swipe.handleTouchMove = function(evt){
		evt.stopPropagation();
		evt.preventDefault();
		
		swipe.touchPos.end = swipe.getCoordinates(evt);
		// console.log('swipe.touchMove ... ', 'x = ' + swipe.touchPos.end.x, 'y = ' + swipe.touchPos.end.y);
	};
	
	swipe.handleTouchEnd = function(evt){
		// console.log("touch end");
		evt.stopPropagation();
		evt.preventDefault();
		
		var sx = swipe.touchPos.start.x;
		var sy = swipe.touchPos.start.y;
		var ex = swipe.touchPos.end.x;
		var ey = swipe.touchPos.end.y;
		var totalX = Math.abs(sx - ex);
		var totalY = Math.abs(sy - ey);
		
		swipe.elapsedTime = new Date().getTime();
		
		//  && ((swipe.elapsedTime - swipe.startTime) > swipe.allowedTime)
		if((totalX > 20 || totalY > 20)){
			// console.info('more than 30 for moving');
			swipe.touchPos.end = swipe.getCoordinates(evt);
			
			switch(swipe.swipeDirection()){
				case 'left':
					swipe.swipeLeft();
					break;
				case 'right':
					swipe.swipeRight();
					break;
				case 'up':
					swipe.swipeUp();
					break;
				case 'down':
					swipe.swipeDown();
					break;
				default:
					break;
			}
		}
		
		// console.log('swipe.touchEnd ... ', 'x = ' + swipe.touchPos.end.x, 'y = ' + swipe.touchPos.end.y);
	};
	
	swipe.handleTouchCancel = function(evt){
		if(evt){
			evt.stopPropagation();
		}
		
		swipe.touchPos.end = swipe.getCoordinates(evt);
		// console.log('swipe.touchCancel ...', 'x = ' + swipe.touchPos.end.x, 'y = ' + swipe.touchPos.end.y);
	};
	
	// {'left': left, 'right': right, 'up': up, 'down': down}
	swipe.init = function(ops){
		if(!tools.isAny())
			return;
		
		if(ops){
			swipe.swipeLeft = ops.left;
			swipe.swipeRight = ops.right;
			swipe.swipeUp = ops.up;
			swipe.swipeDown = ops.down;
			swipe.container = ops.div;
		}
		
		var IE11touch = navigator.pointerEnabled;
		var IE9_10touch = navigator.msPointerEnabled;
		var w3ctouch = (function(){
			var supported = tools.isAny() || false;
			try{
				var div = document.createElement('div');
				div.ontouchstart = function(){
					supported = true;
				}
				var e = document.createElement('TouchEvent');
				e.initUIEvent('touchstart', true, true);
				div.dispatchEvent(e);
			}catch(err){
				
			}
			div = div.dispatchEvent = null;
			
			return supported;
		})();
		
		swipe.touchSupported = !!(IE11touch || IE9_10touch || w3ctouch);
		
		swipe.touchNames = ["mousedown", "mousemove", "mouseup", ""];
		if(IE11touch){
			swipe.touchNames = ["pointerdown", "pointermove", "pointerup", "pointercancel"];
		}else if(IE9_10touch){
			swipe.touchNames = ["MSPointerDown", "MSPointerMove", "MSPointerUp", "MSPointerCancel"];
		}else if(w3ctouch){
			swipe.touchNames = ["touchstart", "touchmove", "touchend", "touchcancel"];
		}
		
		swipe.bindEvent();
	};
	
