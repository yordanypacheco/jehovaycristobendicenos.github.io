    module.api = tools.scrollInitNotAuto("appModule_file_list");
  };

  module.load = function() {
    module.time = null;
    Config.Runtime.isWepai === false ? module.loadLocalList() : module.loadWebList();
  };

  module.loadLocalList = function() {
    if (appModuleController.appList.length > 0) {
      //			Config.Runtime.eventBus.emit('appList',{'data':'0'});
      module.api.reinitialise();
      return;
    } else {
      var m = {};
