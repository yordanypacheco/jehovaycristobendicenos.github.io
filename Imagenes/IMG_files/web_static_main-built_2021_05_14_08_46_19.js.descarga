define('fileUpFlag',["config",
		"tools"
        ], 
    function(Config,
             tools
    		){
	
	var Mrg = {
      
    fileMgr: function(vm , files , listTree){
    	var arr=[];
    	for(var i=0 ; i<files.length ; i++){
    		var fpush = {
    				upId:tools.guidGenerator() + "/" + files[i].name
    		}
  			 
    		if(files[i]['group']){
    			fpush["upId"] = files[i]['group'] + '&' + fpush["upId"];
    		}

    		this.file(vm , files[i] , fpush, listTree)

    		arr.push({
    			files: [files[i]],	// 以数组形式存储一个文件到这个对象里
    			fpush: fpush
    		});
    	}

    	return arr;
    },

	file: function(vm , file , fpush , listTree){

		type="file";
  		fpush['path'] = file.savePath + "/" + file.name;
  		fpush['isFile'] = "true"; //file.isFile;
  		fpush['size'] = file.size;
  		fpush['name'] = file.name;

  		var ftype = file['name'].substring(file['name'].lastIndexOf('.'));
  		fpush['type'] = tools.getFileType(ftype);

  		if(file['dragPath']){
  			fpush['isFile'] = "false"; //file.isFile;
  			
  			vm.uploadFolder[file['group']]['total'] = parseInt(vm.uploadFolder[file['group']]['total']) + parseInt(file["size"])/1024
  			vm.uploadFolder[file['group']]['size'] = parseInt(vm.uploadFolder[file['group']]['size']) + parseInt(file["size"])/1024
 
  			if(vm.uploadFolder[file['group']]['newName']){
  				if(file["dragPath"].indexOf('/') > 0){
  					file["dragPath"] = vm.uploadFolder[file['group']]['newName'] + file['dragPath'].substring(file['dragPath'].indexOf("/"));
  				}else{
  					file["dragPath"] = vm.uploadFolder[file['group']]['newName'] ;
  				}
  			}

  			fpush['savePath'] = file.savePath + ":/" + file["dragPath"];
  			
  			if(vm.uploadFolder[file['group']]['val'] > 1){
  				vm.uploadFolder[file['group']]['el'].innerHTML = vm.uploadFolder[file['group']]['val'];
  			}else{
  				fpush['name'] =  file['dragPath'] ;
  				if(!fpush['isFile']) listTree.pathFileList[vm.currentPath].splice(0, 0, fpush);
  				
	      		vm.currentPathFiles.splice(0, 0, fpush);
	      		vm.uploadFolder[file['group']]['name'] = fpush["upId"];
	 
