define('mainPdf',["avalon", "text!../template/pdf.html", "Animation", "config", "tools"], function(avalon, tpl_pdf, animation, Config, tools) {
    //	define(["avalon"], function(avalon) {
    //console.log("anim" , Animation)
    var module = {};
    module.init = function(container) {
        container.innerHTML = tpl_pdf;

        //$(container).append(tpl_pdf);
    };

    var pdf = {
            hqx_DEFAULT_URL: "",
            Channel: "",
            flag: false,
            init: function() {
                //console.log("arguments:" , arguments);
                //container.innerHTML = tpl_pdf;
                this.Channel = arguments[0];
                $("#ClosePdf").die().live("click", pdf.closePdf);
                //document.getElementById("ClosePdf").click(pdf.closePdf);
                this.Channel.on("pdfaction", this.readOrder);
                this.Channel.on("pdfclose", this.closePdf);
                this.Channel.on("fileurl", this.onFileurl);
                this.Channel.on("pdfFlag", this.onPdfFlag)
            },

            onPdfFlag: function() {
                this.flag = true;
            },

            closePdf: function() {
                document.getElementById("pdf_hellod").src = "";

                animation("CloseWin", {
                    opacity: 0
                }, 200, 'Tween.Circ.easeInOut', {
                    start: function(el) {},
                    complete: function(el) {
                        $("#CloseWin").css("display", "none");
                    }
                })

            },
            openUrl: function(url) {
                if (/^[a-z][a-z0-9+\-.]*:/i.test(url)) {} else {
                    url = window.location.href + url;
                }
                require(["Animation"], function(animation) {

                    var div = animation("CloseWin", {
                        opacity: 1
                    }, 200, 'Tween.Circ.easeInOut', {
                        start: function(el) {},
                        complete: function(inst) {}
                    })
                })

                tools.hqxCloseBlock(Config.block, "sendFilePdf");

                document.getElementById("CloseWin").style.display = "block";
                document.getElementById("pdf_hellod").src = "pdf/web/viewer.html";
                var fun = function() {
                    setTimeout(function() {

                            if (document.getElementById("pdf_hellod").contentWindow.runPdf) {

                                document.getElementById("pdf_hellod").contentWindow.runPdf(url);

                            } else {
                                fun();
                            }
                        },
                        300
                    )
                }

                fun();
            },
            onFileurl: function(data) {
                //console.log("open pdf data.url:" , data.url)
                var typeArr = data.url.split('.');
                var type = typeArr[typeArr.length - 1];
                if (type == "pdf" && this.flag) {
                    this.flag = false;
                    pdf.openUrl(data.url);


                    //Config.Runtime.eventBus.emit('imageclose',{});
                }
            },
            /*接受channel发送过来的命令*/
            readOrder: function(url) {
                //console.log("url_open Pdf:" , url);
                document.getElementById("pdf_hellod").contentWindow.changePage(url);
            }

        }
        //	module.avalon = pdf;
        //return pdf;

    module.pdf = pdf;
    return module;
    //return pdf;

});

