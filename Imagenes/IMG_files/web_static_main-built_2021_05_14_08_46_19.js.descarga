	 * settings = {title, content, buttonCounts, fn()}
	 */
	TipMsg.prototype.init = function(settings){

		var self = this;
		
		if(settings != undefined){
			this.settings = settings;
		}

		this.fnParam = this.settings.fnParam ? this.settings.fnParam : null;
		this.title = this.settings.title ? this.settings.title : this.title;
