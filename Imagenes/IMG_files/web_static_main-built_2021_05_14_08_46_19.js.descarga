define('feedback',[
        "avalon", 
        "tools", 
        "config", 
        "text!../template/feedback.html", 
        "appLanguage"
        ], 
function(
		avalon, 
		tools, 
		Config, 
		tpl_feedback, 
		appLanguage
		){
	
	var module = {};
	module.api = null;
	module.init = function(container){
		container.innerHTML = tpl_feedback;
		
		var settings = {};
		var pane = $("#session_list").jScrollPane();
		pane.jScrollPane(settings);
		
		module.api = pane.data('jsp');
	};
	module.unload = function(){
		feedbackController.feedbackList = [];
	};
	
    var feedbackController = avalon.define("feedback", function(vm){
		vm.visible = false;
		vm.content = '';
		tools.logs(appLanguage)
		vm.lastContent = appLanguage["changeLang"]["feedbackInput"];
		vm.curTime = "";
		vm.feedbackList = [];
		
		vm.keyupSub = function(e){
			var self = feedbackController;
			if(e.keyCode == "13"){
				self.subFeedback(e);
			}
		};
		
		vm.focusInput = function(e){
			var self = feedbackController;
			$(this).addClass("selected");
			$(this).attr("value", "");
		};
		
		vm.blurInput = function(e){
			var self = feedbackController;
			$(this).removeClass("selected");
			$(this).attr("value", self.content);
		};
		
		vm.subFeedback = function(e){
			e.stopPropagation();
			var self = feedbackController;
			if(self.content == ""){
				return;
			}
			if(self.content == self.lastContent){
				if(appLanguage["changeLang"]["feedbackInput"] == self.content){
					return;
				}else{
					self.content = appLanguage["changeLang"]["feedbackTip1"];
				}
				return;
			}
			self.lastContent = self.content;
			
			var t = tools.getDate("yyyy-MM-dd");
			tools.logs("self.curTime = " + self.curTime);
			if(t == self.curTime){
				t = "";
			}
			
			var m = {};
			m.type = "feedback";
			m.data = {"type": "0", "time": t, "content": self.content};
			tools.sendMsgToChannel(m);
			
			self.content = "";
			
//			self.feedbackList.set(self.feedbackList.length, m.data);
			self.feedbackList.push(m.data);
			
//			self.getFeedbackList();
			
//			Config.Runtime.eventBus.emit('feedbackCount',{infoCount:0});
		};
		
		vm.getFeedbackList = function(){
			var m = {};
			m.type = "requestFeedback";
			m.data = {};
			tools.sendMsgToChannel(m);
		};
		
		vm.onFeedbackList = function(data){
			var self = feedbackController;
			self.feedbackList.length = 0;
			self.curTime = "";
			
			if(data.length != 0){
				self.curTime = data[0].time;
			}
			
			for(i in data){
				data[i].time = data[i].time.substring(0, data[i].time.indexOf(" "));
				if(self.curTime == data[i].time){
					data[i].time = "";
				}else{
					self.curTime = data[i].time;
					continue;
				}
			}
			self.feedbackList = data;
			
		};
		
		vm.onVisibleFeedback = function(data){
			var self = feedbackController;
			
			if(data.visible){
				self.content = appLanguage["changeLang"]["feedbackInput"];
				self.getFeedbackList();
			}
			
			self.visible = data.visible;
		};
		
		vm.onNewFeedback = function(data){
			var self = feedbackController;
			
			if(data.length == 0){
				self.getFeedbackList();
			}else{
				return;
			}
		};
		
		vm.onFeedback = function(data){
			var self = feedbackController;
			var t = data.time.substring(0, data.time.indexOf(" "));
			data.time = t;
			if(self.curTime == t){
				data.time = "";
			}
			self.feedbackList.push(data);
		};
		
		vm.initListeners = function(bus){
			var self = feedbackController;
			bus.on("feedbackList", self.onFeedbackList);
			bus.on("visibleFeedback", self.onVisibleFeedback);
			bus.on('newFeedback', self.onNewFeedback);
			bus.on('feedback', self.onFeedback);
		};
	});
    
    feedbackController.feedbackList.$watch("length", function(){
    	module.api.reinitialise();
    	module.api.scrollToBottom();
    });
	//avalon.scan()
	return module;
});

