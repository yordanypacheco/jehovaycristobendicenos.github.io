], function(avalon, $, jScrollPane, tpl_appModule, tools, Config, EventBus, Channel, upModel, downloader, tipMsg, appLanguage) {
  var module = {};
  module.name = "appModule";
  module.title = appLanguage["changeLang"]["appModule"];
  module.fileType = "app";
  module.appTypes = {
    ".apk": true
  };
