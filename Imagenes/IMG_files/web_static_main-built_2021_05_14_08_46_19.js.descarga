        vm.clickSort = function(v, e) {
            var self = musicModuleController;

            var e = e || window.event;
            var es = e.target || e.srcElement;
            var evt = $(es).closest("li");

            var selectedPaths = [];

            for (var i = 0; i < self.selectedItem.length; i++) {
                selectedPaths.push(self.musicList[self.selectedItem[i]].path);
            }

            var sortSelected = function(musArr) {
                self.selectedClass = [];
                var index = 0;
                for (var j = 0; j < selectedPaths.length; j++) {
                    for (var i = 0; i < musArr.length; i++) {
                        if (musArr[i].path == selectedPaths[j]) {
                            self.selectedItem[index] = i;
                            self.selectedClass.set(i, "selected");
                            index++;
                        }
                    }
                }
            };

            if ($(evt).hasClass("pressed")) {
                $(evt).removeClass("pressed");
                $(evt).addClass("pressed");
            } else {
                $(evt).addClass("pressed");
            }

            var musicArray = self.musicList.slice(0);
            if (v == "name") {
                self.musicSort = 1;
            } else {
                self.musicSort = 2;
            }
            if (self.sortKey[v] != undefined) {
                if (self.sortKey[v] == "des") {
                    self.sortKey[v] = "asc";

                    musicArray.sort(function(a, b) {
                        return a[v].localeCompare(b[v]);
                    });
                    $(evt)
                        .find("span")
                        .last()
                        .removeClass()
                        .addClass("order_up");
                } else {
                    self.sortKey[v] = "des";
                    musicArray.sort(function(a, b) {
                        return b[v].localeCompare(a[v]);
                    });
                    $(evt)
                        .find("span")
                        .last()
                        .removeClass()
                        .addClass("order_down");
                }
            } else {
                self.sortKey[v] = "asc";
                musicArray.sort(function(a, b) {
                    return a[v].localeCompare(b[v]);
                });
                $(evt)
                    .find("span")
                    .last()
                    .removeClass()
                    .addClass("order_up");
            }

            self.musicList = musicArray;
            sortSelected(musicArray);

            for (var i = 0; i < self.musicList.length; i++) {
                if (self.curPath == self.musicList[i].path) {
                    self.curIdx = i;
                }
            }
        };

