        "appLanguage"], 
function(avalon, 
		Config, 
		tools, 
		debug, 
		tpl_recordsCenter,
		tipMsg,
		appLanguage){
	
	var module = {};
	module.api = null;
	module.listType = 'all'; // 'missed'
	module.allList = [];
	module.missedList = [];
	module.seqIcons = {};
	module.barButtons = [];
	module.isVisible = false;
	module.fileType = 'records';
	module.time = null;
	
	module.thead = {"name": appLanguage["changeLang"]["name"], "num": appLanguage["changeLang"]["phoneNumber"], "time": appLanguage["changeLang"]["time"], "duration": appLanguage["changeLang"]["duration"]};

	module.init = function(container){
		container.innerHTML = tpl_recordsCenter;
		module.setLanguage();
		
		module.api = tools.scrollInitNotAuto('records_list');
	};
	
	module.setLanguage = function(){
		module.barButtons = module.getButtons();
		
		if(module.isVisible && module.fileType == Config.Runtime.subModule){
			Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: "records"});
		}
	};
	
	module.getButtons = function(){
		return [
                {"code": "delete_disable", "name": "", "title": appLanguage["changeLang"]["delete"]},
                {"code": "refresh", "name": "", "title": appLanguage["changeLang"]["fresh"]}
               ];
	};
	
	module.requestCallList = function(){
		module.isVisible = true;
		
		if(module.isVisible && module.fileType == Config.Runtime.subModule){
			Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: "records"});
		}
		
		var m = {};
		m.type = 'requestCallLogs';
		m.data = 'records';
		tools.sendMsgToChannel(m);
	};

	module.load = function(){
		var records = recordsCenterController;
		
		module.time = null;
		Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: "records"});
		if(records.callList.length > 0){
			Config.Runtime.eventBus.emit('smsIsEmpty', false);
			return;
		}else{
			module.requestCallList();
		}
	};
	
	module.loadMissed = function(){
		var m = {};
		m.type = 'requestCallLogsIgnore';
		m.data = 'records';
		tools.sendMsgToChannel(m);
	};
	
	module.timeOut = function(){
		tools.logs("timeout img");
		
		var r = recordsCenterController;
		r.callList.clear();
		r.curIdx = 0;
		
		r.isSelectAll = false;
		
		r.selectedItem.clear();
		r.selectedClass.clear();
		
		r.icons.clear();
		
		module.time = null;
	};
	
	module.unload = function(){                                                                                    
		module.time = Date.parse(new Date());
	};
	
	module.refresh = function(){
		var records = recordsCenterController;
		
		records.callList = [];
		records.curIdx = -1;
		records.isSelectAll = false;
		
		module.clearSelected();
		
		if(module.listType == 'all'){
			module.requestCallList();
		}else{
			module.loadMissed();
		}
	};
	
	module.onResize = function(){
		module.api.reinitialise();
	};
	
	module.clearSelected = function(){
		var records = recordsCenterController;
		records.selectedItem.splice(0, records.selectedItem.length);
		records.selectedClass = [];
	};
	// 
	module.deleteOneRecord = function(){
		var records = recordsCenterController;
		var args = new Array(records.callList[records.curIdx].id);
		
		var deleteOne = function(){
			records.callList.splice(records.curIdx, 1);
			
			if(records.selectedClass[records.curIdx] == 'selected'){
				records.selectedClass.splice(records.curIdx, 1);
				for(var i = 0; i < records.selectedItem.length; i++){
					if(records.selectedItem[i] === records.curIdx){
						records.selectedItem.splice(i, 1);
					}
				}
			}
			
			module.api.reinitialise();
			
			var m = {};
			m.type = "deleteCallLogs";
			m.data = args;
			tools.sendMsgToChannel(m);
			
			records.isSelectAll = false;
		};
		
		$("#records_list ul:eq(" + records.curIdx + ")").slideUp(300, function(){
			deleteOne();
		});
	};
	// 
	module.deleteRecords = function(){
		var records = recordsCenterController;
		var args = new Array();
		
		
		var deleteRecords = function(){
			records.selectedItem.sort(function(v1, v2){
				return v2 - v1;
			});
			
			for(var i = 0; i < records.selectedItem.length; i++){
				args.push(records.callList[records.selectedItem[i]].id);
				//console.log("args i  = " + args[i]);
				records.callList.splice(records.selectedItem[i], 1);
			}
			records.selectedItem.splice(0, records.selectedItem.length);
			records.selectedClass.splice(0, records.selectedClass.length);
			
			module.api.reinitialise();
			
			var m = {};
			m.type = "deleteCallLogs";
			m.data = args;
			tools.sendMsgToChannel(m);
			
			records.isSelectAll = false;
			
			tools.tipMsg(appLanguage['changeLang']["deleteRecordsSuccess"]);
		};
		
		var finished = false;
		$("#records_list ul.selected").slideUp(300, function(){
			if(!finished){
				deleteRecords();
				finished = true;
			}
		});
	};
	
	module.setListIcon = function(data){
		var records = recordsCenterController;
		module.defaultPath = '/contacticon/-1';
		module.defaultIcons = [];
//		module.contactIcons = [];
		var paths = [];
		
		for(var i = 0; i < data.length; i++){
			module.seqIcons[data[i].icon] = [];
		}
		
		for(var i = 0; i < data.length; i++){
//			console.log("data[" + i + "].icon = " + data[i].icon);
			if(data[i].icon == module.defaultPath){
				module.defaultIcons.push(i);
			}else{
//				module.contactIcons.push(i);
//				module.seqIcons[data[i].icon] = i;
				module.seqIcons[data[i].icon].push(i);
				paths.push(data[i].icon);
			}
		}
		
		if(module.defaultIcons.length > 0){
			module.requestIcons([module.defaultPath]);
		}
		
		if(paths.length > 0){
			module.requestIcons(paths);
		}
	};
	
	module.requestIcons = function(pathArr){
		var m = {};
		m.type = 'requestFile';
		m.data = pathArr;
		tools.sendMsgToChannel(m);
	};
	
	module.setListItemStyle = function(len){
		var r = recordsCenterController;
		var last = r.selectedItem[len];
		var beforeEle = last - 1;
		var afterEle = last + 1;
		
		if(r.selectedItem.length < 2){
			return;
		}else{
			if(r.selectedClass[beforeEle]){
				r.selectedTop.set(last, 'noTop');
			}
			if(r.selectedClass[afterEle] && afterEle < r.callList.length){
				r.selectedTop.set(afterEle, 'noTop');
			}
		}
	};
	
	module.clearListItemStyle = function(len){
		var r = recordsCenterController;
		var last = len;
		var afterEle = last + 1;
		
		if(r.selectedItem.length < 2){
			return;
		}else{
			if(r.selectedClass[afterEle]){
				r.selectedTop.set(last, null);
				r.selectedTop.set(afterEle, null);
			}else{
				r.selectedTop.set(last, null);
			}
		}
	};
	
	var recordsCenterController = avalon.define("recordsCenter", function(vm){
		vm.callList = [];
		vm.curIdx = -1;
		vm.thead = module.thead;
		
		vm.isSelectAll = false;
		
		vm.selectedTop = [];
		vm.selectedItem = [];
		vm.selectedClass = [];
		
		vm.icons = [];
		
		vm.selectAll = function(){
			var self = recordsCenterController;
			
			if(self.isSelectAll){
				self.isSelectAll = false;
				module.clearSelected();
			}else{
				self.selectedItem = [];
				var newArr = [];
				var noTop = [];
				var sarr = [];
				
				for(var i=0; i<self.callList.length; i++){
					newArr.push(i);
					sarr.push("selected");
					
					if(i > 0){
						noTop.push("noTop");
					}else{
						noTop.push(null);
					}
				}
				self.selectedTop = noTop
				self.selectedItem = newArr;
				self.selectedClass = sarr;
				self.isSelectAll = true;
			}
		};
		
		vm.clickItem = function(idx, e){
			var self = recordsCenterController;
			
			var e = e || window.event;
			e.stopPropagation();
			
			if(self.selectedClass[idx] == "selected"){
				self.selectedItem.remove(idx);
				self.selectedClass.set(idx, null);
			}else{
				for(var i = 0; i < self.selectedClass.length; i++){
					self.selectedClass.set(i, null);
				}
				self.selectedItem.splice(0, self.selectedItem.length);
				
				self.selectedItem.push(idx);
				self.selectedClass.set(idx, "selected");
			}
		};
		
		vm.clickCheckbox = function(idx, e){
			var self = recordsCenterController;
			
			var e = e || window.event;
			e.stopPropagation();
			
			if(self.selectedClass[idx] == "selected"){
				self.selectedItem.remove(idx);
				self.selectedClass.set(idx, null);
				self.selectedTop.set(idx, null);
				module.clearListItemStyle(idx);
			}else{
				self.selectedItem.push(idx);
				self.selectedClass.set(idx, "selected");
				module.setListItemStyle(self.selectedItem.length - 1);
			}
		};
		
		vm.clickAddContact = function(idx, e){
			var self = recordsCenterController;
			
			var e = e || window.event;
			e.stopPropagation();
			
			self.curIdx = idx;
			
			var m = {};
			m.phone = self.callList[idx].phone;
			Config.Runtime.eventBus.emit('addContactFromRecords', m);
		};
		
		vm.clickCallPhone = function(idx, e){
			var self = recordsCenterController;
			
			var e = e || window.event;
			e.stopPropagation();
			
			self.curIdx = idx;
			
			Config.Runtime.eventBus.emit('callPhone',{data: self.callList[idx]});
		};
		
		vm.clickDeleteItem = function(idx, e){
			var records = recordsCenterController;
			
			var e = e || window.event;
			e.stopPropagation();
			
			records.curIdx = idx;
			var t = new tipMsg({"title": appLanguage["changeLang"]["deleteRecords"], "content": appLanguage["changeLang"]["deleteRecordsDes"], "fn": module.deleteOneRecord, "tipTheme": "red_the"});
		};
		
		vm.onCallLogsDetails = function(data){
			var self = recordsCenterController;
			if(data.callLogs.length < 1){
				Config.Runtime.eventBus.emit('smsIsEmpty', true);
				return;
			}else{
				Config.Runtime.eventBus.emit('smsIsEmpty', false);
				self.callList = data.callLogs;
				module.setListIcon(self.callList);
				
				setTimeout(function(){module.api.reinitialise();}, 200);
			}
		};
		
		vm.onLoadMessage = function(d){
			if(d.data == "records"){
				module.load();
			}
		};
		
		vm.onCallListType = function(d){
			var self = recordsCenterController;
			module.listType = d.data;
			if(d.data == 'all'){
				if(module.allList.length == 0){
					self.callList = [];
					module.load();
				}else{
					module.missedList = self.callList;
					self.callList = [];
					self.callList = module.allList;
					
					module.api.reinitialise();
				}
			}else{
				if(module.missedList.length == 0){
					self.callList = [];
					module.loadMissed();
				}else{
					module.allList = self.callList;
					self.callList = [];
					self.callList = self.missedList;
					
					module.api.reinitialise();
				}
			}
		};
		
		vm.onCallLogsIgnore = function(d){
			var self = recordsCenterController;
			
			if(d.callLogs.length < 1){
				Config.Runtime.eventBus.emit('smsIsEmpty', true);
				return;
			}else{
				Config.Runtime.eventBus.emit('smsIsEmpty', false);
				self.callList = d.callLogs;
				
				module.api.reinitialise();
				return;
			}
			
		};
		
		vm.onFileUrl = function(d){
			var self = recordsCenterController;
			
			if(module.defaultPath == d.path){
				for(var i = 0; i < module.defaultIcons.length; i++){
					self.icons.set(module.defaultIcons[i], d.url);
				}
				return;
			}
			
			if(module.seqIcons[d.path] != undefined){
//				self.icons.set(module.seqIcons[d.path], d.url);
				for(var i = 0; i < module.seqIcons[d.path].length; i++){
					self.icons.set(module.seqIcons[d.path][i], d.url);
				}
				
				delete module.seqIcons[d.path];
			}
		};
		
		vm.onLanguage = function(data){
			appLanguage["changeLang"] = data;
			module.setLanguage();
		};
		
		vm.initListeners = function(bus){
			var self = recordsCenterController;
			bus.on("callLogsDetails", self.onCallLogsDetails);
			bus.on("loadMessage", self.onLoadMessage);
			bus.on("callListType", self.onCallListType);
			bus.on("callLogsIgnore", self.onCallLogsIgnore);
			bus.on("fileurl", self.onFileUrl);
			bus.on("language", self.onLanguage);
		};
	});
	
	recordsCenterController.selectedItem.$watch("length", function(s){
		var self = recordsCenterController;
		if(self.selectedItem.length == 0){
			module.barButtons[0].code = module.barButtons[0].code + "_disable";
			Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: "records"});
			return;
		}
		
		if(self.selectedItem.length < self.callList.length){
			self.isSelectAll = false;
		}
		
		if(this.length > 0){
			for(var i = 0; i < module.barButtons.length; i++){
				if(i == module.barButtons.length-1){
					continue;
				}
				module.barButtons[i].code = module.barButtons[i].code.replace("_disable", "");
			}
		}else{
			for(var i = 0; i < module.barButtons.length; i++){
				if(i == module.barButtons.length-1){
					continue;
				}
				module.barButtons[i].code = module.barButtons[i].code + "_disable";
			}
		}
		
		Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: "records"});
	});
	
	return module;
});
