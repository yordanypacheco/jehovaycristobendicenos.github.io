            var music = module.music_sendPath[data.path];
            //			console.log("begin --------- " + data.url);
            if (!music) {
                //				console.log("not music");
                return;
            } else {
                delete module.music_sendPath[data.path];
                //				console.log("<<< delete module.music_sendPath = " + data.path);

                setTimeout(function() {
                    if (self.curPath === data.path) {
                        //						console.log("	self.curPath === data.path ");

                        if (self.isAudition) {
                            self.auditionMusic(data.url);
                        } else {
                            module.setMedia(data.url);

                            self.songName = self.musicList[self.curIdx].name;
                            self.singer = self.musicList[self.curIdx].author;
                            Config.Runtime.eventBus.emit("musicPanel", {
                                songName: self.musicList[self.curIdx].name,
                                singer: self.musicList[self.curIdx].author
                            });
                        }

                        module.setSeekBar();
                    } else {
                        //						console.log("	ajax abort");
                        tools.ajaxAbort("GET", data.url);
                    }
                }, 150);
            }
            //			console.log("end ======= " + data.url);
        };

        vm.onSendFile = function(data) {
            var self = musicModuleController;

            if (data.path === undefined) {
                return;
            }

            if (data.fileType === "music" && data.effect === 1) {
                module.pauseMusic();
                return;
            }

            if (data.path === "" && data.fileType === "music" && data.effect === 0) {
                module.playMusic();
                return;
            }

            if (data.path !== "" && data.fileType === "music" && data.effect === 0) {
                //Config.Runtime.fileType = "music";

                var send = function() {
                    var m = {};
                    m.type = "requestFile";
                    m.data = [data.path];
                    tools.sendMsgToChannel(m);

                    for (var i = 0; i < self.musicList.length; i++) {
                        if (self.musicList[i].path.indexOf(data.path) != -1) {
                            self.curIdx = i;
                        }
                    }

                    self.curPath = data.path;
                    self.isAudition = true;
                    module.music_sendPath[data.path] = true;
                    self.songName = data.name ? data.name : "";
                    self.singer = data.author ? data.author : "";

                    Config.Runtime.eventBus.emit("mediaStatus", {
                        mediaStatus: "play"
                    });
                    Config.Runtime.eventBus.emit("musicPanel", {
                        songName: self.songName,
                        singer: self.singer
                    });
                };

                if (self.curIdx < 0) {
                    send();
                } else {
                    // 
                    for (var i = 0; i < self.musicList.length; i++) {
                        if (data.path === self.musicList[i].path) {
                            self.curIdx = i;
                        }
                    }
                    send();
                }

                return;
            }
        };

        vm.onAck = function(f) {
            var self = musicModuleController;
            var file = f.file;
            var upId = f.path;

            if (f.fileType == "duomi") {
                var newFile = f.file;
                self.musicList.unshift(newFile);
                return;
            }

            if (f.fileType == "kugou") {
                var newFile = f.file;
                self.musicList.unshift(newFile);
                return;
            }

            if (f.fileType != "music") return;

            if (f.file == "10000") {
                self.leftLen = 0;
                self.totalLen = 0;
                self.queue.length = 0;
                tools.tipMsg(appLanguage["changeLang"]["noSpace"]);
                return;
            }

            if (f.file == "10001") {
                tools.tipMsg(appLanguage["changeLang"]["uploadError"]);
                return;
            }

            if (f.file == "10002") {
                self.leftLen = 0;
                self.totalLen = 0;
                self.queue.length = 0;
                tools.tipMsg(appLanguage["changeLang"]["permissions"]);
                return;
            }

            file["size"] = (file["size"] / (1024 * 1024)).toFixed(2) + "MB";
            self.musicList.splice(0, 0, file);

            self.setAddSelect();

            Config.Runtime.eventBus.emit("setCount", {
                module: "musicModule",
                count: self.musicList.length
            });
        };

        vm.$watch("leftLen", function(val) {
            if (val > 0) return;
            module.startUpload();
        });

        vm.setAddSelect = function() {
            var self = musicModuleController;
            self.selectedClass.splice(0, 0, "selected");
            for (var i = 0; i < self.selectedItem.length; i++) {
                self.selectedItem[i] = self.selectedItem[i] + 1;
            }
            self.selectedItem.push(0);

            for (var i = self.selectedTop.length - 1; i > -1; i--) {
                self.selectedTop[i + 1] = self.selectedTop[i];
            }
            self.selectedTop.set(0, null);
            module.setListItemStyle(0);

            Config.Runtime.eventBus.emit("buttonStatus", {
                data: module.barButtons,
                fileType: "music"
            });
        };

        vm.initListeners = function(bus) {
            var self = musicModuleController;
            bus.on("fileList", self.onFileList);
            bus.on("fileurl", self.onFileurl);
            bus.on("sendFile", self.onSendFile);
            bus.on("musicUpload", module.onMusicUpload);

            Config.Runtime.eventBus.emit("addModel", module);
            bus.on("language", module.onLanguage);
            bus.on("ack", self.onAck);
        };
    });

    musicModuleController.selectedItem.$watch("length", function() {
        var m = musicModuleController;

        if (this.length > 0) {
            if (this.length === m.musicList.length) {
                m.isSelectAll = true;
                for (var i = 0; i < m.musicList.length; i++) {
                    m.selectedClass.set(i, "selected");
                    if (i > 0) {
                        m.selectedTop.set(i, "noTop");
                    }
                }
            } else {
                m.isSelectAll = false;
            }
            for (var i = 0; i < module.barButtons.length; i++) {
                if (i === module.barButtons.length) {
                    continue;
                }
                module.barButtons[i].code = module.barButtons[i].code.replace("_disable", "");
            }
        } else {
            m.isSelectAll = false;
            for (var i = 0; i < m.musicList.length; i++) {
                m.selectedClass.set(i, null);
            }
            for (var i = 0; i < module.barButtons.length; i++) {
                if (i === 0 || i === module.barButtons.length - 1) {
                    continue;
                }
                module.barButtons[i].code = module.barButtons[i].code + "_disable";
            }
        }

        Config.Runtime.eventBus.emit("buttonStatus", {
            data: module.barButtons,
            fileType: "music"
        });
    });

    module.musicModuleController = musicModuleController;
    return module;
});
