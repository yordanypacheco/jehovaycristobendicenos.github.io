define(
    'listTree',[
        'tools',
        'config'
    ],
    function(
        tools,
        Config
    ) {

        var listTree = {
            pathFileList: {}, // listTree.pathFileList[path]
            pathFolderList: {}, // listTree.pathFolderList[path]
            pathFolderStatus: {}, //  null, opened or closed
            pathHistory: { // 
                'dir': [],
                'rename': {},
                'delete': {
                    'cateList': {
                        //					'path': {
                        //						'folder': '',
                        //						'name': '',
                        //						'isFile': false,
                        //						'size': '',
                        //						'time': '',
                        //						'path': ''
                        //					} 
                    },
                    'treeList': {
                        //					'path': {
                        //						'folder': '',
                        //						'name': '',
                        //						'isFile': false,
                        //						'size': '',
                        //						'time': '',
                        //						'path': ''
                        //					}
                    }
                }
            },
            op: ""
        };

        listTree.openCloseLeaf = function(path, code, fileModule) {
            this.op = code;
            tools.logs("path = " + path, "code = " + code, "this.pathFolderList[path] = " + this.pathFolderList[path]);
            /**
             * showTree
             * showList
             * refresh
             */
            this.pathHistory['dir'].push(path);
            switch (code) {
                case "showTree":
                    // open, loading
                    if (this.pathFolderList[path] == undefined) {
                        listTree.loadingLeaf(path);
                        this.pathFolderList[path] = [];
                        this.pathFolderStatus[path] = 'opened';

                        listTree.sendReq(path);
                    } else {
                        // open or close, 
                        if (this.pathFolderStatus[path] !== 'opened') {
                            this.pathFolderStatus[path] = 'opened';
                        }
                        listTree.setArrowStatus(path);
                        listTree.setExistPath(path, fileModule);
                    }

                    //listTree.closeOthers(fileModule);
                    break;
                case "refresh":
                    // refresh, loading
                    listTree.loadingLeaf(path);
                    $("#" + listTree.getPath(path) + " ul").remove();

                    this.pathFolderList[path] = [];
                    this.pathFolderStatus[path] = 'opened';

                    listTree.sendReq(path);
                    break;
                case "showList":
                    // show list
                    this.pathFolderStatus[path] = 'null';
                    listTree.sendReq(path);
                    break;
                default:
                    break;
            }

        };

        /**
         * open parent folder from history
         */
        listTree.openParents = function(fileModule) {
            var currentPath = fileModule.currentPath;
            var status = this.pathFolderStatus;

            for (i in status) {
                if (status[i] === 'closed') {
                    if (currentPath.indexOf(i) !== -1) {
                        if (currentPath === i) {
                            continue;
                        } else if (currentPath.length > i.length) {
                            if (status[i] !== 'opened') {
                                status[i] = 'opened';
                            }

                            if (status[currentPath] === 'opened') {
                                listTree.setArrowStatus(currentPath);
                            }

                            listTree.setArrowStatus(i);
                            listTree.setExistPath(i, fileModule);
                        }
                    } else {
                        continue;
                    }
                }
            }
        };

        /*
         * When you open a folder shut down other folders
         */
        listTree.closeOthers = function(fileModule) {
            var history = this.pathHistory['dir'];
            var len = history.length;
            var currentPath = fileModule.currentPath;
            var lastPath = "";

            if (len < 2) {
                return;
            } else {
                for (var i = len - 1; i >= 0; i--) {
                    var prePath = history[i];
                    //				console.log("prePath = " + prePath, "len = " + len);
                    if (currentPath.indexOf(prePath) != -1 && currentPath.length === prePath.length) {
                        continue;
                    } else {
                        lastPath = prePath;

                        lastLen = lastPath.split("/").length;
                        currentLen = currentPath.split("/").length;

                        if (lastPath !== currentPath && lastLen === currentLen) {
                            // open or close, 
                            if (this.pathFolderStatus[lastPath] !== 'closed') {
                                this.pathFolderStatus[lastPath] = 'closed';
                                //							console.log("close other path = " + lastPath);
                                listTree.setArrowStatus(lastPath);
                            }
                            if (len - i > 20) {
                                //							console.log("===== break ========");
                                break;
                            }
                        }
                    }
                }
            }
        };

        listTree.sendReq = function(path) {
            var m = {};
            m.type = "getStorageList";
            m.data = path;
            tools.sendMsgToChannel(m);
        };

        listTree.setExistPath = function(path, fileModule) {
            if (this.pathFolderList[path]) {
                fileModule.currentPathFiles = this.pathFileList[path];
            } else {
                fileModule.currentPathFiles = [];
            }
        };

        // 
        listTree.getLeaf = function(data) {
            var leaf = [];

            for (var i = 0; i < data.files.length; i++) {
                if (data.files[i].isFile == 'true') {
                    continue;
                } else {
                    leaf.push(data.files[i]);
                }
            }

            return leaf;
        };

        listTree.loadingLeaf = function(path) {
            var arrow = "#" + listTree.getPath(path) + " div:first";
            $(arrow).removeClass("tree_open tree_null tree_close").addClass("tree_loading");
        };

        listTree.setArrowStatus = function(path) {
            var li = "#" + listTree.getPath(path);
            var arrow = "#" + listTree.getPath(path) + " div:first";

            if ($(arrow).hasClass("tree_null") && listTree.pathFolderList[path].length < 1) {
                this.pathFolderStatus[path] = 'null';
                tools.logs(" === tree_null");
                return;
            }

            if ($(arrow).hasClass("tree_null") && listTree.pathFolderList[path].length > 0) {
                $(arrow).removeClass("tree_null").addClass("tree_loading");
            }

            tools.logs('list-tree: 123' + '$(arrow).hasClass("tree_close") = ' + $(arrow).className + ' ==> $(arrow).hasClass("tree_loading")' + $(arrow).hasClass("tree_loading"));
            if ($(arrow).hasClass("tree_close") || $(arrow).hasClass("tree_loading")) {
                $(arrow).removeClass("tree_close tree_loading").addClass("tree_open");
                this.pathFolderStatus[path] = 'opened';

                $(li + ">ul").show();
            } else {
                $(arrow).removeClass("tree_open tree_loading").addClass("tree_close");
                this.pathFolderStatus[path] = 'closed';
                $(li + ">ul").hide();
            }
        };

        listTree.nullLeaf = function(path) {
            var arrow = "#" + listTree.getPath(path) + " div:first";
            $(arrow).removeClass("tree_open tree_close tree_loading").addClass("tree_null");
        };

        /** 
         * document.getElementById(), $(ID) ID
         * document.getElementById("/storage/emulated/0/111/555/dsf") ==> 
         * $("#\\/storage\\/emulated\\/0\\/111\\/555\\/dsf")
         */
        listTree.getPath = function(path) {
            //		var p = path.replace(/\//g, "\\/").replace(/\./g, "\\.").replace(/\ /g, "\\ ").replace(/\(/g, "\\(").replace(/\)/g, "\\)").replace(/\[/g, "\\[").replace(/\]/g, "\\]").replace(/\}/g, "\\}").replace(/\{/g, "\\{");
            //		var p = path.replace(/(:|\.|\[|\]|\/|)/g, "\\$1");
            var p = path.replace(/\./g, "\\.");
            p = p.replace(/\!/g, "\\!");
            p = p.replace(/\=/g, "\\=");
            p = p.replace(/\+/g, "\\+");
            p = p.replace(/\^/g, "\\^");
            p = p.replace(/\$/g, "\\$");
            p = p.replace(/\#/g, "\\#");
            p = p.replace(/\@/g, "\\@");
            p = p.replace(/\&/g, "\\&");
            p = p.replace(/\*/g, "\\*");
            p = p.replace(/\//g, "\\/");
            p = p.replace(/\ /g, "\\ ");
            p = p.replace(/\(/g, "\\(");
            p = p.replace(/\)/g, "\\)");
            p = p.replace(/\[/g, "\\[");
            p = p.replace(/\]/g, "\\]");
            p = p.replace(/\}/g, "\\}");
            p = p.replace(/\{/g, "\\{");
            p = p.replace(/\</g, "\\<");
            p = p.replace(/\>/g, "\\>");
            p = p.replace(/\|/g, "\\|");

            //		tools.on_off = true;
            //		tools.logs("path = " + path);
            //		tools.logs("p = " + p);
            //		tools.on_off = false;

            return p;
        };

        listTree.addHideLeaf = function(path, data) {
            var welineIcon = ""
            if (data.name == 'weline') {
                welineIcon = 'weline_icon';
            }
            var html =
                '<li id="' + data.path + '">' +
                '<div class="tree_leaf tree_close">' +
                '<div class="tree_btn"></div>' +
                '<div class="tree_folder_icon ' + welineIcon + '"></div>' +
                '<span class="tree_folder_name">' + data.name + '</span>' +
                '</div>' +
                '</li>';

            try {
                var newD = document.createElement("ul");
                newD.className = "tree_item";
                newD.style.display = "none";
                newD.innerHTML = html;

                document.getElementById(path).appendChild(newD);
            } catch (e) {
                tools.logs(e);
            }

        };

        listTree.addLeaf = function(path, data) {
            var welineIcon = ""
            if (data.name == 'weline') {
                welineIcon = 'weline_icon';
            }

            //console.log("path , data:" , path , data)
            var html =
                '<li id="' + data.path + '">' +
                '<div class="tree_leaf tree_close">' +
                '<div class="tree_btn"></div>' +
                '<div class="tree_folder_icon ' + welineIcon + '"></div>' +
