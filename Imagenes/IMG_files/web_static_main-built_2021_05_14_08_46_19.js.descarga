		vm.onDirectOK = function(){
			var self = ctController;
			self.cType = module.typeList[0]['type'];
		};
		
		vm.initListeners = function(bus){
			var self = ctController;
			bus.on("language" , module.onLanguage);
			bus.on("directOK", self.onDirectOK);
			bus.on("visibleConnectionType", self.onVisibleCT);
		};
	});
	
	
	return module;
});

