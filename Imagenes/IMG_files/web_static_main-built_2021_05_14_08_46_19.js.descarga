	                }
	                if (that.length > 0 && that.flag) aysncImg();

	            }, 200)
	        }

	        aysncImg();
	    };


	    upload.prototype.uploadComplete = function() {

	        var that = this.upload;

	        if (that.scope.callback) {
	            that.scope.callback();
	        }

	        if (that.scope.obj && that.scope.length) {
	            that.scope.nextOK = true;
	            that.scope.obj.leftLen--;
	            that.scope.length--;
	            if (that.scope.obj.totalLen) that.scope.obj.totalLen--;
	        }

	        if (that.pro.el) {
	            that.pro.el.style.width = "0px";
	        }

	    }

	    upload.prototype.uploadFailed = function() {
	        var that = this.upload;

	        if (that.scope.callback) {
	            that.scope.callback();
	        }
	        if (that.scope.obj && that.scope.obj.uploadErr != undefined) {
	            that.scope.nextOK = true;
	            that.scope.obj.uploadErr++;
	        }
	    }

	    upload.prototype.uploadProgress = function(evt, pro) {
	        if (evt.lengthComputable) {
	            var percentComplete = Math.round(evt.loaded * 100 / evt.total);
	            var that = this;
	            // console.log('percentComplete:', percentComplete , this.pro.el , this.pro.el.tagName)
	            if (!this.pro.el) return;
	            if (this.pro.el.tagName == "PROGRESS") {
	                this.pro.el.value = percentComplete.toString();
	            } else {
	                this.pro.el.style.width = percentComplete.toString() + "%";
	            }
	        }
	    };

	    upload.prototype.uploadCanceled = function() {
	        var that = this.upload;
	        if (that.scope.obj && that.scope.obj.uploadErr != undefined) {
	            that.scope.nextOK = true;
	            that.scope.obj.uploadErr++;
	        }
	    }


	    upload.prototype.isFolder = function(files, pro, path) {

	        var that = this;

	        var set = function() {
	            that.obj.totalLen--;
	            that.obj.leftLen--;
	            that.length--;
	            that.nextOK = true;
	            tools.tipMsg(appLanguage['changeLang']["noFolder"]);
	            Config.Runtime.eventBus.emit('upDir', { "upId": path });
	        }
	        try {
	            var fd = new FileReader;
	            fd.onerror = function(evt) {
	                switch (evt.target.error.code) {
	                    case evt.target.error.ABORT_ERR:
	                        //that.sendData(files , pro , path);
	                        break; // noop
	                    default:
	                        set();
	                };
	            };

	            fd.onabort = function() {

	                that.sendData(files, pro, path);
	            }


	            fd.onprogress = function(evt) {
	                if (evt.lengthComputable) {
	                    var percentLoaded = Math.round((evt.loaded / evt.total) * 100);
	                    if (percentLoaded > 5) {
	                        fd.abort();
	                    }
	                }
	            }

	            fd.onload = function() {
	                that.sendData(files, pro, path);
	            };

	            fd.readAsDataURL(files);
	        } catch (e) {}
	    }

	    upload.prototype.send = function(files, pro, path) {

	        var that = this;
	        that.sendData(files, pro, path);
	    }

	    upload.prototype.sendData = function(files, pro, path) {

	        var fd = new FormData();
	        var imei = tools.getSessionId();
	        if (imei) {
	            imei = imei.substring(0, 7);
	        } else {
	            imei = '';
	        }

	        console.log("files ---:", files)
	        fd.append("size", files["size"]);
	        // fd.append("path", path.substring(0, path.lastIndexOf("/")));
	        fd.append("path", path);
	        fd.append("imei", imei);
	        if (Config.Runtime.version == 1) {
	            fd.append("version", "1");
	        }
	        tools.googleAna("uploadTotal")
	        fd.append("mime", Config.map.get(files.name.substring(files.name.lastIndexOf("."))));

	        if (files["base64"]) {
	            fd.append("base64file", files["data"]);
	        } else {
	            fd.append("filename", files)
	        }

	        var that = this;

	        var xhr = new XMLHttpRequest();
	        var url = Config.Config.baseUrl + "upload";
	        if (Config.Config.directUpload) {
	            url = Config.Config.directUpload;
	        }
	        console.log("URL", url)
	        xhr.upload.addEventListener("progress", this.uploadProgress, false);
	        xhr.addEventListener("load", this.uploadComplete, false);
	        xhr.addEventListener("error", this.uploadFailed, false);
	        xhr.addEventListener("abort", this.uploadCanceled, false);
	        setTimeout(function() {
	            xhr.open("POST", url);
	            xhr.upload.pro = pro;
	            xhr.upload.scope = that;
	            xhr.send(fd);
	        }, 500)

	    };
	    /*
	     upload.prototype.del = function(value){
	    	 $(value).remove;
	     };
	     */
	    return upload

	});
