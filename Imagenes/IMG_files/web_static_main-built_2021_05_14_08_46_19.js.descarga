define(
	'rectSelect',[ 
    'tools', 
    'config'
    ], 
function(
	tools, 
	Config
    ){
	
	/**
	 * itemClass  每个项目共有的类名
	 * 还需要给出这些项目的容器对象container
	 */
	var RectSelect = function(container, itemClass){
		this.rect = null;
		
		this.beginX = 0;
		this.beginY = 0;
		this.endX = 0;
		this.endY = 0;
		this.startX = 0;
		this.startY = 0;
		this.selectItems = [];
		this.container = container;
		this.ic = itemClass;
		this.LEFT = 60;
		this.isDown = false;			// 鼠标是否按下
		this.isStop = false;			// 是否阻止click事件冒泡
		this.init(container);
	};
	
	RectSelect.prototype.init = function(container){
		var self = this;
		
		var op = {};
		
		$(container).die().live('click', function(e){
			var evt = e || window.event;
			
			if(evt && self.isStop){
				evt.stopPropagation();
			}
		});
		
		$(container).die().live('mousedown', function(e){
			var evt = e || window.event;
			if(evt){
				evt.stopPropagation();
			}
			
			op = self.getParentsOffset();
			self.beginX = e.pageX - op.left;
			self.beginY = e.pageY - op.top;
			self.startX = e.pageX - op.left;
			self.startY = e.pageY - op.top;
			
			self.isDown = true;
			self.isStop = false;
			
			if(self.rect){
				self.removeRect();
			}
		});
		
		$(container).die().live('mousemove', function(e){
			var evt = e || window.event;
			if(evt){
				evt.stopPropagation();
			}
			self.endX = e.pageX - op.left;
			self.endY = e.pageY - op.top;
			
			if(!self.isDown){
				return;
			}
			
			var w = Math.abs(parseInt(self.endX, 10) - parseInt(self.beginX, 10));
			var h = Math.abs(parseInt(self.endY, 10) - parseInt(self.beginY, 10));
			
			if(w < 20 || h < 20){
				return;
			}
			
			self.isStop = true;
			
			if(!self.rect){
				self.createRect();
			}else if(self.isDown){
				self.setRectSize();
			}
		});
		
		$(container).die().live('mouseup', function(e){
			var evt = e || window.event;
			if(evt){
				evt.stopPropagation();
			}
			
			self.endX = e.pageX - op.left;
			self.endY = e.pageY - op.top;
			
			self.isDown = false;

			self.getItems();
			self.removeRect();
		});
		
	};
	
	RectSelect.prototype.getParentsOffset = function(){
		var self = this;
		var op = {"left":0, "top":0};
		var parent = $(self.container).parent();
		var component = $("#compnents");
		
		while(true){
			if(parent.attr("id") != 'compnents'){
				var o = $(parent).offset();
				if(o.left >= op.left){
					op.left = o.left;
				}
				if(o.top >= op.top){
					op.top = o.top;
				}
				
				parent = $(parent).parent();
			}else{
				break;
			}
		}
		
		return op;
	};
	
	RectSelect.prototype.createRect = function(){
		var self = this;
		
		self.rect = document.createElement('div');
		self.rect.id = 'rect_select';
		self.rect.className = 'rect_select';
		self.rect.style.left = self.startX + 'px';
		self.rect.style.top = self.startY + 'px';
//		var b = document.getElementsByTagName('body')[0];
//		b.appendChild(self.rect);
		self.container.appendChild(self.rect);
		
		tools.addEvt(self.rect, 'mousemove', function(e){
			//不处理，冒泡上去处理
//			e.stopPropagation();
//			self.endX = e.pageX;
//			self.endY = e.pageY;
			//tools.logs("move x = " + e.pageX, "move y = " + e.pageY);
			
//			self.setRectSize();
		});
		
		tools.addEvt(self.rect, 'mouseup', function(e){
			//不处理，冒泡上去处理
//			e.stopPropagation();
//			self.isDown = false;
//			self.getItems();
//			self.removeRect();
		});
	};
	
	RectSelect.prototype.removeRect = function(){
		var self = this;
		
		if(self.rect && self.container){
			self.rect.parentNode.removeChild(self.rect);
			self.rect = null;
//			self.getItems();
			tools.logs("remove success");
		}
	};
	
	RectSelect.prototype.setRectSize = function(){
		var self = this;
		
		if(self.rect){
			var w = Math.abs(parseInt(self.endX, 10) - parseInt(self.beginX, 10));
			var h = Math.abs(parseInt(self.endY, 10) - parseInt(self.beginY, 10));
			//right top
			if(self.endX > self.beginX && self.endY < self.beginY){
				self.rect.style.left = self.startX + 'px';
				self.rect.style.top = self.endY + 'px';
			}
			//left top
			if(self.endX < self.beginX && self.endY < self.beginY){
				self.rect.style.left = self.endX + 'px';
				self.rect.style.top = self.endY + 'px';
			}
			//right bottom
			if(self.endX > self.beginX && self.endY > self.beginY){
				self.rect.style.left = self.startX + 'px';
				self.rect.style.top = self.startY + 'px';
			}
			//left bottom
			if(self.endX < self.beginX && self.endY > self.beginY){
				self.rect.style.left = self.endX + 'px';
				self.rect.style.top = self.startY + 'px';
			}
			
			self.rect.style.width = w + 'px';
			self.rect.style.height = h + 'px';
//			tools.logs("self.endX = " + self.endX, "self.beginX = " + self.beginX, "self.endY = " + self.endY, "self.beginY = " + self.beginY);
//			tools.logs("w = " + w, "h = " + h);
		}
	};
	
	RectSelect.prototype.getItems = function(){
		var self = this;
		var scrollValue = self.container.scrollTop;
		
		if($(self.rect).width() < 10 || $(self.rect).height() < 10){
			return;
		}
		
		self.selectItems = [];
		
		var items = $(self.container).find("."+self.ic);
		//left top position
		var ox = 0;
		var oy = 0;
		//right bottom position
		var oox = 0;
		var ooy = 0;
		
		var itemWidth = $(items[0]).width();
		var itemHeight = $(items[0]).height();
		
		$(items).removeClass("selected");
		
		for(var i = 0; i < items.length; i++){
			ox = items[i].offsetLeft;
			oy = items[i].offsetTop - scrollValue;
			
			oox = ox + itemWidth;
			ooy = oy + itemHeight;
			
			if(self.beginX > self.endX && self.beginY > self.endY){
				self.startX = self.endX;
				self.startY = self.endY;
				
				self.endX = self.beginX;
				self.endY = self.beginY;
			}
			
			if(self.beginX < self.endX && self.beginY > self.endY){
				self.startY = self.endY;
				
				self.endY = self.beginY;
			}
			
			if(self.beginX > self.endX && self.beginY < self.endY){
				self.startX = self.endX;
				
				self.endX = self.beginX;
			}
			
			// left top
			if((self.startX < ox && ox < self.endX) && (self.startY < oy && oy < self.endY)){
				self.selectItems.push(i);
				
				$(items[i]).addClass("selected");
			}else{
				if((self.startX < ox && ox < self.endX) && (self.startY < ooy && ooy < self.endY)){
					self.selectItems.push(i);
					
					$(items[i]).addClass("selected");
				}else if((self.startX < oox && oox < self.endX) && (self.startY < oy && oy < self.endY)){
					self.selectItems.push(i);
					
					$(items[i]).addClass("selected");
				}else if((self.startX < oox && oox < self.endX) && (self.startY < ooy && ooy < self.endY)){
					tools.logs("ol = " + oox, "ot = " + ooy, "self.startX = " + self.startX, "endX = " + self.endX, "self.startY = " + self.startY, "endY = " + self.endY);
					self.selectItems.push(i);
					
					$(items[i]).addClass("selected");
				}
			}
		}
		items = null;
		var eventType = Config.Runtime.fileType+'Rect'
		Config.Runtime.eventBus.emit(eventType,{data: self.selectItems});
		tools.logs("Config.Runtime.fileType+'Rect' = " + Config.Runtime.fileType+'Rect', "self.selectItems = " + self.selectItems);
	};
	
	RectSelect.prototype.clearSelected = function(){
		var self = this;
		self.selectItems.splice(0, self.selectItems.length);
	};
	
	return RectSelect;
});
