		vm.submitContact = function(){
			var self = contactCenterController;
			var id="";
			var data = {};
			
			var submitFlag = self.judge();
			
			if(submitFlag == 1){
				tools.tipMsg(appLanguage['changeLang']["errPhone"]);
				return;
			}else if(submitFlag == 2){
				tools.tipMsg(appLanguage['changeLang']["errName"]);
				return;
			}
			
			data["name"] = self.editContact["localName"];
			data["notes"] = self.editContact["localNotes"];
			data["company"] = self.editContact["company"];
			
			var tempPhone = "";
			if(self.editContact["phone"].length >0){
				tempPhone = self.editContact["phone"].join(",");
			}
			
			if(self.editContact["localPhone"]){
				tempPhone ? (tempPhone = self.editContact["localPhone"] + "," + tempPhone) : tempPhone = self.editContact["localPhone"];
			}
			
			data["phone"] = tempPhone;
			var tempMail = "";
			if(self.editContact["mailBox"].length >0){
				tempMail = self.editContact["mailBox"].join(",");
			}
			
			if(self.editContact["localMailBox"]){
				tempMail ? (tempMail = self.editContact["localMailBox"] + "," + tempMail) : tempMail = self.editContact["localMailBox"];
			}
			
			data["mailBox"] = tempMail;
			
			var tempQQ = "";
			if(self.editContact["qq"].length >0){
				tempQQ = self.editContact["qq"].join(",");
			}
			
			if(self.editContact["localQQ"]){
				tempQQ ? (tempQQ = self.editContact["localQQ"] + "," + tempQQ) : tempQQ = self.editContact["localQQ"];
			}
			
			data["qq"] = tempQQ;
			
			var tempStreet = "";
			if(self.editContact["street"].length >0){
				tempStreet = self.editContact["street"].join(",");
			}
			
			if(self.editContact["localStreet"]){
				tempStreet ? (tempStreet = self.editContact["localStreet"] + "," + tempStreet) : tempStreet = self.editContact["localStreet"];
			}
			
			data["street"] = tempStreet;
			
			var index = self.sameName(data);
		//	console.log("lastSelect:" , lastSelect)
			if(index != null && lastSelect==null){
				var sameD = self.contactList[index];
				lastSelect = index;
				
				for(obj in sameD){
					//console.log("obj:" , sameD , obj)
					if(obj == 'phone' || obj=='qq' || obj == "mailBox" || obj == "street"){
						if(sameD[obj]){
							 data[obj] = self.checkoutSameName(sameD[obj] + "," + data[obj]);
						}
					}else if(obj == 'id' || obj == 'icon' || obj == 'src' || obj=="path" || obj=='notes' || obj=='company'){
						if(data[obj] == '' && sameD[obj]){
							 data[obj] = sameD[obj];
						}
					}	
					
				}
				
				
			}
			
			if(lastSelect == null){
				
				//self.icon.splice(0 , 0 , self.imgCropData);
				if(ackImgPath){
					
					data["icon"] = ackImgPath ;
					data["src"] = self.imgCropData;
					
				}else{
					
					data["icon"] = defaultSet;
					data["src"] = defaultSet;
					
				}
				
				lastSelect = 0;
				
				self.contactList.splice(0 , 0 , data);
				
			}else{
				
				if(ackImgPath){
					
					data["icon"] = ackImgPath ;
					data["src"] = self.imgCropData;
					
				}else{
					
					data["icon"] = self.contactList[lastSelect]["icon"];
					data["src"] = self.contactList[lastSelect]["src"];
					
				}
				
				id = self.contactList[lastSelect]["id"];
				data["id"] = id; 
				self.contactList.splice(lastSelect , 1 , data);
			}
			
			if(!data["id"]){
				var guidPath = tools.guidGenerator();
				self.contactList[lastSelect]['path'] = guidPath;
				data["path"] = guidPath;
				//console.log("data:" , data['path'])
				//addNewChangeId["path"] = data["path"];
			}
			
			
			var t={};
			for(var key in data){
				t[key] = data[key];
			}
			delete t.src;
			tools.sendMsgServer("newContact" , t);
			
			self.showContact = true;
			self.setContacList();
			
		};
		
		vm.checkoutSameName = function(strArr){
			var arr = strArr.split(",");
			
		//	console.log("start arr:" , arr)
			var newArr=[];
			var flag = true;
			for(var i=0 ; i< arr.length ; i++){
				flag = true;
				for(var j=i+1 ; j<arr.length;j++){
					if(arr[i] == arr[j] || arr[i]==""){ flag=false ;break}
				}
				if(flag) newArr.push(arr[i]);
			}
			
			//console.log("newArr:" , newArr)
			
			return newArr.join(",");
		};
		
