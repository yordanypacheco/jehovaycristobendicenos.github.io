        vm.onAck = function(f) {
            var file = f.file;
            var upId = f.path;
            var self = fileModuleController;
            tools.logs("upId = " + upId);
            if (f.fileType != "file") return;

            if (file == 10000) {
                tools.tipMsg(appLanguage['changeLang']["noSpace"]);
                self.currentPathFiles.splice(0, 1);
                self.delUploadTip(upId)
                return
            }

            if (file == 10001) {
                tools.tipMsg(appLanguage['changeLang']["uploadError"]);
                // self.currentPathFiles.splice(0, 1);
                self.delUploadTip(upId)
                return
            }

            if (file == 10002) {
                // tools.tipMsg(appLanguage['changeLang']["permissions"]);
                tools.tipPanel(appLanguage["changeLang"]["followPrompts"], appLanguage["changeLang"]["Jurisdiction"]);
                self.currentPathFiles.splice(0, 1);
                self.delUploadTip(upId)
                return
            }

            if (file["type"].indexOf(".") < 0) {
                file["type"] = tools.getFileType("." + file["type"]);
            }
            var floderTag = upId.substring(0, upId.indexOf("&"));
            console.log("self.uploadFolder[floderTag]:", self.uploadFolder[floderTag], floderTag)
            if (self.uploadFolder[floderTag]) {
                self.uploadFolder[floderTag]["val"] = self.uploadFolder[floderTag]["val"] - 1;

                self.uploadFolder[floderTag]["size"] = parseInt(self.uploadFolder[floderTag]["size"]) - file["size"] / 1024;

                var value =
                    (parseInt(self.uploadFolder[floderTag]["total"]) - parseInt(self.uploadFolder[floderTag]["size"])) /
                    parseInt(self.uploadFolder[floderTag]["total"]);
                self.uploadFolder[floderTag]["bar"].value = value * 100;
                self.uploadFolder[floderTag]["el"].innerHTML = self.uploadFolder[floderTag]["val"];
                if (self.uploadFolder[floderTag]["val"] == 0) {
                    upId = self.uploadFolder[floderTag]["name"];
                    file["name"] = self.uploadFolder[floderTag]["fName"];
                    file["path"] = self.uploadFolder[floderTag]["path"];

                    file["size"] = 0;

                    self.delUploadTip(upId, floderTag)


                    listTree.pathFileList[file["path"]] = undefined;
                    listTree.pathFolderList[file["path"]] = undefined;
                    //listTree.op = "refresh";
                    listTree.pathFolderList[self.currentPath].push(file);
                    listTree.pathFileList[self.currentPath].push(file);
                    //listTree.sendReq(self.currentPath);
                    listTree.addLeaf(self.currentPath, {
                        name: self.uploadFolder[floderTag]["fName"],
                        path: file["path"]
                    });
                    //	module.refresh();
                    delete file["type"];
                    delete self.uploadFolder[floderTag];
                    file["isFile"] = "false";
                } else {
                    return;
                }
            }
            for (var i = 0; i < self.currentPathFiles.length; i++) {
                console.log("del file:", self.currentPathFiles[i]["upId"], self.currentPathFiles[i]["upId"] == upId)
                if (self.currentPathFiles[i]["upId"] && self.currentPathFiles[i]["upId"] == upId) {
                    try {
                        var dom = document.getElementById(upId);
                        var progress = document.getElementById("progress_" + upId);
                        dom.parentNode.removeChild(dom);
                        progress.parentNode.removeChild(progress);
                    } catch (e) {}

                    self.currentPathFiles.splice(i, 1, file);
                    return;
                }
            }
            //			console.log("self.currentPathFiles.slice(0):" , self.currentPathFiles.slice(0) , self.currentPath)
            listTree.pathFileList[self.currentPath] = self.currentPathFiles.slice(0);
        };

