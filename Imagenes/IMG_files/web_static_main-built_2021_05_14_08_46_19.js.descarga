                if (!path["zip"]) {

                    if (path["data"].length > 4 || (tools.noIe() && path["data"].length > 1)) path["zip"] = true;

                }


                if (tools.noIe() && path["data"].length > 1) {
                    path["zip"] = true;
                }

                if (path['type'] && path['type'] == "app") {

                    path["zip"] = false;

                }

                if (tools.noIe() && path["data"].length > 1 && path['type'] == "app") {
                    path["zip"] = true;
                }


            }




            for (var i = 0; i < path['data'].length; i++) {
                var index = path['data'][i].indexOf("/cutimage/");

                if (index >= 0) {
                    data.push(tools.cutImage(path['data'][i]));
                } else {
                    data.push(path['data'][i]);
                }

            }

            if (path["zip"]) {
                var callBcakPath = tools.guidGenerator();

                if (data.length == 1) {
                    callBcakPath = "/" + callBcakPath + data[0].substring(data[0].lastIndexOf("/")) + ".zip";
