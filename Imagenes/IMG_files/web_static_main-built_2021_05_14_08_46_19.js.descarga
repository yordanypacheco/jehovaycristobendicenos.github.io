				$.blockUI({
					message: "",
					css:{"background":"#000"},
					overlayCSS: {"opacity": 0.6}
				});
				self.visible = true;
			}else{
				$.unblockUI();
				self.visible = false;
			}
		};
		
		vm.onVisibleYoutube = function(data){
			var self = helpModuleController;
			
			if(data.visible){
				$.blockUI({
					message: "",
					css:{"background":"#000"},
					overlayCSS: {"opacity": 0.6}
				});
				
				self.visible = true;
				
				$(".blockUI").live("click", function(e){
					e.stopPropagation();
					module.closeYoutube();
				})
			}else{
				$(".blockUI").die("click");
				
				module.closeYoutube();
			}
		};
		
		vm.initListeners = function(bus){
			var self = helpModuleController;
			
			bus.on("visibleHelp", self.onVisibleHelp);
			bus.on("visibleYoutube", self.onVisibleYoutube);
			
		};
	});
	
	return module;
});

