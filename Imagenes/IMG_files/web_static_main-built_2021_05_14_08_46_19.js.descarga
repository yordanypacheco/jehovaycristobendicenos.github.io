            $("#player_seek_bar")
                .off()
                .on("click", function(e) {
                    var seekWidth = $(this).width();
                    var el = e.clientX;
                    var percentSeek = ((el - 60) / seekWidth) * 100;
                    $("#jquery_jplayer_1").jPlayer("playHead", percentSeek);
                    tools.logs("percentSeek = " + percentSeek);
                });
