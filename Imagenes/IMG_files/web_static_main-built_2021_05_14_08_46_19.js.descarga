                var j = 1;
                var runAnim = function(time) {
                    /*if(close && close > 0){
                    	dom.style.opacity = 1;
                    	return
                    }
                    */
                    op = 1 - opacity * j;
                    if (j < i) {
                        dom.style.opacity = op;
