            workProcess: function(obj, arr, fn, O) {

                var step = 50;

                var length = 0;

                var addValue = function() {

                    length = Math.min(arr.length - obj.length, step);

                    if (length > 0) {
                        obj.pushArray(arr.slice(obj.length, obj.length + length));
                        fn();
                    }

                    if (length == step) {
                        setTimeout(addValue, 800);
                    } else {

                        if (O) {
                            O["addOk"] = true;
                        }
                    }

                }

                addValue();

