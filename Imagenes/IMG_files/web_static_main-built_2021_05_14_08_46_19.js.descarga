sms_null_time\' : \'\'}}" ms-class-2="last_sms_time: 0 == $index">{{el.date}}</div>\n\t\t\t\t\t\t\t\t\t<div ms-class="{{el.type==\'2\' ? \'sms_from_me\' : \'sms_from_other\'}}"><p>{{el.content}}</p><div class="arrow"></div></div>\n\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t\t<div class="sms_edit"  ms-visible="isShowRecords">\n\t\t\t\t\t\t\t<textarea rows="" cols="" ms-duplex="imMessage" ms-keyup="pressReplaySMS($event)"></textarea>\n\t\t\t\t\t\t\t<div class="sms_btns">\n\t\t\t\t\t\t\t\t<div id="replaySendBtn" class="send_btn buttons" ms-click="clickReplySMS()">{{changeLang["send"]}}</div>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t\t<!-- -->\n\t\t\t\t\t\t<div class="empty_list" ms-visible="!isShowRecords">\n\t\t\t\t\t\t\t<div id="empty_message" class="empty_img empty_message"></div>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n\t\t\t\t</div>';});

