        vm.onStorageList = function(data) {
            var self = fileModuleController;
            var list = [];
            var hideList = [];
            data.files = module.getSuffix(data.files);

            if (data.path !== self.currentPath) {
                // 为非当前路径下的listTree添加叶子
                //				console.log("path !============== current path > " + data.path);
                hideList = listTree.getLeaf(data);

                // 没有子目录，去掉箭头
                if (hideList.length == 0) {
                    //					console.log("!= null folder");
                    listTree.nullLeaf(data.path);
                } else if (listTree.pathFileList[data.path] && listTree.pathFileList[data.path].length === data.files.length) {
                    return;
                } else {
                    listTree.pathFileList[data.path] = data.files;
                    listTree.pathFolderList[data.path] = hideList;

                    // 删除树节点下的目录
                    listTree.clearLeafs(data.path);

                    for (var j = 0; j < hideList.length; j++) {
                        // 添加叶子
                        //						console.log("添加隐藏叶子" + j);
                        listTree.addHideLeaf(data.path, hideList[j]);
                    }

                    var li = "#" + listTree.getPath(data.path);
                    var arrow = "#" + listTree.getPath(data.path) + " div:first";
                    $(arrow)
                        .removeClass("tree_open tree_loading")
                        .addClass("tree_close");

                    listTree.closeOthers(self);
                    //					listTree.pathFolderStatus[data.path] = 'closed';

                    $(li + ">ul").hide();
                }

                return;
            }
            //			console.log("-------- current path = " + self.currentPath, "data.path = " + data.path);
            if (data.files.length == 0 || data.files == "") {
                //				console.log("self.isEmpty");
                self.isEmpty = true;
                listTree.nullLeaf(data.path);
                listTree.pathFileList[data.path] = [];
                self.currentPathFiles = [];
                return;
            } else {
                self.isEmpty = false;
            }

            list = listTree.getLeaf(data);
            listTree.pathFileList[data.path] = data.files;

            // 没有子目录，去掉箭头
            if (list.length == 0) {
                //				console.log("null folder");
                listTree.nullLeaf(self.currentPath);
                self.currentPathFiles = data.files;
                return;
            }

            //if (list[0].path.indexOf(self.currentPath) != -1) {
            self.currentPathFiles = data.files;
            self.currentItem = data.files[0];
            //}

            if (listTree.op === "showList") {
                //				console.log("op = showList");
                return;
            }

            if (listTree.pathFolderList[self.currentPath]) {
                var ul = $("#" + listTree.getPath(self.currentPath) + " > ul");

                if (ul.length > 0 && listTree.op != "refresh") {
                    // 重复请求
                    //					console.log("重复请求，不回应");
                    return;
                }
                listTree.pathFolderList[self.currentPath] = list;
                for (var j = 0; j < list.length; j++) {
                    // 添加叶子
                    //					console.log("添加叶子");
                    listTree.addLeaf(self.currentPath, list[j]);
                }
