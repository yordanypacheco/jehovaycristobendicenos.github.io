			this.elContainer.style.cursor = "mover";

		};
		
		module.sendDragFile = function(e , uploadPath , group){
			var model = Config.Runtime.fileType + 'Upload';
			e.file(function(d){
				d["dragFile"] = true;
				d["dragPath"] = uploadPath;
				d["group"] = group;
				if(uploadPath){
					var folder = uploadPath.substring(0 , uploadPath.indexOf('/'));
					
						if(folder){
							d["group"] = group + folder;
						}else{
							d["group"] = group + uploadPath;
						}
					
				}
				var newFile = module.limitFile([d]);
				if(newFile.length > 0){
					Config.Runtime.eventBus.emit(model,newFile);
				}
				
			})
		}
		
		module.treeTraverse = function(e , uploadPath , group){
			var that = this;
			var model = Config.Runtime.fileType + 'Upload';
			if(e.isDirectory){
				var newP = uploadPath;
				uploadPath ? newP = uploadPath + "/" + e.name : newP = e.name ;
			
				var redDirect = e.createReader();
				redDirect.readEntries(function(o){
			
					for(var t=0 ; t<o.length ; t++){
						that.treeTraverse(o[t] , newP , group);
					}
				})
				
			}else if(e.isFile){
				e.file(function(d){
					d["dragFile"] = true;
					d["dragPath"] = uploadPath;
					d["group"] = group;
					if(uploadPath){
						var folder = uploadPath.substring(0 , uploadPath.indexOf('/'));
						
							if(folder){
								d["group"] = group + folder;
							}else{
								d["group"] = group + uploadPath;
							}
						
					}
					var newFile = module.limitFile([d]);
					if(newFile.length > 0){
						Config.Runtime.eventBus.emit(model,newFile);
					}
				})
				
			}
		};
		
		module.getFiles = function(e){
			
			e.stopPropagation();
			e.preventDefault();
			var modelType = Config.Runtime.fileType;
			this.elContainer.style.display = "none"; 
			var model = modelType + 'Upload';
			e["drag"] = true;
			
			var path="";
			var group = tools.guidGenerator();
			
			if(modelType === "file" && !Config.Runtime.isUpload){
				return;
			}

			if(e.dataTransfer.items && $.isFunction(e.dataTransfer.items[0].webkitGetAsEntry) && (modelType === "home" || modelType === "file" || modelType === "image" || modelType === "music" || modelType === "video" || modelType === "document")){
				var files=[];
				for (var i = e.dataTransfer.items, n = 0; i.length > n; n++) {
                   
					var o = i[n].webkitGetAsEntry();	
					
					if(o.isDirectory){
						countFile.start(o);
					}else{
						this.sendDragFile(o ,path , group);
					}
					
					
                }
				
			}else{
				
				var files = e.target.files || e.dataTransfer.files;
				var newFile = module.limitFile(files);
				
				for(var k=0 ; k< newFile.length ; k++){
					console.log("newFile.size:" , newFile[k].size)
					module.sendFileFast(newFile[k] , model);
				}
				
			//	Config.Runtime.eventBus.emit(model, newFile);
			}
			
		};
		
		module.isFolder = function(files , model){
			 
			 var that = this;
			 
			 var set = function(){
	         	tools.tipMsg(appLanguage['changeLang']["noFolder"]);
	         	//Config.Runtime.eventBus.emit('upDir',{"upId" : path});
			 }
			 try{
				var fd = new FileReader;
	            fd.onerror = function (evt) {
	            	 switch(evt.target.error.code) {
		                 case evt.target.error.ABORT_ERR:
		                   //that.sendData(files , pro , path);
		                   break; // noop
	                 default:
	                	 set();
	               };
	            };
	            
	            fd.onabort = function(){
	    
	            	Config.Runtime.eventBus.emit(model, [files]);
	            }
	            
	            
	            fd.onprogress  = function(evt){
	            	if (evt.lengthComputable) {
	            	      var percentLoaded = Math.round((evt.loaded / evt.total) * 100);
	            	      if(percentLoaded > 5){
	            	    	  fd.abort();
	            	      }
	            	 }
	            }
	            
	            fd.onload = function () {
	            	Config.Runtime.eventBus.emit(model, [files]);
	            };
	            
	            fd.readAsDataURL(files);
			 }catch(e){}
		 }
		
		module.sendFileFast = function(newFile , model){
			console.log("newFile:" , newFile)
			 if(newFile.type == ''){
				 var ntype = newFile.name.substring(newFile.name.lastIndexOf('.'))
				 
				 if(ntype == '.rmvb' || ntype == '.mkv' ||(newFile.size && newFile.size > 1024*1024)){
					 Config.Runtime.eventBus.emit(model, [newFile]);
					 console.log("move:")
					 return;
				 }
				 
				 if(newFile.size == 0 || newFile.size == 1024){

					 tools.tipMsg(appLanguage['changeLang']["noFolder"]);
					 return;
				 }
				
				 module.isFolder(newFile , model);
				 
			 }else{
				 Config.Runtime.eventBus.emit(model, [newFile]);
			 }
		}
		
		module.limitFile = function(f){
			
			if(Config.Runtime.isDirect == true){
				return f;
			}else{
				var newF =[];
				for(var i=0 ; i< f.length ; i++){
					if(f[i]['size'] < Config.limitSize){
						newF.push(f[i]);
					}else{
						new tipMsg({"title": appLanguage["changeLang"]["tipTitle"], "content": appLanguage["changeLang"]["singFileSize"], "fn": ''});
					}
				}
			}
			
			return newF;
			
		};
		
		module.onUploadFolder = function(dir){
			var group = tools.guidGenerator();
			//module.treeTraverse(dir['dir']['obj'] ,"" , group);
			var files = dir["dir"]["files"];
			
			for(var i=0 ; i<files.length ; i++){
				module.sendDragFile(files[i]["file"] ,files[i]["path"], group);
			}
			
		};
		
		module.onOpenUpload = function(accept){
			var dom = document.getElementById("pic_uploads");
			$("#modelUpload").remove();
			dom.innerHTML = "<input type='file' name='Filedata' id='modelUpload'  multiple/>";
			switch(accept.model){
				case "image":
					document.getElementById("modelUpload").setAttribute("accept" , "image/*");
					break;
				case "video":
					document.getElementById("modelUpload").setAttribute("accept" , "video/*");
					break;
				case "music":
					document.getElementById("modelUpload").setAttribute("accept" , "audio/*")
					break;
			/*	case "doc":
					//console.log("doc")
					document.getElementById("modelUpload").setAttribute("accept" , "application/vnd.ms-works");
					break;
				case "apk":
					document.getElementById("modelUpload").setAttribute("accept" , "*.apk");
					break;
				*/	
				default:
					document.getElementById("modelUpload").setAttribute("accept" , ".")
					break;
			}
			
			$("#modelUpload").change(function(e){
				uploadFiles.submitImgChange(e)
			})
			document.getElementById("modelUpload").click();
		}
		
		
	var uploadFiles = avalon.define('upload_files', function(vm){
		vm.title = "";
		vm.accept = "";
		vm.submitImgChange = function(e){
			
			var model = Config.Runtime.fileType + 'Upload';
			
			var files = e.target.files || e.dataTransfer.files;
			var newFile = module.limitFile(files);
			Config.Runtime.eventBus.emit(model, newFile);
			/*
			if(!window.addEventListener){
				document.getElementById("modelUpload").outerHTML='';
			}else{
				document.getElementById("modelUpload").value='';
			}
			*/
		};
		
		vm.initListeners = function (bus){
			bus.on("openUpload", module.onOpenUpload);
			bus.on("upLoadFolder", module.onUploadFolder);
		};
	});
	return module;
});

