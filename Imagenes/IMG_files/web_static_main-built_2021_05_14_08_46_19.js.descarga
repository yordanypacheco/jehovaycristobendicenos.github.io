				ackImgPath = null;
			})
			*/
		};
		
		vm.initListeners = function(bus){
			var self = contactCenterController;
			//console.log("init control")
			bus.on("contactList", self.onContactList);
			
			bus.on("loadMessage", self.onLoadMessage);
			bus.on("selectAll", self.onSelectAll);
			
			bus.on("refreshContact", self.onRefreshContact);
			bus.on("deleteContact", self.onDeleteContact);
			bus.on("newContact", self.onNewContact);
			bus.on("fileurl", self.onFileUrl);
			bus.on("ack" , self.onAck);
			
			bus.on("language" , module.onLanguage);
			bus.on("uploadCropImg", self.uploadCropImg);
			
			bus.on("smsUpload", module.onSmsUpload);
			
			bus.on("NewContactStatus" , self.onNewContactStatus);
			bus.on("EditContactStatus" , self.onEditContactStatus);
			bus.on("DeleteContactStatus" , self.onDeleteContactStatus);
			
			bus.on("uploadInfo" , self.onUploadInfo);
			
			Config.Runtime.eventBus.emit('addModel',module);
			
			self.init();
		};
	});
	
	
	return module;
});

