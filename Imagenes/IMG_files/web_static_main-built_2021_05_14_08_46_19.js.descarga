        var module = {};
        module.init = function(container) {
            login.init(document.getElementById('loginPanel'));
            mainScreen.init(document.getElementById('mainScreen'));

            APPController.resizeWindow();
            window.onresize = function() {
                APPController.resizeWindow();
                //tools.logs("onresize: start");

                var dms = dm.single.getInstance();

                if (dms.loadApp()) {
                    appModule.onResize();
                    return;
                }
                tools.scrollReinit("groupPicList");
                //			$("#videoModule_file_list").jScrollPane();
                login.onResize();
                musicModule.onResize();
                docModule.onResize();
                //			smsModule.onResize();			delete by wlm 01.20.2015
                appModule.onResize();
                videoModule.onResize();
                fileModule.onResize();

                imgModule.imgC.onResize();
                help.onResize();
                Config.Runtime.eventBus.emit('moduleResize', {});
            }
        };

        var APPController = avalon.define("APPController", function(vm) {
            // home, img, music, doc
            //	tools.logs("APP:" , APP.Config.minWidth-202)
            vm.pageHeight = Config.Config.minWidth;
            vm.pageWidth = Config.Config.minHeight;
            vm.rightWidth = Config.Config.minWidth - 60;
            vm.rightListHeight = Config.Config.minHeight - 109;
            vm.channelstatus = Config.Runtime.channelstatus;
            vm.musicListHeight = Config.Config.minHeight - 176;
            vm.docListHeight = Config.Config.minHeight - 55;
            vm.isLogin = false;
            vm.title = "";

            vm.loadPage = function() {
                document.getElementById("loginPanel").style.display = "none";
                this.isLogin = true;
            };

            vm.resizeWindow = function() {
                //alert(Tools)
                APPController.pageHeight = tools.getPageHeight();
                APPController.pageWidth = tools.getPageWidth();
                //APPController.rightWidth =  tools.getPageWidth() - 60

                APPController.rightWidth = ((Config.Config.minWidth > APPController.pageWidth) ? Config.Config.minWidth - 60 : APPController.pageWidth - 60);
                APPController.rightListHeight = ((Config.Config.minHeight > APPController.pageHeight) ? Config.Config.minHeight : APPController.pageHeight) - 109;
                APPController.musicListHeight = ((Config.Config.minHeight > APPController.pageHeight) ? Config.Config.minHeight : APPController.pageHeight) - 203;
                APPController.docListHeight = ((Config.Config.minHeight > APPController.pageHeight) ? Config.Config.minHeight : APPController.pageHeight) - 55;
            };

            vm.onOpened = function() {
                APPController.channelstatus = "channel已连接";
            };

            vm.onPhoneConnected = function(data) {
                var self = APPController;
                imgModule.imgC.noneIos = data.channel;
                videoModule.videoModuleController.noneIos = data.channel;
                musicModule.musicModuleController.noneIos = data.channel;
                if (!tools.getCookie("language")) {
                    var lang = data.language.toLowerCase();
                    language.change(lang, true);
                }
                tools.logs("data.channel = " + data.channel);
                //preLoad.image(); //预加载照片列别;
                tools.logs("preload:")

                Config.uploadpath = data['uploadpath'];
                //console.log("Config.uploadpath:" , Config.uploadpath)
                APPController.loadPage();
                APPController.channelstatus = "手机已连接!";
                console.log("手机连接成功---")
                setTimeout(function() {
                    try {
                        if (!Config.Runtime.isDirect) {
                            console.log("云联")
                            ga('send', 'event', 'cloudConnectPhone')
                            if (Config.Runtime.connect == "W") {
                                console.log("wifi 云联")
                                ga('send', 'event', 'cloudWifi')
                            }
                        }
                    } catch (e) {

                    }
                }, 3000)
                Config.Runtime.channelV = data.channel;
                channelV.setV();
            };

            vm.onPhoneDisconnected = function(data) {
                //			window.location.href= Config.Config.baseUrl + "webclient?action=logout"
                mainScreen.logout(data);
            };

            vm.onLanguage = function() {
                APPController.title = Config.Runtime.isWepai ? appLanguage["changeLang"]['wepai'] : appLanguage["changeLang"]['welineTitle'];
            };

            vm.initListeners = function(bus) {
                //			tools.debugOn();
                APPController.title = Config.Runtime.isWepai ? appLanguage["changeLang"]['wepai'] : appLanguage["changeLang"]['welineTitle'];
                bus.on("opened", APPController.onOpened);
                bus.on("phoneConnected", APPController.onPhoneConnected);
                bus.on("phoneDisconnected", APPController.onPhoneDisconnected);
                bus.on("language", APPController.onLanguage);
            };
        });

        return module;
    });
