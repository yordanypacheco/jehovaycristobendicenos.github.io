        module.singleSelect = function(o, idx) {
            var d = docModuleController;

            if (d.selectClass[idx] === 'selected') {
                d.selectedItem.remove(idx);
                d.selectClass.set(idx, null);
                module.clearListItemStyle(idx);
            } else {
                d.selectedItem = [];
                d.selectClass = [];
                d.selectedTop = [];
                d.selectedItem.push(idx);
                d.selectClass.set(idx, 'selected');
            }

            if (d.docList.length === 1) {
                d.isSelectAll = true;
            } else {
                d.isSelectAll = false;
            }

        };

        module.multiSelect = function(o, idx) {
            var d = docModuleController;
            if (d.selectClass[idx] === 'selected') {
                d.selectedItem.remove(idx);
                d.selectClass.set(idx, null);

                d.selectedTop.set(idx, null);
                module.clearListItemStyle(idx);
            } else {
                d.selectedItem.push(idx);
                d.selectClass.set(idx, 'selected');

                module.setListItemStyle(d.selectedItem[d.selectedItem.length - 1]);

                if (d.selectedItem.length === d.docList.length) {
                    d.isSelectAll = true;
                } else {
                    d.isSelectAll = false;
                }
            }

        };

        module.setListItemStyle = function(pos) {
            var d = docModuleController;
            var beforeEle = pos - 1;
            var afterEle = pos + 1;

            if (d.selectedItem.length < 2) {
                return;
            } else {
                if (d.selectClass[beforeEle]) {
                    d.selectedTop.set(pos, 'noTop');
                }
                if (d.selectClass[afterEle] && afterEle < d.docList.length) {
                    d.selectedTop.set(afterEle, 'noTop');
                }
            }
        };

        module.clearListItemStyle = function(len) {
            var d = docModuleController;
            var last = len;
            var afterEle = last + 1;

            if (d.selectedItem.length < 2) {
                return;
            } else {
                if (d.selectClass[afterEle]) {
                    d.selectedTop.set(last, null);
                    d.selectedTop.set(afterEle, null);
                } else {
                    d.selectedTop.set(last, null);
                }
            }
        }


        module.onUploadDoc = function(files) {
            var self = docModuleController;

            var newDoc = [];

            for (var i = 0; i < files.length; i++) {
                var type = files[i].name.substring(files[i].name.lastIndexOf("."))
                    // if(!(files[i].type.indexOf("image") == 0 || Config.vMap.get(files[i].type) || Config.music[type])){
                if (Config.map.get(type) && Config.map.get(type).indexOf('text') === 0) {
                    newDoc.push(files[i]);
                } else {
                    tools.tipMsg(appLanguage['changeLang']["onlyDoc"]);
                }
            }

            if (newDoc.length == 0) {
                // tools.tipMsg(appLanguage['changeLang']["noFolder"]);
                return
            }
            self.queue.push(newDoc);

            if (self.totalLen == 0) self.disLen = 0;

            self.totalLen = self.totalLen + newDoc.length;
            self.disLen = self.disLen + newDoc.length;
            if (self.leftLen <= 0) {
                module.startUpload();
            }
        }

