			}, 500);
		};
		
		vm.onNotificationCenter = function(data){
			var self = noCenterController;
			if(data.visible){
				self.visible = data.visible;
				
				module.setListHeight();
				
				if(self.notificationList.length == 0){
					self.isEmpty = true;
				}else{
					self.isEmpty = false;
				}
			}else{
				self.visible = data.visible;
				self.isEmpty = false;
			}
		};
		
		vm.stopEvent = function(e){
			var e = e || window.event;
			e.stopPropagation();
		};
		
		vm.onNotificationTip = function(data){
			var self = noCenterController;
			self.visibleTip = true;
			if(self.visibleTip){
				setTimeout(function(){
					module.fadeOut("notificationTip", 500);
				}, 3000);
			}
			
			self.newTip = data;
			
			self.notificationList.push(data);
			
			self.seq_icon_path[self.notificationList[self.notificationList.length - 1].icon] = true;

			var pathArr = [self.notificationList[self.notificationList.length - 1].icon];
			module.getUrl(pathArr);
		};
		
		vm.onFileurl = function(data){
			var self = noCenterController;
//			console.log("data.url = " + data.url);
			if(self.seq_icon_path[data.path]){
				delete self.seq_icon_path[data.path];
				
				for(var i = 0; i < self.notificationList.length; i++){
					if(data.path == self.notificationList[i].icon){
						var newObj = self.notificationList[i];
						newObj.icon = data.url;
						
						self.newTip.icon = data.url;
						self.notificationList.set(i, newObj);
					}
				}
			}
		};
		
		vm.initListeners = function(bus){
			var self = noCenterController;
			bus.on("visibleNotificationCenter", self.onNotificationCenter);
			bus.on("notificationTip", self.onNotificationTip);
			bus.on("fileurl", self.onFileurl);
		};
	});
	
	noCenterController.notificationList.$watch("length", function(){
		module.setListHeight();
	});
	
	return module;
});

