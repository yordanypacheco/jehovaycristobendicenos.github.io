    module.renameOrcreate = function() {
        var fileC = fileModuleController;
        if (fileC.fileItemStatus === "normal") {
            return;
        }

        var items = $(".file_item");

        if (fileC.fileItemStatus == "create") {
            if (items.length < 1) {
                return;
            }

            var input = $(".file_item:first .file_createFolder input")[0].value;
            input = input.replace(/[ ]/g, "");

            if (input == "") {
                module.clearCreate();

                module.clearStatus();
                return;
            }

            fileC.createFolderFn(input);

            return;
        }

        if (fileC.fileItemStatus == "rename") {
            if (fileC.selectedIndex.length < 1 || items.length < 1) {
                return;
            }

            var input = $(".file_item:eq(" + fileC.selectedIndex[0] + ") .file_rename input")[0].value;
            input = input.replace(/[ ]/g, "");

            if (input == "") {
                module.clearStatus();
                return;
            }

            module.renameFileFn(input);

            //			$(".file_item").removeClass("selected file_status_rename file_status_create");
            //			fileC.fileItemStatus = "normal";
            module.clearStatus();

            return;
        }
    };

    module.renameFileFn = function(v) {
        var fileC = fileModuleController;

        if (!module.checkFileName(v)) {
            return;
        }

        if (module.hasName(v) != 0) {
            if (module.hasName(v) === 1) {
                return;
            }
            tools.tipMsg(appLanguage["changeLang"]["renameError"]);
            return;
        }

        module.renameItem.newName = v;

        var m = {};
        m.type = "fileRename";
        m.data = module.renameItem;

        tools.sendMsgToChannel(m);

        // 记录重命名操作
        var oldPath = module.renameItem.path + "/" + module.renameItem.oldName;
        listTree.pathHistory["rename"][oldPath] = module.renameItem;

        var filePath = module.renameItem.path + "/" + module.renameItem.newName;
        fileC.currentPathFiles.set(fileC.selectedIndex[0], {
            name: v,
            path: filePath
        });

        if (module.renameItem.isFile == "false") {
            listTree.pathFolderList[fileC.currentPath][fileC.selectedIndex[0]].name = module.renameItem.newName;
            listTree.pathFolderList[fileC.currentPath][fileC.selectedIndex[0]].path = filePath;

            var nv = $("#" + listTree.getPath(oldPath) + " .tree_folder_name:first");
            $(nv).html(module.renameItem.newName);
            $("#" + listTree.getPath(oldPath)).attr("id", filePath);

            listTree.pathFileList[fileC.currentPath] = fileC.currentPathFiles.slice(0);
        } else {
            listTree.pathFileList[fileC.currentPath] = fileC.currentPathFiles.slice(0);
        }

        //		$($(".file_item").get(0)).removeClass("file_status_rename");
        $("#file_item_container").focus();
        fileC.selectedIndex.splice(0, fileC.selectedIndex.length);
        fileC.fileItemStatus = "normal";
    };

