define('eventMag',["config", "tools"], function(Config, tools){
	var eventManager = {};
	
	eventManager.elist = {};
	
	eventManager.init = function(){
		eventManager.buttonsEvent();
		eventManager.opEvent();
		eventManager.bodyEvent();
	};
	
	eventManager.buttonsEvent = function(){
		$(".buttons, .buttons_gray, .downUp").die("mousedown mouseup mouseout").live("mousedown mouseup mouseout", function(e){
			var e = e || window.event;
			var es = e.target || e.srcElement;
			if(e.type == 'mousedown'){
				$(es).removeClass("pressed").addClass("pressed");
				return;
			}
			if(e.type == 'mouseup'){
				$(es).removeClass("pressed");
				return;
			}
			if(e.type == 'mouseout'){
				$(es).removeClass("pressed");
				return;
			}
		});
	};
	
	eventManager.opEvent = function(){
		$(".op").die("mousedown mouseup mouseout").live("mousedown mouseup mouseout", function(e){
			var e = e || window.event;
			var es = e.target || e.srcElement;
		
			e.stopPropagation();
			if(e.type == 'mousedown'){
				$(es).parent().removeClass("pressed").addClass("pressed");
			}
			if(e.type == 'mouseup'){
				$(es).parent().removeClass("pressed");
				return;
			}
			if(e.type == 'mouseout'){
				$(es).parent().removeClass("pressed");
				return;
			}
		});
	};
	
	eventManager.bodyEvent = function(){
		var self = eventManager;
		$("body").die().live("click", function(e){
//			console.log("document click event");
			$("#appBar li").removeClass("pressed");

			tools.sendAppBarEventBus('all', false);
			
			if(Config.Runtime.fileType == "file"){
				Config.Runtime.eventBus.emit('clearSelected',{"clearAll": true});
			}
		});
		$("#feedbackModule, #music_panel_container").unbind().bind("click", function(e){
			e.stopPropagation();
		});
	};
	
	return eventManager;
});
