                        flag = true

                        if (self.albums[0].title == '') {
                            self.albums[0].title = file.date;
                        }

                        var savePath = self.noSavePath[upId];

                        if (self.pictureDis && self.photoDis) {
                            if (f.isCamera) fileImage.files.splice(0, 0, file);
