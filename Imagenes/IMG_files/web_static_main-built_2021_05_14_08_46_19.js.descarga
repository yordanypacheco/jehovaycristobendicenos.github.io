        module.downloadAnim = function(index) {
            var self = docModuleController;
            var dom = $("#doc_list_body ul");
            var parent = document.getElementById("listCenter");
            var docH = document.getElementById("doc_list_body").style.height;
            var scrollTop = $("#doc_list_body .jspPane").get(0).style.top;
            var setValue = function(dex) {
                var D = dom[dex];

                var width = parseInt(parent.style.width);
                var top = D.offsetTop;
                var T = top + parseInt(scrollTop);
                if (T < -40 || T > parseInt(docH)) {
                    return;
                }

                var dom1 = D.cloneNode(true);
                parent.appendChild(dom1)
                $(dom1).css({ "position": "absolute", "left": 18, "top": top, "width": width })
                module.animDelEl(dom1)
            }

            if (index == undefined) {
                for (var i = 0; i < self.selectedItem.length; i++) {
                    setValue(self.selectedItem[i]);
                }
            } else {
                setValue(index);
            }
        };

