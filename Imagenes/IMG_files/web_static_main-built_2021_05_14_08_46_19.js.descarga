    module.singleSelect = function(o, idx) {
        var m = musicModuleController;
        if (m.selectedClass[idx] === "selected") {
            m.selectedItem.remove(idx);
            m.selectedClass.set(idx, null);
            module.clearListItemStyle(idx);
        } else {
            m.selectedItem = [];
            m.selectedItem.push(idx);
            m.selectedClass = [];
            m.selectedTop = [];
            m.selectedClass.set(idx, "selected");
        }
    };

    module.multiSelect = function(o, idx) {
        var m = musicModuleController;

        if (m.selectedClass[idx] === "selected") {
            m.selectedItem.remove(idx);
            m.selectedClass.set(idx, null);
            m.selectedTop.set(idx, null);
            module.clearListItemStyle(idx);
        } else {
            m.selectedItem.push(idx);
            m.selectedClass.set(idx, "selected");
            module.setListItemStyle(m.selectedItem[m.selectedItem.length - 1]);
        }
    };

    module.setListItemStyle = function(pos) {
        var m = musicModuleController;
        var beforeEle = pos - 1;
        var afterEle = pos + 1;

        if (m.selectedItem.length < 2) {
            return;
        } else {
            if (m.selectedClass[beforeEle]) {
                m.selectedTop.set(pos, "noTop");
            }
            if (m.selectedClass[afterEle] && afterEle < m.musicList.length) {
                m.selectedTop.set(afterEle, "noTop");
            }
        }
