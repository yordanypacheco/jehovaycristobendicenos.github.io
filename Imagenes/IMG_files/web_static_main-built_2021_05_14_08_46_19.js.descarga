            { "code": "refresh", "name": "", "title": appLanguage['changeLang']["fresh"] }
        ];

        module.init = function(container) {
            container.innerHTML = tpl_docModule;
            //module.setWords();
            module.api = tools.scrollInitNotAuto('doc_list_body');
        };

        module.onLanguage = function() {

            var self = docModuleController;
            module.title = appLanguage['changeLang']["docModule"];
            module.barButtons = [
                { "code": "docs_add", "name": appLanguage['changeLang']["uploadDoc"], "title": appLanguage['changeLang']["docToPhone"] },
                { "code": "export_disable", "name": "", "title": appLanguage['changeLang']["export"] },
                { "code": "delete_disable", "name": "", "title": appLanguage['changeLang']["delete"] },
                { "code": "refresh", "name": "", "title": appLanguage['changeLang']["fresh"] }
            ];
            if (module.fileType == Config.Runtime.fileType) {

                Config.Runtime.eventBus.emit('buttonStatus', { data: module.barButtons, fileType: "document" });
            }

        };

        module.load = function() {
            module.time = null;
            if (docModuleController.docList.length > 0) {
                //			Config.Runtime.eventBus.emit('fileList',{'data':'0'});
                module.api.reinitialise();
                //			return;
            } else {
                var m = {};
                m.type = 'requestFileList';
                m.data = 'document';
                tools.sendMsgToChannel(m);
