		var winWidth =  tools.getPageWidth();
		var winHeight = tools.getPageHeight();
		var imgWidth = img.width;
		var imgHeight = img.height;

		var rect = {};
		
		var winRatio = winWidth / winHeight;
		
		var imgRatio = imgWidth / imgHeight;
		
		if( winRatio >= imgRatio ){
			
			rect.height = winHeight;
			rect.width = imgWidth / imgHeight * rect.height;
		}else{
			rect.width = winWidth;
			rect.height = imgHeight / imgWidth * rect.width;
		}
		
		rect.top = (winHeight - rect.height) / 2;
		rect.left = (winWidth - rect.width) /2;
		if(img.bigImgRato == "270" || img.bigImgRato == "90"){
			rect.top = (winHeight - rect.width) / 2;
			rect.left = (winWidth - rect.height) /2;
