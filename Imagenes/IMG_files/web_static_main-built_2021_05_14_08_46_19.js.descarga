define('countFile',[
        "config","tools" ,  "tipMsg" ,  "appLanguage"],
function(
		Config,tools , tipMsg ,  appLanguage){
	var countFile={};
	var dir = {
	}
	countFile.traverse = function(e ,group , uploadPath){
	//	var newP = uploadPath;
		var that = this;
		if(uploadPath == ""){
			dir[group] = {
				obj:e,
				count:0,
				files:[],
				back:0
			}
		}
		
		var newP = uploadPath;
		uploadPath !="" ? newP = uploadPath + "/" + e.name : newP = e.name ;
		
		var redDirect = e.createReader();
		redDirect.readEntries(function(o){
			if(uploadPath !="") dir[group]['back'] = dir[group]['back'] -1;
			
			if(dir[group]['count'] > 3000){
				tools.tipMsg(appLanguage['changeLang']["over3000"]);
				return;
			}
			
			if(o.length == 0){
				tools.tipMsg(appLanguage['changeLang']["emptyFolder"]);
				return
			}
			
			for(var t=0 ; t<o.length ; t++){
				if(o[t].isDirectory){
					that.traverse(o[t] , group , newP);
					dir[group]['back'] = dir[group]['back'] + 1;
				}else{
					dir[group]['files'].push({
						file:o[t],
						path:newP
					})
					dir[group]['count']++;
				}
				
			}
			
			if(dir[group]['back'] ==0){
				Config.Runtime.eventBus.emit("upLoadFolder",{"dir":dir[group]});
				delete dir[group] ;
			}
		})
	}
	
	countFile.start = function(o){
		var group = tools.guidGenerator() + o.name;
		this.traverse(o , group , "")
	}
	return countFile;	
});
