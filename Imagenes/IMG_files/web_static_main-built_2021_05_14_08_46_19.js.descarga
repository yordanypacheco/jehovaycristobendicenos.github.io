    module.onFileUpload = function(files) {
        var self = fileModuleController;
        var uploadPath = self.currentPath.substring(0, cTree.titlePath.length);
        console.log("self followPrompts  .curentPath xxxx x:", self.currentPath, self.rootName);

        // if (self.rootName.indexOf("Permission needed") > 0) {
        if (self.rootName.indexOf('(') > 0 && self.rootName.indexOf(')') > 0) {
            tools.tipPanel(appLanguage["changeLang"]["followPrompts"], appLanguage["changeLang"]["Jurisdiction"]);
            return;
        }

        var setLastedClass = function() {
            $($(document.getElementById(cTree.titlePath)).find("div.tree_root")[0]).removeClass("selected");
            $($(document.getElementById(cTree.titlePath)).find("div.category_leaf")).removeClass("selected");
            $($(document.getElementById(cTree.getLastPath())).find("div.category_leaf")[0]).addClass("selected");
            self.currentPath = cTree.getLastPath();

            self.currentPathFiles = cTree.categoryList[self.currentPath];
        };

        if (cTree.titlePath === uploadPath) {
            var c = $(document.getElementById(cTree.titlePath))
                .children("div")
                .first();
            var $c = $(c);
            if (self.currentPath === cTree.titlePath) {
                if ($c.hasClass("category_close")) {
                    cTree.openCloseLeaf(cTree.titlePath, "showTree", self);
                    setLastedClass();
                    module.setCrrentPath();
                } else {
                    setLastedClass();
                    module.setCrrentPath();
                }
            } else {
                setLastedClass();
            }
        }

        var newFiles = [];
        for (var i = 0; i < files.length; i++) {
            if (files[i]["dragFile"]) {
                if (files[i]["dragPath"]) {
                    if (!self.uploadFolder[files[i]["group"]]) {
                        var newName = self.findSameName(files[i]["dragPath"]);
                        self.uploadFolder[files[i]["group"]] = {
                            val: 1,
                            size: 0,
                            total: 0,
                            newName: newName
                        };
                    } else {
                        self.uploadFolder[files[i]["group"]]["val"] = self.uploadFolder[files[i]["group"]]["val"] + 1;
                    }
                }
            }

            if (cTree.titlePath === uploadPath) {
                files[i].savePath = "uploadpath";
            } else {
                files[i].savePath = self.currentPath;
            }

            if (files[i].size > 0) {
                newFiles.push(files[i]);
            }
        }

        if (newFiles.length == 0) {
            tools.tipMsg(appLanguage["changeLang"]["noFileSize"]);
            return;
        }
        self.isEmpty = false;

        module.onAsyncUpload(newFiles);
        //var upFile = new upModel();
        //upFile.upload(newFiles, self);
    };

