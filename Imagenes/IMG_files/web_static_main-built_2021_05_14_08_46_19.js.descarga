define('ctCenter',[
		"tools", 
		"config",
		"appLanguage",
        "text!../template/connectionType-center.html"
		], 
	function(
    		tools, 
    		Config,
    		appLanguage,
			tpl_connectionType){
	
	var module = {};
	module.name = "connection";
	module.typeList = [
	                   {"type": "direct"},
	                   {"type": "cloud"}
	                  ];
	module.init = function(container){
		container.innerHTML = tpl_connectionType;
	};
	
	module.fadeOut = function(id, time){
		var cc = ctController;
		
		var dom = document.getElementById(id);
		
		var af = tools.animationFrame();
		var i = time/10;
		var opacity = 1/i;
		var j=1;
		
		var runAnim = function(time){
			op = 1 - opacity * j;
			
			if(op <= 0){
				cc.visible = false;
				dom.style.opacity = 1;
				return;
			}
			
			if(j < i){
				dom.style.opacity = op;
				af(runAnim);
				j++;
			}
		}
		af(runAnim);
	};
	
	module.onLanguage = function(){
		var cc = ctController;
	};
	module.show = function(){
		var ctc = ctController;
		
		ctc.visible = true;
		module.fadeOut("connectionType_center", 2000);
		
	};
	
	var ctController = avalon.define("ctCenter", function(vm){
		vm.visible = false;
		vm.cType = module.typeList[1]['type'];
		
		vm.stopEvent = function(e){
			var e = e || window.event;
