        vm.deleteMusic = function() {
            var self = musicModuleController;

            var deleteOne = function() {
                self.delMusicSendMesg([self.musicList[module.deleteId].path]);

                for (var i = 0; i < self.selectedItem.length; i++) {
                    if (self.selectedItem[i] == module.deleteId) {
                        self.selectedItem.splice(i, 1);
                    }

                    if (self.selectedItem[i] > module.deleteId) {
                        self.selectedClass.set(self.selectedItem[i], "");
                        self.selectedItem[i] = self.selectedItem[i] - 1;
                        self.selectedClass.set(self.selectedItem[i], "selected");
                    }
                }
                self.musicList.splice(module.deleteId, 1);

                if (self.musicList.length === 0) {
                    module.stopMusic();
                    self.isEmpty = true;
                    module.api.reinitialise();
                    self.selectedItem = [];
                    self.curIdx = -1;
                    self.curPath = "";
                    return;
                }

                if (self.isAudition) {
                    if (module.deleteId == self.curIdx) {
                        $("#jquery_jplayer_1").jPlayer("stop");

                        if (self.musicList.length >= self.curIdx) {
                            self.curIdx = 0;
                            module.sendRequestFile(self.curIdx);
                        } else {
                            self.curIdx = self.musicList.length > 0 ? self.musicList.length - 1 : 0;
                            self.isAudition = false;
                        }
                    } else if (module.deleteId < self.curIdx) {
                        self.curIdx--;
                    }
                } else {
                    if (self.musicList.length <= self.curIdx) {
                        self.curIdx = self.musicList.length > 0 ? self.musicList.length - 1 : 0;
                        self.isAudition = false;
                    }
                    module.sendRequestFile(self.curIdx);
                }

                module.api.reinitialise();
            };

            $("#list_body ul:eq(" + module.deleteId + ")").slideUp(300, function() {
                deleteOne();
            });
        };

