			})
			
		};
		
		
		vm.delSelectVideo = function(par){
			var self = videoModuleController;
			
			var data = [];
			
			//self.videoSelect.reverse();
			
			self.videoSelect.sort(function(x , y){
				if(x > y){
					return -1;
				}else if(x < y){
					return 1;
				}else{
					return 0;
				}
					
			});
			
			if(par.config !=null){     //
				self.delVideoSendMesg([self.files[par.config].path]);
				self.files.splice(par.config , 1);
				self.delParam(par.config);
				if(self.videoSelect.length == 0)self.setDleteClass();
				return
			}
			
			for(var i=0 ; i<self.videoSelect.length ; i++){
				var index = self.videoSelect[i];
				var path = self.files[index].path;
				data.push(path);
				self.delParam(index);
				self.files.splice(index , 1);
			}	
		
			
			if(self.files.length == 0)
				self.isShowVideo = false;
			
			self.videoSelect.length = [];
			self.delVideoSendMesg(data);
			
			self.setDleteClass();
			if(self.posterUrls.srcs.length ==0) self.selecteClass ="";
			
		};
		
		
