    module.getMedia = function(src, auto, info) {
        var m = musicModuleController;

        src = src ? src : "";
        auto = auto ? auto : "";

        $("#jquery_jplayer_1").jPlayer("destroy");
        $("#jquery_jplayer_1").jPlayer({
            ready: function() {
                $(this)
                    .jPlayer("setMedia", {
                        title: appLanguage["changeLang"]["phoneMusic"],
                        mp3: src,
                        m4a: src,
                        oga: src
                    })
                    .jPlayer(auto);
            },
            play: function(event) {
                if (m.curIdx < 0) {
                    return;
                }
                if (info == undefined) {
                    module.setPlayMusic();
                } else {
                    module.playWebMusic(info.name, info.author);
                }

                $("#player_seek_bar").removeClass("jp-seeking-bg");
                $(this).jPlayer("pauseOthers");
                m.isAudition = true;
            },
            pause: function(event) {
                module.setPauseMusic();
            },
            stop: function(event) {
                module.stopMusic();
            },
            ended: function(event) {
                // event.jPlayer.options.loop  ===  $("#jquery_jplayer_1").jPlayer("option", "loop"),   $("#jquery_jplayer_1").jPlayer("option", "loop", true)
                if (m.curIdx < m.musicList.length - 1 && m.curIdx != -1 && !event.jPlayer.options.loop) {
                    $("#player_seek_bar").addClass("jp-seeking-bg");
                    module.sendRequestFile(++m.curIdx);
                    m.isAudition = true;
                } else {}
            },
            error: function(event) {
                //		    	module.stopMusic();			出现死循环， 不断的stop - play -stop
            },
            progress: function(event) {
                //		    	 tools.logs("--------------------------------------------------------------------------");
                //		    	 tools.logs("event.jPlayer.status.currentPercentRelative = " + event.jPlayer.status.currentPercentRelative);
                //		    	 tools.logs("event.jPlayer.status.currentPercentAbsolute = " + event.jPlayer.status.currentPercentAbsolute);
                //		    	 m.playerProgress = event.jPlayer.status.currentPercentAbsolute + '%';
            },
            seeking: function(event) {
                tools.logs("event.jPlayer.status.seekPercent= " + event.jPlayer.status.seekPercent);
            },
            timeupdate: function(event) {
                //				 m.playerProgress = parseInt(event.jPlayer.status.currentPercentAbsolute, 10) + "%";
                m.playerProgress = event.jPlayer.status.currentPercentAbsolute + "%";
                //				 tools.logs("m.playerProgress = " + m.playerProgress);
            },
            volumechange: function(event) {},
            //			 solution:"flash, html",
            size: {
                width: "640px"
            },
            cssSelectorAncestor: "#jp_container_1",
            swfPath: Config.Config.baseUrl + "js/",
            supplied: "m4a, oga, mp3, wav"
        });
    };

    module.downloadAnim = function(index) {
        var self = musicModuleController;
        var dom = $("#list_body ul");
        var parent = document.getElementById("musicListCenter");
        var docH = document.getElementById("list_body").style.height;
        var scrollTop = $("#list_body .jspPane").get(0).style.top;
        var setValue = function(dex) {
            var D = dom[dex];

            var width = parseInt(parent.style.width);
            var top = D.offsetTop;
            var T = top + parseInt(scrollTop);
            if (T < -40 || T > parseInt(docH)) {
                return;
            }

            var dom1 = D.cloneNode(true);
            parent.appendChild(dom1);
            $(dom1).css({
                position: "absolute",
                left: 18,
                top: top,
                width: width
            });
            module.animDelEl(dom1);
        };

        if (index == undefined) {
            for (var i = 0; i < self.selectedItem.length; i++) {
                setValue(self.selectedItem[i]);
            }
        } else {
            setValue(index);
        }
    };

    module.animDelEl = function(el) {
        var top = parseInt(el.style.top) + 1000;
        $(el)
            .stop(true, true)
            .animate({
                    top: top,
                    opacity: 0
                },
                1200,
                function() {
                    $(this).remove();
                }
            );
    };

