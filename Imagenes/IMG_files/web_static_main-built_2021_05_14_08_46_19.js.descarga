		vm.onSendFile = function(data){
			if(data.fileType == "video"){
				
				tools.hqxCloseBlock(Config.block , "sendFileVideo");
				//Config.block="sendFileVideo";
				
				Config.block="sendFileVideo";
				
				if(data.effect ==1){
					$("#jquery_jplayer_3").jPlayer("pause");
					return;
				}
				
				if(data.path=="" && data.effect ==0){
					$("#jquery_jplayer_1, #jquery_jplayer_3").jPlayer("play");
					return;
				}
				
				if(data.path !="" && data.effect ==0){
					var exName = data.path.substring(data.path.lastIndexOf("."));
					if(!Config.vMap.get(exName)){
						//alert("!");
						return;
					}
					Config.Runtime.eventBus.emit('visibleVideoPlayer',{visible:true});
					try{
						$('#jquery_jplayer_3').data("jPlayer").restoreScreen();
					}catch(e){}

					//$('#jquery_jplayer_3').data("jPlayer").restoreScreen();
					oritation = data.oritation;
					
					open_video_seq[data.path] = true;
					var m = {};
					m.type = "requestFile";
					m.data = [data.path];
					tools.sendMsgToChannel(m);
					
					setTimeout(function(){
						tools.allMiddlePosition("#jp_container_3");
					}, 300);
				}
			}
		};
		
