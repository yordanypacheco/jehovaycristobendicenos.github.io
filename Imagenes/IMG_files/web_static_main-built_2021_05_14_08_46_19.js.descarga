                            self.delUploadFile();
                            self.leftLen = 0;
                            return
                        }

                        self.files.splice(i, 1, file);
                        data.push(self.files[i]["thumbpath"]);
                        flag = true

                        if (self.albums[0].title == '') {
                            self.albums[0].title = file.date;
                        }

                        var savePath = self.noSavePath[upId];

                        if (self.pictureDis && self.photoDis) {
                            if (f.isCamera) fileImage.files.splice(0, 0, file);
                        }

                        Config.Runtime.eventBus.emit('addPic', f); //;


                        try {
                            var dom = document.getElementById(upId);
                            var progress = document.getElementById("progress_" + upId);
                            dom.parentNode.removeChild(dom);
                            progress.parentNode.removeChild(progress);
                        } catch (e) {}

                        ackPath[self.files[i]["thumbpath"]] = i;

                        self.albums[0].addHead(i, file);

                        var m = {};
                        m.type = "requestFile";
                        m.data = data;
                        tools.sendMsgToChannel(m);

                    }
                }

                Config.Runtime.eventBus.emit('refreshUpload', f)

            };

            vm.setScrollDiv = function() {
                document.getElementById("imgModule_file_list").scrollTop = 100;
            };

            vm.initListeners = function(bus) {

                var self = imgModuleController;
                bus.on("fileList", self.onFileList);
                bus.on("fileurl", self.onFileurl);
                bus.on("imageUpload", module.onUploadImg);
                bus.on("changeImg", self.onChangeImg);
