}" ms-visible="!isEmpty">\n                    <div class="file_item" ms-repeat="currentPathFiles" ms-click="clickSelect($index, $event)" ms-mouseenter="showItemDetail($index, $event)" ms-dblclick="dblclickItem($index, $event)" ms-attr-path="{{el.path}}">\n                        <div ms-class-1="{{el.isFile == \'false\' ? (\'folder_item_size\' + \' \' + \'folder_item_icon\') : (\'file_icon_size\' + \' \' + el.type + \'_item_icon\')}}" ms-title="{{el.name}}"></div>\n                        <span class="file_name" ms-title="{{el.name}}">{{el.name}}</span>\n                        <span class="file_rename">\n\t\t\t\t\t\t\t\t\t\t\t<input type="text" ms-value="el.name" ms-keydown="renameFile(this.value, $event)"\n\t\t\t\t\t\t\t\t\t\t\t\tms-click="returnMouse($event)" />\n\t\t\t\t\t\t\t\t\t\t</span>\n                        <span class="file_createFolder">\n\t\t\t\t\t\t\t\t\t\t\t<input type="text" ms-value="el.name" ms-keyup="createFolder(this.value, $event)"\n\t\t\t\t\t\t\t\t\t\t\t\tms-click="returnMouse($event)" />\n\t\t\t\t\t\t\t\t\t\t</span>\n                    </div>\n                </div>\n\n                <!-- 空数据页面 -->\n                <div class="empty_list" ms-visible="isEmpty">\n                    <div id="empty_file" class="empty_img empty_file"></div>\n                </div>\n            </div>\n            <!-- 当前选中项信息 -->\n            <div class="selected_item_info">\n                <!--<img ms-src="{{(currentItem.isFile == \'true\' || currentItem.isFile == undefined) ? \'img/ic_small_document.png\' : \'img/ic_small_file.png\'}}" ms-visible="currentItem.name != undefined" />-->\n                <div class="selected_icon" ms-class="{{(currentItem.isFile == \'true\' || currentItem.isFile == undefined) ? \'selected_file_icon\' : \'selected_folder_icon\'}}" ms-visible="currentItem.name != undefined"></div>\n                <ul class="selected_item_des">\n                    <li>{{currentItem.name}}</li>\n                    <li>{{currentItem.time}}</li>\n                </ul>\n            </div>\n        </div>\n    </div>\n    <!-- 空数据页面 \n\t\t\t\t\t<div class="empty_list" ms-visible="isEmpty">\n\t\t\t\t\t\t<div id="empty_file" class="empty_img empty_file"></div>\n\t\t\t\t\t</div>\n\t\t\t\t\t-->\n</div>';});

