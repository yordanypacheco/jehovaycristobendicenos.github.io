		var lang = tools.getCookie("language");
		
		var local = "zh-cn";
		
		if(lang){
			return lang;
		}else{
			return tools.getLangCode();
		}
		
	};
	
	language.set = function(zh){
		var code = tools.getLangCode(zh);
		var url = window.location.href.toLowerCase();
		var host = window.location.hostname.toLowerCase();
		
		var lang = null;
		if(host.indexOf('haier') > -1){
			lang = allLanguageHaier.find(code);
		}else if(host.indexOf('ridha') > -1){
			lang = allLanguageRidha.find(code);
		}else{
			lang = allLanguage.find(code);
		}
		appLanguage['changeLang'] = lang;
		Config.Runtime.eventBus.emit('language',lang);
	};
	
	language.setLanguage = function(){
		var zh = language.show();
		
		language.set()
		
	};
	
	language.change = function(target, ck){
		target = tools.getLangCode(target);
		var lang = tools.getCookie("language");
		//var target = this.nation[zh];
		
		if(!target) return ;
		if(lang){
			if(target == lang)return;
		}else{
		
		}
		
		if(!ck){
			tools.setCookie("language", target,"s2000");
		}
		
		language.set(target)
		
	};
	
	
	
	return language ;
	
});

