				var top=parseInt($("#bigShowImgClick").get(0).style.top);
				
				if(document.getElementById("imgShadowClick").style.display != "none"){
					 document.getElementById("imgShadowClick").style.display = "none";
				 }
				
				
				var ww = width + width * scan;
				var hh = height + height * scan ;
				if(ww > 22234 || hh>22234) return
				
				var movT = top-height * scan/2;
				var movL = left - width * scan/2;
				
				if(e){
					
					var EX = e.pageX || e.clientX;
					var EY = e.pageY || e.clientY;
					movT = top - (EY - top)*scan;
					movL = left - (EX - left)*scan;
