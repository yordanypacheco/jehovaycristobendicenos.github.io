define('smsModule',["avalon", 
        "config", 
        "tools", 
        "text!../template/sms-module.html", 
        "debug", 
        "tipMsg", 
        "smsCenter",
        "contactCenter",
        "recordsCenter",
        "newSMS",
        "appLanguage",
        "text!../template/contact-center.html",
        "text!../template/records-center.html",], 
function(avalon, 
		Config, 
		tools, 
		tpl_smsModule, 
		debug, 
		tipMsg, 
		smsCenter,
		contactCenter,
		recordsCenter,
		newSMS,
		appLanguage,
		tpl_contactPart,
		tpl_recordsPart){
	
	var module = {};
	module.name = "smsModule";
	module.title = appLanguage["changeLang"]["smsModule"];
	module.fileType = "sms";
	module.lastTab = "sms";
	
	module.barButtons = smsCenter.barButtons;

	module.init = function(container){
		container.innerHTML = tpl_smsModule;
		
		smsCenter.init(document.getElementById("smsPart"));
		contactCenter.init(document.getElementById("contactPart"));
		recordsCenter.init(document.getElementById("recordsPart"));
		
	};

	module.load = function(type){
		var sms = smsModuleController;
		
		if(type){
			sms.currentType = type;
		}
		
		switch(sms.currentType){
			case 'sms':
				smsCenter.load();
				break;
			case 'contact':
			contactCenter.load();
			//module.barButtons = contactCenter.barButtons;
			//Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: 'contact'});
			self.currentType = 'contact';
			Config.Runtime.subModule = 'contact';
			// module.setListVisible(self.currentType);
			module.barButtons = contactCenter.barButtons;
			Config.Runtime.eventBus.emit('loadMessage',{data: 'contact'});
				break;
			case 'records':
				recordsCenter.load();
				break;
			default:
				break;
		}
	};
	
	module.unload = function(){
		var sms = smsModuleController;
		
		if(sms.currentType === 'sms'){
			smsCenter.unload();
		}
		
		if(sms.currentType === 'contact'){
			contactCenter.unload();
		}
		
		if(sms.currentType === 'records'){
			recordsCenter.unload();
		}
	};
	
	module.refresh = function(){
		var sms = smsModuleController;
	};
	
	module.onResize = function(){
		smsCenter.onResize();
		contactCenter.onResize();
		recordsCenter.onResize();
	};
	
	module.clickBarButtons = function(idx, code, obj){
		var sms = smsModuleController;
		
		if(sms.currentType === 'sms'){
			switch(code){
				case 'sendSMS':
//					module.barButtons[idx].code = 'sendSMS_pressed';
//					Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: 'sms'});
					
					newSMS.openSMSWindow();
					if(newSMS.allContactList.length === 0){
						newSMS.sendReqAll();
					}
					break;
				case 'delete':
					var t = new tipMsg({"title": appLanguage["changeLang"]["deleteSMS"], "content": appLanguage["changeLang"]["deleteSMSDes"], "fn": smsCenter.deleteSMS, "tipTheme": "red_the"});
	
//					module.barButtons[idx].code = 'delete_pressed';
//					Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: 'sms'});
					
					break;
				case 'refresh':
					smsCenter.refresh();
					sms.iSelectAll = false;
//					module.barButtons[idx].code = 'refresh_pressed';
//					Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: 'sms'});
					
					tools.rotateObj(2, 1, obj);
					
					break;
				default:
					break;
			}
			return;
		}
		
		if(sms.currentType === 'contact'){
			switch(code){
				case 'contacts_add':
					Config.Runtime.eventBus.emit('newContact', {});
					break;
				case 'delete':
					Config.Runtime.eventBus.emit('deleteContact', {});
					break;
				case 'refresh':
					Config.Runtime.eventBus.emit('refreshContact', {});
//					module.barButtons[idx].code = 'refresh_pressed';
//					Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: 'contact'});
					
					tools.rotateObj(2, 1, obj);
					
					break;
				default:
					break;
			}
			return;
		}
		
		if(sms.currentType === 'records'){
			switch(code){
				case 'delete':
					var t = new tipMsg({"title": appLanguage["changeLang"]["deleteRecords"], "content": appLanguage["changeLang"]["deleteRecordsDes"], "fn": recordsCenter.deleteRecords, "tipTheme": "red_the"});
					break;
				case 'refresh':
					recordsCenter.refresh();
//					module.barButtons[idx].code = 'refresh_pressed';
//					Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: 'records'});
					
					tools.rotateObj(2, 1, obj);
					
					break;
				default:
					break;
			}
			return;
		}
		
	};
	/*
	module.upAppBarLeft = function(idx, code, obj){
		if(code.indexOf("pressed") != -1){
			module.barButtons[idx].code = code.replace("_pressed", "");
			Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: Config.Runtime.fileType});
		}
	};
	
	module.outAppBarLeft = function(idx, code, obj){
		if(code.indexOf("pressed") != -1){
			module.barButtons[idx].code = code.replace("_pressed", "");
			Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: Config.Runtime.fileType});
		}
	};
	*/
	module.setListVisible = function(type){
		switch(type){
			case 'sms':
				smsCenter.isVisible = true;
				contactCenter.isVisible = false;
				recordsCenter.isVisible = false;
				break;
			case 'contact':
				smsCenter.isVisible = false;
				contactCenter.isVisible = true;
				recordsCenter.isVisible = false;
				break;
			case 'records':
				smsCenter.isVisible = false;
				contactCenter.isVisible = false;
				recordsCenter.isVisible = true;
				break;
			default:
				break;
		}
	};
	
	module.clearLastModule = function(type){
		var sms = smsModuleController;
		if(type == sms.currentType){
			return;
		}
		switch(type){
			case 'sms':
				smsCenter.unload();
				break;
			case 'contact':
				contactCenter.unload();
				break;
			case 'records':
				recordsCenter.unload();
				break;
			default:
				break;
		}
	};
	
	var smsModuleController = avalon.define("smsModule", function(vm){
		vm.name = module.name;
		vm.listTab = [{"type": "sms", "name": appLanguage["changeLang"]["listTypeSMS"]},{"type": "contact", "name": appLanguage["changeLang"]["listTypeContact"]},{"type": "records", "name": appLanguage["changeLang"]["listTypeRecords"]}];
		vm.currentType = vm.listTab[0].type;
		vm.listType = 'all'; //'missed'
		
		vm.isEmpty = false;
		
//		 
		vm.hasNewSMS = false;
		
		// 
		vm.seletAll = appLanguage["changeLang"]["selectAll"];
		
		// 
		vm.isVisible = false;
		
		vm.iSelectAll = false;
		vm.selectAll = function(){
			var self = smsModuleController;
			
			if(self.iSelectAll){
				self.iSelectAll = false;
				Config.Runtime.eventBus.emit('selectAll',{"type": self.currentType, "code": "0"});
			}else{
				self.iSelectAll = true;
				Config.Runtime.eventBus.emit('selectAll',{"type": self.currentType, "code": "1"});
			}
		};
		
		vm.onSelectAll = function(data){
			var self = smsModuleController;
			
			if(data.type === 'smsModule'){
				if(data.code == 0){
					self.iSelectAll = false;
				}else if(data.code == 1){
					self.iSelectAll = true;
				}
			}
		};
		
		vm.clickListTab = function(type, e){
			var e = e || window.event;
			if(e != undefined)
				e.stopPropagation();
			var self = smsModuleController;
			switch(type){
				case 'sms':
					module.lastTab = self.currentType;
					self.currentType = 'sms';
					Config.Runtime.subModule = 'sms';
					module.setListVisible(self.currentType);
					module.barButtons = smsCenter.barButtons;
					Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: 'sms'});
					// 
					Config.Runtime.eventBus.emit('loadMessage',{data: 'sms'});
					
					module.clearLastModule(module.lastTab);
					break;
				case 'contact':
					module.lastTab = self.currentType;
					self.currentType = 'contact';
					Config.Runtime.subModule = 'contact';
					module.setListVisible(self.currentType);
					module.barButtons = contactCenter.barButtons;
					Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: 'contact'});
					// 
					Config.Runtime.eventBus.emit('loadMessage',{data: 'contact'});
					
					module.clearLastModule(module.lastTab);
					break;
				case 'records':
					module.lastTab = self.currentType;
					self.currentType = 'records';
					Config.Runtime.subModule = 'records';
					module.setListVisible(self.currentType);
					module.barButtons = recordsCenter.barButtons;
					Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: 'records'});
					// 
					Config.Runtime.eventBus.emit('loadMessage',{data: 'records'});
					
					module.clearLastModule(module.lastTab);
					break;
				default:
					break;
			}
		};
		
		vm.clickCallTab = function(n, e){
			var self = smsModuleController;
			var e = e || window.event;
			e.stopPropagation();
			
			if(self.listType == n){
				return;
			}
			
			self.listType = n;
			Config.Runtime.eventBus.emit('callListType',{data: n});
		};
		
		vm.onCountInfo = function(data){
			var self = smsModuleController;
			self.totalCount = data.sms;
		};
		
		vm.onAddContactFromRecords = function(data){
			var self = smsModuleController;
			self.clickListTab('contact');
			Config.Runtime.eventBus.emit('newContact', data);
		};
		
		vm.onLanguage = function(data){
			var self = smsModuleController;
			appLanguage["changeLang"] = data;
			self.seletAll = appLanguage["changeLang"]["selectAll"]; 
			module.title = appLanguage["changeLang"]["smsModule"];
			self.listTab[0].name = appLanguage["changeLang"]["listTypeSMS"];
			self.listTab[1].name = appLanguage["changeLang"]["listTypeContact"];
			self.listTab[2].name = appLanguage["changeLang"]["listTypeRecords"];
		};
		
		vm.onButtonStatus = function(data){
			var self = smsModuleController;
			if(data == undefined){
				if(smsCenter.isVisible){
					module.barButtons = smsCenter.barButtons;
					return;
				}
				
				if(contactCenter.isVisible){
					module.barButtons = contactCenter.barButtons;
					return;
				}
				
				if(recordsCenter.isVisible){
					module.barButtons = recordsCenter.barButtons;
					return;
				}		
			}else{
				if(smsCenter.isVisible){
					module.barButtons = data.data;
					return;
				}
				
				if(contactCenter.isVisible){
					module.barButtons = data.data;
					return;
				}
				
				if(recordsCenter.isVisible){
					module.barButtons = data.data;
					return;
				}				
			}
			
		};
		
		vm.onSmsIsEmpty = function(data){
			var self = smsModuleController;
			self.isEmpty = data;
		}
		
		vm.initListeners = function(bus){
			var self = smsModuleController;
			bus.on("countInfo", self.onCountInfo);
			bus.on("addContactFromRecords", self.onAddContactFromRecords);
			bus.on("language", self.onLanguage);
			bus.on("buttonStatus", self.onButtonStatus);
			bus.on("smsIsEmpty", self.onSmsIsEmpty);
			bus.on("selectAll", self.onSelectAll);
		};
	});
	
	return module;
});

