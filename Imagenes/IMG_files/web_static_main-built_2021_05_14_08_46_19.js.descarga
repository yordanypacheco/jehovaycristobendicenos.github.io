define(
	'cTree',[ 
    'tools', 
    'config',
    'appLanguage'
    ], 
function(
	tools, 
	Config,
	appLanguage
    ){
	
	var CategoryTree = {
		title: appLanguage["changeLang"]["fileCate"],
		titlePath: "/" + appLanguage["changeLang"]["fileCate"],
		rootCategory: [],
		categoryList: {},
		categoryHistory: [],
		op: ""
	};
	
	CategoryTree.getRoot = function(){
		return [{"path":"/" + appLanguage["changeLang"]["fileCate"] + "/" + appLanguage["changeLang"]["lastFile"], "name": appLanguage["changeLang"]["lastFile"], "type": "getLastedFileList"},
		        {"path":"/" + appLanguage["changeLang"]["fileCate"] + "/" + appLanguage["changeLang"]["bigFile"], "name": appLanguage["changeLang"]["bigFile"], "type": "getBigFileList"},
		        {"path": "/" + appLanguage["changeLang"]["fileCate"] + "/" + appLanguage["changeLang"]["zipFile"], "name": appLanguage["changeLang"]["zipFile"], "type": "getZipFileList"},
		        {"path": "/" + appLanguage["changeLang"]["fileCate"] + "/" + appLanguage["changeLang"]["installPackage"], "name": appLanguage["changeLang"]["installPackage"], "type": "getApkFileList"}];
	};
	
	CategoryTree.getLastPath = function(){
		return "/" + appLanguage["changeLang"]["fileCate"] + "/" + appLanguage["changeLang"]["lastFile"];
	};
	
	CategoryTree.init = function(){
		CategoryTree.title = appLanguage["changeLang"]["fileCate"];
		CategoryTree.titlePath = "/" + appLanguage["changeLang"]["fileCate"];
		CategoryTree.rootCategory = CategoryTree.getRoot();
		CategoryTree.categoryList[CategoryTree.titlePath] = true;
		
		for(var i = 0; i < CategoryTree.rootCategory.length; i++){
			CategoryTree.categoryList[this.rootCategory[i].path] = [];
		}
	};
	
	CategoryTree.openCloseLeaf = function(path, code, fileModule){
		this.op = code;
		/**
		 * showList
		 * refresh
		 */
		// open or close, 
		if(this.categoryList[path] != undefined && code === "showTree"){
			CategoryTree.setArrowStatus(path);
			CategoryTree.setExistPath(path, fileModule);
			return;
		}
		// refresh, loading
		if(path != undefined && code === "refresh"){
			var arrow = document.getElementById(path);
			var $arrow = $(arrow).children("div").get(0); // "#" + CategoryTree.getPath(path) + " div:first"
			if($($arrow).hasClass("category_close")){
				CategoryTree.setArrowStatus(path);
				CategoryTree.setExistPath(path, fileModule);
			}else{
				CategoryTree.loadingLeaf(path);
				
				fileModule.rootCategory = [];
				
				setTimeout(function(){
					CategoryTree.setArrowStatus(path);
					CategoryTree.setExistPath(path, fileModule);
					fileModule.rootCategory = CategoryTree.rootCategory;
				}, 100);
			}
			return;
		}
		
		// show list
		if(path != undefined && code === "showList"){
			tools.logs("show list path1 = " + path, " currentPath = " + fileModule.currentPath);
			fileModule.currentPathFiles = [];
			
			return;
		}
	};
	
	CategoryTree.sendReq = function(type){
		var m = {};
		m.type = type;
		m.data = "";
		tools.sendMsgToChannel(m);
	};
	
	CategoryTree.setExistPath = function(path, fileModule){
		if(this.categoryList[path] && path != CategoryTree.titlePath){
			fileModule.currentPathFiles = this.categoryList[path]; 
		}else{
			fileModule.currentPathFiles = [];
		}
	};
	
	CategoryTree.loadingLeaf = function(path){
		var arrow = document.getElementById(path);// "#" + CategoryTree.getPath(path) + " div:first";
		var $arrow = $(arrow).children("div").get(0);
		$($arrow).removeClass("tree_loading category_close category_null").addClass("tree_loading");
	};
	
	CategoryTree.setArrowStatus = function(path){
		var li = document.getElementById(path); // "#" + CategoryTree.getPath(path);
		var arrow = document.getElementById(path); // "#" + CategoryTree.getPath(path) + " div:first";
		var $arrow = $(arrow).children("div").get(0);
		
		if($($arrow).hasClass("category_null")){
			return;
		}
		
		if($($arrow).hasClass("category_close") || $($arrow).hasClass("tree_loading")){
			$($arrow).removeClass("tree_loading category_close category_null").addClass("category_open");
			$(li).find("ul").show();
		}else{
			$($arrow).removeClass("tree_loading category_close category_null").addClass("category_close");
			$(li).find("ul").hide();
		}
	};
	
	CategoryTree.nullLeaf = function(path){
		var arrow = document.getElementById(path);// "#" + CategoryTree.getPath(path) + " div:first";
		var $arrow = $(arrow).children("div").get(0);
		$($arrow).removeClass("tree_loading category_close category_null").addClass("category_null");
	};
	
	CategoryTree.getPath = function(path){
		return path.replace(/\//g, "\\/");
	};
	
	CategoryTree.deleteListItem = function(path, folder){
		var len = path.length;
		for(i in CategoryTree.rootCategory){

			if(folder){
				for(var j = 0; j < CategoryTree.categoryList[folder]; j ++){
					if(path === CategoryTree.categoryList[folder][j]){
						CategoryTree.categoryList[folder].splice(j, 1);
					}
				}
			}else{
				for(var j = 0; j < CategoryTree.categoryList[i]; j ++){
					if(path === CategoryTree.categoryList[i][j]){
						CategoryTree.categoryList[i].splice(j, 1);
					}
				}
			}
			
		}
		
	};
	
	CategoryTree.init();
	
	return CategoryTree;
});
