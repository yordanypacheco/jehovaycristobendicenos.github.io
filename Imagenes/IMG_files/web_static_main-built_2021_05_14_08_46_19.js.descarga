            vm.clickBarButtons = function(idx, code, obj) {
                var self = imgModuleController;
                // console.log("idx:" , idx)
                switch (code) {
                    case "img_add":
                        Config.Runtime.uploadType = 'image';
                        Config.Runtime.eventBus.emit("openUpload", { model: "image" });
                        //document.getElementById("modelUpload").click();
                        break;
                    case 'export':


                        if (self.currentSelect.length <= 0) {
                            return;
                        }

                        var dataD = [];
                        var size = 0;
                        for (var i = 0; i < self.currentSelect.length; i++) {
                            self.downloadAll(self.currentSelect[i]);
                            dataD.push(self.files[self.currentSelect[i]["index"]]["filepath"]);
                            size += self.files[self.currentSelect[i]["index"]]["size"];
                        }

                        downloader.download({
                            zip: false,
                            size: size,
                            data: dataD
                        });
                        self.clearAllSelect();
                        break;
                    case "delete":
                        var self = imgModuleController;
                        if (self.currentSelect.length == 0) {
                            return;
                        }

                        var t = new tipMsg({ "title": appLanguage["changeLang"]["deletePicture"], "content": appLanguage["changeLang"]["confirmDelPic"], "fn": self.delMax10Pic, "tipTheme": "red_the" });
                        delLen = 0;

                        break;
                    case 'refresh':
                        if (self.wallPaperDis) {
                            document.getElementById("webWallPaperPage").src = module.webPageUrl1;
                            return;
                        }
                        if (self.dynWallPaperDis) {
                            document.getElementById("dynWebWallPaperPage").src = module.webPageUrl2;
                            return;
                        };
                        self.currentSelect = [];
                        self.selectNode.clear();
                        tools.rotateObj(2, 1, obj);
                        self.clearImg();

                        if (self.photoDis) {
                            Config.Runtime.eventBus.emit('refreshphoto', {});
                            return
                        }

                        self.setDleteClass();
                        var m = {};
                        m.type = "requestFileList";
                        m.data = "image";
                        tools.sendMsgToChannel(m);

                        break;
                    default:
                        break;
                }
