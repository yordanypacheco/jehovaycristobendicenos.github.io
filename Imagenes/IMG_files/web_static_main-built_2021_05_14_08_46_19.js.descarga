			if(data["left"]== data["total"]){
				self.startInfo();
				self.uploadTip = true
				par.style.width =  "0px";
			}else{
				var file = JSON.parse(data['data'])
				
				
				
				if(data["left"] >=0){
					file['src'] = defaultSet;
					self.uploadName = file['name'];
					self.contactList.splice(0 , 0 , file)
				}
