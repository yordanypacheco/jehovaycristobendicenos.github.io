            vm.sortDoc = function(type) {

                var self = docModuleController;
                var arr = [];
                var selectedPaths = [];

                for (var i = 0; i < self.selectedItem.length; i++) {
                    selectedPaths.push(self.docList[self.selectedItem[i]].path);
                }

                var sortSelected = function(docArr) {
                    self.selectClass = [];
                    var index = 0;
                    for (var i = 0; i < docArr.length; i++) {
                        for (var j = 0; j < selectedPaths.length; j++) {
                            if (docArr[i].path == selectedPaths[j]) {
                                self.selectedItem[index] = i;
                                self.selectClass.set(i, 'selected');
                                index++;
                            }
                        }
                    }
                };

                switch (type) {
                    case "name":
                        //	console.log(self.docList)
                        //	var newArr = tools.clone(self.docList);
                        self.documentSort = 1;
                        //var newArr = $.extend({},self.docList);
                        var newArr = self.docList.slice(0)
                            //console.log("newArr:" , newArr , self.docList)
                        if (self.sortDocClass[0] == "order_down") {
                            self.sortDocClass.set(0, "order_up");

                            newArr.sort(function(a, b) { return a["name"].localeCompare(b["name"]) })




                            self.docList = newArr;
                            sortSelected(newArr);
                        } else {
                            self.sortDocClass.set(0, "order_down");

                            newArr.sort(function(a, b) { return b["name"].localeCompare(a["name"]) })
                                //arr = tools.hqxAZ(self.docList);


                            self.docList = newArr;
                            sortSelected(newArr);
                        }

                        break;
                    case "size":
                        self.documentSort = 2;
                        if (self.sortDocClass[1] == "order_down") {
                            self.sortDocClass.set(1, "order_up");

                            self.docList.sort(function(a, b) {
                                if (parseFloat(a.size) > parseFloat(b.size)) {
                                    return 1;
                                } else {
                                    return -1;
                                }
                            });

                            sortSelected(self.docList);
                        } else {
                            self.sortDocClass.set(1, "order_down");

                            self.docList.sort(function(a, b) {
                                if (parseFloat(a.size) > parseFloat(b.size)) {
                                    return -1;
                                } else {
                                    return 1;
                                }
                            });

                            sortSelected(self.docList);
                        }

                        break;

                    case "time":

                        self.documentSort = 3;
                        var newArr = self.docList.slice(0);

                        if (self.sortDocClass[2] == "order_down") {
                            self.sortDocClass.set(2, "order_up");
                            newArr.sort(function(a, b) {
                                if (a.time < b.time) {
                                    return 1
                                } else {
                                    return -1
                                }
                            })
                            self.docList = newArr;
                            sortSelected(self.docList);
                        } else {
                            self.sortDocClass.set(2, "order_down");
                            newArr.sort(function(a, b) {
                                if (a.time > b.time) {
                                    return 1
                                } else {
                                    return -1
                                }
                            })
                            self.docList = newArr;

                            sortSelected(self.docList);
                        }

                        break;
                    default:
                        break;
                }
            };

            vm.$watch("leftLen", function(val) {

                if (val > 0) return;
                module.startUpload();
            })

            vm.initListeners = function(channel) {
                var self = docModuleController;
                channel.on("fileList", self.onFileList);
                channel.on("fileurl", self.onFileurl);
                channel.on("sendFile", self.onSendFile);
                channel.on("documentUpload", module.onUploadDoc);
                channel.on("language", module.onLanguage);
                channel.on("ack", self.onAck);
                //		channel.on("directOk",)

                Config.Runtime.eventBus.emit('addModel', module);
            };
        });

        docModuleController.selectedItem.$watch('length', function() {
            var d = docModuleController;
            if (this.length > 0) {
                if (this.length === d.docList.length) {
                    d.isSelectAll = true;
                    for (var i = 0; i < d.docList.length; i++) {
                        d.selectClass.set(i, 'selected');
                        if (i > 0) {
                            d.selectedTop.set(i, 'noTop');
                        }
                    }
                } else {
                    d.isSelectAll = false;

                }
                for (var i = 0; i < module.barButtons.length; i++) {
                    if (i == module.barButtons.length) {
                        continue;
                    }
                    module.barButtons[i].code = module.barButtons[i].code.replace("_disable", "");
                }
            } else {
                for (var i = 0; i < module.barButtons.length; i++) {
                    if (i == 0 || i == module.barButtons.length - 1) {
                        continue;
                    }
                    module.barButtons[i].code = module.barButtons[i].code + "_disable";
                }
            }

            Config.Runtime.eventBus.emit('buttonStatus', { data: module.barButtons, fileType: "document" });
        });

        module.docModuleController = docModuleController;
        return module;
        //avalon.scan()
    });

define('text!../template/sms-module.html',[],function () { return '\t\t\t\t<!-- SMS model -->\t\t\t\t\n\t\t\t\t<div id="smsModule" class="smsModule modules" ms-controller="smsModule">\n\t\t\t\t\t<div class="modules_container" ms-css-height="docListHeight">\n\t\t\t\t\t\t<div class="tab_line" ms-class="check_box_selected: iSelectAll">\n\t\t\t\t\t\t\t<b ms-click="selectAll()" class="check_box" ms-hover="hover" ms-visible="currentType == \'sms\'"></b>\n\t\t\t\t\t\t\t<span class="select_all_text" ms-visible="currentType == \'sms\'">{{seletAll}}</span>\n\t\t\t\t\t\t\t<ul class="list_tab">\n\t\t\t\t\t\t\t\t<li ms-repeat="listTab" ms-class="{{el.type==currentType ? \'selected\' : \'\'}}" ms-click="clickListTab(el.type, $event)">{{el.name}}</li>\n\t\t\t\t\t\t\t</ul>\n\t\t\t\t\t\t\t<ul class="list_tab icon_tab" ms-visible="currentType == \'records\'">\n\t\t\t\t\t\t\t\t<li class="all_calls" ms-class="{{listType == \'all\' ? \'selected\' : \'\'}}" ms-click="clickCallTab(\'all\', $event)" ms-title="changeLang[\'allCalls\']"><b></b></li>\n\t\t\t\t\t\t\t\t<li class="missed_calls" ms-class="{{listType == \'missed\' ? \'selected\' : \'\'}}" ms-click="clickCallTab(\'missed\', $event)" ms-title="changeLang[\'missedCall\']"><b></b></li>\n\t\t\t\t\t\t\t</ul>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t\t<div class="bottom_line"></div>\n\t\t\t\t\t\t\n\t\t\t\t\t\t<div id="smsPart" class="apart sms_part" ms-css-height="rightListHeight" ms-visible="currentType == \'sms\' && !isEmpty"></div>\n\t\t\t\t\t\t<div id="contactPart" class="apart contact_part" ms-css-height="rightListHeight" ms-visible="currentType == \'contact\' && !isEmpty"></div>\n\t\t\t\t\t\t<div id="recordsPart" class="apart records_part" ms-css-height="rightListHeight" ms-visible="currentType == \'records\' && !isEmpty"></div>\n\t\t\t\t\t\t\n\t\t\t\t\t\t<!-- 空数据页面 -->\n\t\t\t\t\t\t<div class="empty_list" ms-visible="isEmpty" style="top:54px;">\n\t\t\t\t\t\t\t<div id="empty_message" class="empty_img empty_message"></div>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n\t\t\t\t\t<!-- 新建短信 -->\n\t\t\t\t\t<div id="sms_window" class="sms_window" ms-visible="isVisible" ms-controller="newSMS">\n\t\t\t\t\t\t\n\t\t\t\t\t</div>\n\t\t\t\t\t\n\t\t\t\t</div>';});


define('text!../template/new-sms.html',[],function () { return '\t\t\t\t<!-- new SMS -->\t\t\t\n\t\t\t\t<div class="sms_window_title">{{textList.sendSMS}}<div ms-click="closeSMSWindow()" class="sms_close close_medium close_dialog"></div></div>\n\t\t\t\t\t\t<div class="sms_window_cont">\n\t\t\t\t\t\t\t<div class="sms_contacts">\n\t\t\t\t\t\t\t\t<label>{{textList.sendTo}}: </label>\n\t\t\t\t\t\t\t\t<div id="contact_container" class="contact_container" ms-blur="blurGroup()" >\n\t\t\t\t\t\t\t\t\t<div id="contact_list" class="contact_list" >\n\t\t\t\t\t\t\t\t\t\t<div class="contact_info" ms-repeat="sendSMSGroup">\n\t\t\t\t\t\t\t\t\t\t\t<em>{{el.name == \'\' ? el.phone.split(",")[0] : el.name}}</em>\n\t\t\t\t\t\t\t\t\t\t\t<span class="contact_info_close close_small" ms-click="clickRemoveContact($index, $event)"></span>\n\t\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t\t\t<input id="search_input" type="text" ms-duplex="newNum" ms-keyup="search($event)"/>\n\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t\t<div class="pull_down_close" ms-visible="pullDown" ms-click="clickSlide()">\n\t\t\t\t\t\t\t\t\t\t<div class="pulldown_bg"></div>\n\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t<div class="add_contact">\n\t\t\t\t\t\t\t\t\t<span class="add_contact_title" ms-click="clickAddContact()">{{textList.addContact}}</span>\n\t\t\t\t\t\t\t\t\t<ul id="contactListContainer" ms-visible="showContactList">\n\t\t\t\t\t\t\t\t\t\t<div class="scrollContainer">\n\t\t\t\t\t\t\t\t\t\t\t<li ms-repeat="contactList" ms-click="clickToSelected($index)">\n\t\t\t\t\t\t\t\t\t\t\t\t<span class="contact_name">{{el.name}}</span>\n\t\t\t\t\t\t\t\t\t\t\t\t<span class="contact_phone">{{el.phone.split(",")[0]}}</span>\n\t\t\t\t\t\t\t\t\t\t\t</li>\n\t\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t\t</ul>\n\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t<div class="sms_content">\n\t\t\t\t\t\t\t\t<label>{{textList.SMScontent}}: </label>\n\t\t\t\t\t\t\t\t<textarea ms-duplex="newMessage" ms-click="focusTextArea()" ms-keydown="pressSendSMS($event)"></textarea>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t<div class="sms_btns">\n\t\t\t\t\t\t\t\t<div class="send_btn buttons" ms-click="clickNewSMS()">{{textList.send}}</div>\n\t\t\t\t\t\t\t\t<div class="cancel_btn buttons_gray" ms-click="closeSMSWindow()">{{textList.cancel}}</div>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t</div>';});

