	};
	
	// youtube
	module.youtubeCode = function(){
		
	};
	module.closeYoutube = function(){
		var h = helpModuleController;
		
		h.visible = false;
		$.unblockUI();
	};
	
	module.setLink = function(src){
		helpModuleController.videoLink = src;
	};
	
	var helpModuleController = avalon.define("helpModule", function(vm){
		vm.visible = false;
		
		vm.videoLink = "";
		vm.jpOrCn = "gp";
		vm.pageWidth = 0;
		vm.pageHeight = 0;
		
		vm.closeHelp = function(e){
			var e = e || window.event;
			var es = e.target || e.srcElement;
			
			$(es).removeClass("pressed").addClass("pressed");
			
			$("#appBar li").removeClass("pressed");
			
			Config.Runtime.eventBus.emit('visibleHelp',{"visible":false});
			module.setLink("");
		};
		
		vm.outClose = function(e){
			var e = e || window.event;
			var es = e.target || e.srcElement;
			
			$(es).removeClass("pressed");
		};
		
		vm.onVisibleHelp = function(data){
			var self = helpModuleController;
			if(data.visible){
