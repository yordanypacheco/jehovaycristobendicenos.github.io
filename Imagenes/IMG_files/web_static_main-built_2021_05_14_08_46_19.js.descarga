            judgeDate: function(a, b) {
                var newA = a.split(" ");
                var newB = b.split(" ");

                var newYearA = newA[0].split("/");
                var newYearB = newB[0].split("/");

                var A = newYearA[0] * 12 * 365 + newYearA[1] * 12 + newYearA[2];
                var B = newYearB[0] * 12 * 365 + newYearB[1] * 12 + newYearB[2];

                if (A > B) {
                    return true;

                } else if (A == B) {
                    var newMa = newA[1].split(":");
                    var newMb = newB[1].split(":");

                    var timeA = newMa[0] * 60 + newMa[1];
                    var timeB = newMb[0] * 60 + newMb[1];

                    if (timeA > timeB) {
                        return true;
                    } else {
                        return false;
                    }
                } else {
                    return false;
                }
            },

