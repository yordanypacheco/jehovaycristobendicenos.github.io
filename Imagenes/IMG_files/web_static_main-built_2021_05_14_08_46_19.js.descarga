		tc.style.width = this.tWidth + "px";
		tc.style.height = this.tHeight + "px";
		tc.onselectstart = function(){return false};
		
		var dms = dm.single.getInstance();
		var contentClass = dms.getClassName().content;
		
		var html = //'<div id="tipMsg" class="tip_msg" style="width:' + this.tWidth + 'px;height:' + this.tHeight + 'px;' + '">'
					'<div class="tip_title">'
						+ '<span>' + this.title + '</span>'
						+ '<div id="tip_close" class="close_medium tip_close"></div>'
					+ '</div>'
					+ '<div class="tip_content">'
						+ '<div class="' + contentClass +'" id="tipMsgH">' + this.content + '</div>'
					+ '</div>'
					+ '<div class="tip_btns">'
						+ '<div id="tComfirm" class="buttons">' + appLanguage['changeLang']['submit'] + '</div>'
						+ '<div id="tCancel" class="buttons_gray">' + appLanguage['changeLang']['cancel'] + '</div>'
					+ '</div>';
				  //+ '</div>';
		tc.innerHTML = html;
		
		
		
		document.body.appendChild(block);
		document.body.appendChild(tc);
		//console.log(document.getElementById("tipMsgH").style.height , document.getElementById("tipMsgH").clientHeight)
		//document.getElementById("tipMsgH").style.marginTop = (120 + parseInt(document.getElementById("tipMsgH").style.height))/2 + "px";
		//document.getElementById("tipMsgH").style.marginTop = "30px";
		var bindEvt = function(){
			block.onclick = function(e){
				var evt = e || window.event;
				if(evt){
					evt.stopPropagation();
				}
			}
			
			$("#" + tc.id).die().live('click', function(e){
				var evt = e || window.event;
				if(evt){
					evt.stopPropagation();
				}
			});
			
			$("#tip_close").die().live("click", function(e){
				self.close();
				return;
			});
			
			$("#tComfirm").die().live("click", function(e){
				self.comfirm(self.fnParam);
				self.cancel();
			});
			
			$("#tCancel").die().live("click", function(e){
				self.cancel();
			});
		};
		bindEvt();
		
		var $btns = $("#tipMsg .tip_btns");
		var $buttons = $("#tipMsg .buttons");
		switch(this.buttonCounts){
			case 0:
				$btns.hide();
				break;
			case 1:
				$("#tCancel").hide();
				break;
			case 2:
				break;
			default:
				return;
		}
	};
	
	TipMsg.prototype.tip = function(){
		tools.allMiddlePosition("#tipMsg");
		$("#tipMsg").fadeIn();
	};
	
	TipMsg.prototype.comfirm = function(){
		this.close();
	};
	
	TipMsg.prototype.close = function(){
		var tc = document.getElementById("tipMsg")
		if(tc){
			tc.parentNode.removeChild(tc);
		}
		
		var block = document.getElementById("blockTip");
		if(block){
			block.parentNode.removeChild(block);
		}
	};
	
	TipMsg.prototype.cancel = function(){
		this.close();
		return;
	};
	
	return TipMsg;
});
define('downloader',["tools", "tipMsg", "appLanguage", "config"],

    function(tools, tipMsg, appLanguage, Config) {

        var downloader = {};
        downloader.timeout = null
        downloader.downPath = {};


        downloader.onFileUrl = function(data) {
            var flag = downloader.downPath[data.path];
            clearTimeout(downloader.timeout)
            if (flag) {
                delete downloader.downPath[data.path];

                if (data.url == 10000) {
                    tools.tipMsg(appLanguage['changeLang']["downError"]);
                } else if (data.url == 10001) {
                    tools.tipMsg(appLanguage['changeLang']["downMany"]);
                } else {
                    Config.Runtime.eventBus.emit('downloadfileTitle', {
                        path: data.path,
                        type: 'del'
                    });
                    downloader.downloadFile(data.url);
                }
            }
        };


        downloader.downloadFile = function(url) {

            var explorer = window.navigator.userAgent;
            if (url.indexOf("version=1") > 0) {
                url = url + "&download=1"
            } else {
                url = url + "?download=1"
            }
            console.log("----------down url-----------:", url)
            if (explorer.indexOf("Chrome") >= 0 && false) {

                var adownLoda = document.createElement("a");
                adownLoda.href = url;
                adownLoda.filename = new Date();
                adownLoda.download = url;
                document.body.appendChild(adownLoda);
                adownLoda.click();
                // $(adownLoda).remove();

            } else {

                var iframe = document.createElement('iframe');
                iframe.src = url;
                document.body.appendChild(iframe);
                iframe.onload = function() {
                    var that = this;
                    setTimeout(function() {
                        $(that).remove();
                    }, 10000);

                }

            }

        };

        downloader.sendMsgServer = function(path) {
            var m = {};
            m.type = "requestFile";
            m.download = 1;
            m.data = [path];
            if (!Config.Runtime.isDirect == true) {
                Config.Runtime.eventBus.emit('downloadfileTitle', {
                    path: path,
                    type: 'add'
                });
                tools.googleAna("cloudDownload")
            } else {
                tools.googleAna("directDownload")
            }
            clearTimeout(downloader.timeout)
            downloader.timeout = setTimeout(function() {
                tools.tipMsg(appLanguage['changeLang']["SlowInternet"]);
            }, 5000)

            tools.sendMsgToChannel(m);
        };

        downloader.sendMsgZip = function(data, path) {
            var m = {};
            m.type = "requestZipFile";
            m.download = 1;
            m.data = {
                filelist: data,
                requestPath: path
            };
            tools.sendMsgToChannel(m);
        };

        var download = function(path) {

            var data = [];

            //

            if (!Config.Runtime.isDirect == true) {
                if (path['size'] == NaN || path['size'] == undefined) path['size'] = 1;
                path['size'] = path['size'].toString();

                if (tools.noIe() && path["data"].length > 1) {
                    new tipMsg({
                        "title": appLanguage["changeLang"]["tipTitle"],
                        "content": appLanguage["changeLang"]["notIe"],
                        "fn": ''
                    });
                    return;
                }

                if (path['size'].indexOf('MB') > -1) {
                    path['size'] = parseInt(path['size']) * 1024 * 1024;
                }

                if (path['size'] && parseInt(path['size']) > Config.limitSize) {
                    new tipMsg({
                        "title": appLanguage["changeLang"]["tipTitle"],
