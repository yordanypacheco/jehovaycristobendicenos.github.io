		*/
		vm.delUploadFile = function(){
			var self = videoModuleController;
			while(self.files.length){
				
				if(self.files[0]["upId"]){
					self.files.shift();
					self.posterUrls.srcs.shift();
				//	Album.del(self.albums , 0);
				}else{
					return;
				}
			}
		}
		
		vm.onAck = function(f){
		    var file = f.file;
			var upId = f.path;
			var self = videoModuleController;
			if(f.fileType != "video") return;
			for(var i=0;i<self.files.length ; i++){
				if(self.files[i]["upId"] && self.files[i]["upId"]==upId){
					//tools.logs("ack file:" , file.fil)
					
					if(file == 10000 || file == 10001){
						if(module.uploadObj){
							module.uploadObj.flag = false;
							self.queue.length = 0;
						}
						//console.log("file:")
						tools.tipMsg(appLanguage['changeLang']["noSpace"]);
						
						self.delUploadFile();
						self.leftLen = 0;
						return
					}
					
					uploadLen--;
					try{
						var dom = document.getElementById(upId);
						var progress = document.getElementById("progress_" + upId);
						dom.parentNode.removeChild(dom);
						progress.parentNode.removeChild(progress);
					}catch(e){
						
					}
					
					self.files.splice(i , 1 , f.file);
					self.addParam(i , f.file);
					self.posterUrls.videoNameVisible.set(i ,  true);
					return ;
				}
			}
		
		};
		
		vm.initListeners = function (bus){
			var self = videoModuleController;
			bus.on("fileList", self.onFileList);
			bus.on("fileurl", self.onFileurl);
			bus.on("sendFile", self.onSendFile);
			bus.on("videoUpload" , module.onUploadVideo);
			bus.on("language" , module.onLanguage);
			bus.on("ack" , self.onAck);
			
			Config.Runtime.eventBus.emit('addModel',module)
		//	tools.logs("colors:" , color , color.red)
		};
	});
	
	module.videoModuleController = videoModuleController;
	return module;
});

