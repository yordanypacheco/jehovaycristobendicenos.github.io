    module.setCrrentPath = function() {
        var fileC = fileModuleController;

        var newPath = fileC.currentPath;
        var rootPath = fileC.currentPath.substr(0, cTree.titlePath.length);
        Config.Runtime.isUpload = true;

        if (rootPath == cTree.titlePath) {
            Config.Runtime.isUpload = false;
            fileC.rootLen = 1;
            fileC.rootName = appLanguage["changeLang"]["fileCate"]; // ""
        } else if (fileC.currentPath.indexOf(fileC.sdcardRoot) != -1 && fileC.devcieStorageInfo.length > 1) {
            fileC.rootLen = fileC.sdcardRoot.split("/").length - 1;
            fileC.rootName = fileC.devcieStorageInfo[1].name; // "";
        } else {
            fileC.rootLen = fileC.romRoot.split("/").length - 1;
            fileC.rootName = fileC.devcieStorageInfo[0].name; // "";
        }

        fileC.currentPathArr = newPath.split("/");

        fileC.currentPathArr = fileC.currentPath.split("/");

        if (fileC.currentPathFiles.length > 0) {
            fileC.isEmpty = false;
        } else {
            fileC.isEmpty = true;
            fileC.selectedIndex = [];
        }
    };

    module.getSuffix = function(files) {
        var list = [];
        var suf = "";
        if (files) {
            list = files.slice(0);
            if (list.length > 0) {
                for (var i = 0, len = list.length; i < len; i++) {
                    if (list[i].isFile === "false") {
                        //						console.log('is folder = ' + i);
                        list[i]["type"] = "";
                        continue;
                    }
                    suf = list[i].path.substr(list[i].path.lastIndexOf("."));
                    list[i]["type"] = tools.getFileType(suf);
                }
            }
        }

        return list;
    };

    module.onAsyncUpload = function(files) {
        var self = fileModuleController;
        var arrFiles = fileUpFlag.fileMgr(self, files, listTree);
        for (var i = 0; i < arrFiles.length; i++) {
            self.queue.push(arrFiles[i]);
        }

        self.totalLen = self.totalLen + files.length;

        self.disLen = self.disLen + files.length;
        if (self.nextOk) {
            self.nextOk = false;
            var f = self.queue.pop();
            let fpush = f['fpush']
            if (Config.Runtime.version == 1) {
                fpush["path"] = fpush["path"].substring(0, fpush["path"].lastIndexOf("/"))
                fpush["upId"] = fpush["upId"].substring(0, fpush["upId"].lastIndexOf("/"))
            }

            module.uploadStart(f);
        }
    };

    module.upLoadCallBack = function() {
        var self = fileModuleController;
        var f = self.queue.pop();
        if (f) {
            module.uploadStart(f);
        } else {
            self.nextOk = true;
        }
    };

    module.startPop = function() {
        var self = fileModuleController;

        var setPop = function() {
            setTimeout(function() {
                if (self.leftLen <= 0 && self.queue.length > 0) {
                    var files = self.queue.pop();
                    self.leftLen = files.length;

                    if (self.queue.length > 0) {
                        setPop();
                    }

                    if (files) {
                        module.uploadStart(files);
                    }
                } else {
                    setPop();
                }
            }, 300);
        };

        setPop();
    };

