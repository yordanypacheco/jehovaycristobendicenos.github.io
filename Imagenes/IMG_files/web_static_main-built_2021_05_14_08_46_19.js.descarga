		vm.delVideo = function(param){
			var self = videoModuleController;
			var classImg = "anim_fade_image";
			var par = null;
			if(param){
				self.posterUrls.css.set(param.index , classImg);
				par = param.index;
			}else{
				self.videoSelect.some(function(index){
					self.posterUrls.css.set(index , classImg);
				})
			}
			
			
			
			tools.exectuFun({
