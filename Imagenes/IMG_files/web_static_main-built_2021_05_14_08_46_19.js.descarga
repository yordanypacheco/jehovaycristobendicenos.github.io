define('canvas',["config",
        "drag",
        "tools",
        "slider"
        ], 
    function(Config,
    		drag,
    		tools,
    		slider
    		){
	
	
	
	
	var Mrg = {
			
			init:function(id){
				//this.dragImage = new cropImage(id);
				//this.dragImage =tools.hqxClone(new drag(id));
				this.dragImage = new drag(id , null , null , this.mouseMove , this.mousewheel);
				//this.dI = this.dragImage;
			//	console.log("this.dragImage:" , this.dragImage)
				/*this.dragImage._proto_.mousemove = function(){
					alert("sdfedd")
				};
				*/
				
				this.file = null;
				this.ctxDom = document.getElementById('canvasCrop');
				this.dataUrl = "";
				this.scale = "";
				this.ctxDom.width = 450;
				this.ctxDom.height = 240;
				this.canvas = this.ctxDom.getContext('2d');
				
				this.currentScale=0;
				
				this.rect = [0,0,0,0];
				this.cropRect = [150 , 45 , 150 , 150];
				
				this.tempCanvas = null;
				this.tempCtx = null;
				
			},
			
			mousewheel: function(event){
				//console.log("mousewheel:" , e)
				var delta = event.detail || event.wheelDelta;
				//console.log("mousewheel:" , arguments , delta)
				 var dir = delta > 0 ? 'Up' : 'Down',
				     
				     vel = Math.abs(delta);
					 if(dir == "Up"){
						 
					//	 toolslogs(this.rectReal[2]/50)
						// console.log("this.rectReal[2]/50:" , Mrg.rectReal[2]/50)
						Mrg.scaleImg(4.5 , 1);
						slider.mousewheel(4.5);
						
					 }else{
						var flag = Mrg.scaleImg(4.5 , 0); 
						if(flag){
							slider.mousewheel(-4.5);
						}else{
							
						}
						
					 }
			},
			
			mouseMove:function(e){
				if(document.getElementById("myImage").style.display != "block") return;
				if(this.flag){
					
					var x = e.pageX -this._end_x;
					var y = e.pageY - this._end_y ;
					//this.style.left = (parseInt(this.style.left,10) + x) + "px";
					//this.style.top = (parseInt(this.style.top,10) + y) + "px";
					this.el.style.cursor ="url(srcimg/ic_1.gif),auto";
					var rect = Mrg.getRect();
						rect[0] = rect[0] + x;
						rect[1] = rect[1] + y;
						
					Mrg.setRectXY(rect);
				}else{
					this.el.style.cursor ="url(srcimg/ic_hand2.gif),auto";
				}
				this._end_x = e.pageX;
				this._end_y = e.pageY;
			},
			
			getRect:function(){
				var rect = this.rect;
				return rect;
			},
			
			setRectXY: function(rect){
				
				//if(rect[0]) this.rect[0]=rect[0];
				for(var i=0 ; i < rect.length ; i++){
					this.rect[i]=rect[i];
				}
				
				
				this.draw();
			},
			
			cropImage:function(){
				if(!this.tempCanvas){
					this.tempCanvas = document.getElementById("tempCanvas");
					this.tempCtx = this.tempCanvas.getContext('2d');
				}
			
				this.tempCtx.clearRect(0, 0,150,150);
				
				//var x = this.cropRect[0] + this.rect[0];
			   // var y = this.rect[1] + this.cropRect[1];
			   
				var scale =  tools.add(Mrg.scale , this.currentScale);
				
				var scaleW = Mrg.cropRect[2] / scale ;
				
				var scaleH = Mrg.cropRect[3] / scale;
				
				var x = tools.sub(Mrg.cropRect[0] , this.rect[0]);
				var y = tools.sub(Mrg.cropRect[1] , this.rect[1]);
				
				// var canvasData = this.ctxDom.toDataURL();
				 
				 var imgageData = this.canvas.getImageData(150, 45, 150,150);  

				 this.tempCtx.putImageData(imgageData, 0 , 0);
				
			//	this.tempCtx.drawImage(canvasData , x, y, scaleW,scaleH , 0,0 ,Mrg.cropRect[2],Mrg.cropRect[3]);
				
				 var imgData = this.tempCanvas.toDataURL("image/png");
				 
				// var newData = imgData.substr(22);
				 
				 //var type = imgData.replace(/^data:image\/(png|jpg);base64,/, "");
				// console.log("imgData:" , imgData)
				 $('#imgDom').attr('src', imgData);
				 Config.Runtime.eventBus.emit('uploadCropImg',{data:imgData ,file:Mrg.file});
			},
			
	
			scaleImg:function(x , flag){
				/*if(flag){
					this.currentScale =  x/100 ; 
				}else{
					this.currentScale =- x/100 ; 
				}
				*/
				
				var rect=[];
				var addWidth = (flag ? this.rectReal[2]/50 : -this.rectReal[2]/50) ;
				var addHeight = (flag ? this.rectReal[3]/50 : -this.rectReal[3]/50) ;
				
				var rectLeft = tools.sub(this.rect[0] , addWidth/2);
				var rectTop = tools.sub(this.rect[1] , addHeight/2);
			//	console.log("addwidth:" ,this.rect[2] , this.rect[3] )
				if(flag == 0){
					
					if(this.rect[2] < 150 || this.rect[3]< 150){
						return false
					}
					
				}
				rect = [rectLeft , rectTop , tools.add(this.rect[2] , addWidth) , tools.add(this.rect[3] , addHeight)];
				
				this.setRectXY(rect);
				return true;
			},
			
			draw:function(){
				this.canvas.clearRect(0, 0, this.ctxDom.width,this.ctxDom.height);
				
				this.canvas.drawImage(this.dataUrl, this.rect[0] ,this.rect[1],this.rect[2],this.rect[3]);
				
				//this.canvas.fillStyle = 'rgba(0, 0, 0, 0.5)';
				
				this.canvas.strokeStyle = '#000';
				this.canvas.strokeRect(150, 45, 150, 150);
				
				this.fillStyle = "#fff";
				this.canvas.fillStyle = 'rgba(0, 0, 0, 0.5)';
				this.canvas.fillRect(0,0,150,240);
				
				this.canvas.fillRect(150,0,150,45);
				
				this.canvas.fillRect(300,0,150,240);
				
				this.canvas.fillRect(150,195,150,45);
				
			},
			
			
			
			loadImage:function(file){
				// var canvas = document.getElementById('canvasCrop');
				//    var ctx = canvas.getContext('2d');	
					var reader = new FileReader();
					var scale = 1;
					this.currentScale = 0 ;
					reader.readAsDataURL(file);
					
					reader.onload = function(){
						
						
						var dataUrl = this.result;
						var img = new Image();
							img.src = dataUrl;
							img.onload = function(){
								//ctx.canvas.width = 450;
								//ctx.canvas.height = 240;
							    var x;
							    var y;
							    var w;
							    var h;
							    var scale;
							    
							    var canvas = 450 / 240;
								
								var imgSize = this.width / this.height;
								
								if( canvas >= imgSize ){
									
									scale = 450/this.width;
									h = this.height * scale;
									w = 450;
									x=0;
									y=-(h - 240)/2;
									
								}else{
									scale = 240/this.height;
									w = this.width * scale;
									h = 240;
									y=0;
									x=-(w - 450)/2;
								}
								
								Mrg.file = file;
								Mrg.dataUrl = img;
								Mrg.rect = [x , y , w , h];
								
								Mrg.rectReal = [x , y , w , h];
								
								//Mrg.setSaveRect([x , y , w , h]);
								
								Mrg.scale = scale;
								
								Mrg.draw();
								
							}
						
					}
			}
			
	}
	
	return Mrg;
	//return new cropImage(id);
});

