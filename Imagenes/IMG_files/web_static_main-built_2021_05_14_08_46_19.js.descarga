define('mouseStop',["config" , "tools"], 
    function(Config
    		 , tools){
	
	var mouseStop = {};
	mouseStop.setTime="";
	mouseStop.moveImg="";
	mouseStop.move = false;
	
	mouseStop.moveFlag=false;
	mouseStop.x=0;
	mouseStop.y=0;
	
	//mouseStop.position = 0;
	
	mouseStop.init = function(){
		document.addEventListener("mousemove" , mouseStop.mousemove);
		document.addEventListener("keydown" , mouseStop.keydown);
		//$(document).on("mouseup" , mouseStop.mouseup);
		
	}
	
	mouseStop.mouseup = function(e){
		e.stopPropagation();
		mouseStop.moveFlag = false;
		if(mouseStop.moveImg != ""){
			clearTimeout(mouseStop.moveImg);
		}
		
		
		mouseStop.moveImg = setTimeout(function(){
			mouseStop.moveFlag = true;
			//console.log("mouseStop.moveFlag:" , mouseStop.moveFlag)
			//clearInterval(mouseStop.moveImg);
		} , 20)
	};
	
	
	mouseStop.mousemove_ = function(){
		//console.log("mousemove")
	}
	
	mouseStop.keydown = function(e){
		Config.Runtime.eventBus.emit('keydown',{event:e});
	}
	
	mouseStop.mousemove= function(e){
		e.stopPropagation();
		var lowValue = 3;
		var EX = e.x || e.clientX;
		var EY = e.y || e.clientY;
		
		var dx = Math.abs(mouseStop.x - EX);
		var dy = Math.abs(mouseStop.y - EY);
		
		//console.log("mouseStop.move:" ,dx > lowValue || dy > lowValue , dx , dy , e)
		if(dx > lowValue || dy > lowValue){
			mouseStop.x = EX;
			mouseStop.y = EY;
		}else{
			return;
		}
		
		clearTimeout(mouseStop.setTime);
		if(!mouseStop.move){
			mouseStop.move = true;
			Config.Runtime.eventBus.emit('mouseStart',{});	
		}
		
		mouseStop.move = true;
			//this.position = e.pageX + e.pageY;
		mouseStop.setTime = setTimeout(mouseStop.timeOut, 1000);
		
		
	}
	
	mouseStop.timeOut = function(){
		
		Config.Runtime.eventBus.emit('mouseStop',{});
		mouseStop.move = false;
		
	}
	
	return {
		init:mouseStop.init
	}
});

define('unload',["config"], function(Config){
	var onload ={};
	
	onload.model=[];
	//onload.interval=900000;
	onload.interval=3000;
	
	onload.initModel = function(){
		
		var timestamp = Date.parse(new Date());
		var time=0;
		//console.log("this.model.length:" , this.model.length);
		for(var i=0 ; i<this.model.length; i++){
			//console.log("model:" , this.model[i]);
			//if(this.model[i].time != null ){
			  if(!(this.model[i].time == null || this.model[i].time == "timeOut")){
				time=timestamp - this.model[i].time;
				
				if(time > this.interval){
					this.model[i].timeOut();
				}
			}
		}
	};
	
	onload.polling = function(){
		//console.log("polling")
		setTimeout(function(){
			onload.initModel.call(onload);
			onload.polling.call(onload);
		} , 2000)
	}
	
	onload.onAddModell = function(model){
		onload.model.push(model);
	};
	
	onload.init = function(bus){
		
		bus.on("addModel" , onload.onAddModell);
		onload.polling();
	}
	
	return {
		init:onload.init
	}
});
