    module.opPaste = function() {
        var fileC = fileModuleController;

        module.setClipBoard({
            newPath: fileC.currentPath
        });

        if (fileC.clipBoard.fromPath == fileC.clipBoard.newPath) {
            tools.tipMsg(appLanguage["changeLang"]["pasteError"]);
            return;
        }
        //		有重复文件停止paste
        for (var f = 0; f < fileC.currentPathFiles.length; f++) {
            for (var i = fileC.clipBoard.files.length - 1; i >= 0; i--) {
                tools.logs("file i1 = " + i, "name = " + fileC.clipBoard.files[i].name);
                if (fileC.currentPathFiles[f].name === fileC.clipBoard.files[i].name) {
                    tools.tipMsg('"' + fileC.clipBoard.files[i].name + '"' + appLanguage["changeLang"]["fileExist"]);
                    module.clearClipBoard();
                    return;
                }
            }
        }
        tools.logs(
            "fileC.clipBoard.paths = " + fileC.clipBoard.paths,
            "from = " + fileC.clipBoard.fromPath,
            "to = " + fileC.clipBoard.newPath
        );

        var send = function() {
            var m = {};
            m.type = "fileCopy";
            m.data = {
                files: fileC.clipBoard.paths,
                newPath: fileC.clipBoard.newPath,
                isDelete: fileC.clipBoard.isDelete
            };
            tools.sendMsgToChannel(m);
        };
        send();
    };

