define('text!../template/music-module.html',[],function () { return '\t\t\t\t<!-- music model -->\t\t\t\t\n\t\t\t\t<div id="musicModule" class="musicModule modules" ms-controller="musicModule" onselectstart="javascript: return false;">\n\t\t\t\t\t<div class="tab_line">\n\t\t\t\t\t\t<ul class="img_list_tab list_tab" ms-class="only_one: listTab.length == 1">\n\t\t\t\t\t\t\t<li ms-repeat="listTab" ms-class="{{$index == 0 ? \'selected\' : \'\'}}" ms-click="clickListTab($index, $event)">{{el}}</li>\n\t\t\t\t\t\t</ul>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div class="bottom_line"></div>\n\t\t\t\t\t<div id="musicModule_file_list" class="file_list list_container" ms-css-height="musicListHeight" ms-visible="listType == \'local\'">\n\t\t\t\t\t\t<div class="list_head">\n\t\t\t\t\t\t\t<div class="list_center"  ms-visible="!isEmpty" ms-css-width="rightWidth-40">\n\t\t\t\t\t\t\t\t<ol ms-class="selected: isSelectAll">\n\t\t\t\t\t\t\t\t\t<li class="check_box_con"><b class="check_box" ms-hover="hover" ms-click="selectAll()"></b></li>\n\t\t\t\t\t\t\t\t\t<li class="music_name orders" ms-mousedown="clickSort(\'name\', $event)" ms-mouseup="outSort(\'name\', $event)" ms-mouseout="outSort(\'name\', $event)">\n\t\t\t\t\t\t\t\t\t\t<span id="musicName">{{changeLang[\'musicName\']}}</span>\n\t\t\t\t\t\t\t\t\t\t<span class="order_down" ms-visible="musicSort==1"></span>\n\t\t\t\t\t\t\t\t\t</li>\n\t\t\t\t\t\t\t\t\t<li class="operations"></li>\n\t\t\t\t\t\t\t\t\t<li class="singer orders" ms-mousedown="clickSort(\'author\', $event)" ms-mouseup="outSort(\'author\', $event)" ms-mouseout="outSort(\'author\', $event)">\n\t\t\t\t\t\t\t\t\t\t<span id="songer">{{changeLang[\'songer\']}}</span>\n\t\t\t\t\t\t\t\t\t\t<span class="order_down" ms-visible="musicSort==2"></span>\n\t\t\t\t\t\t\t\t\t</li>\n\t\t\t\t\t\t\t\t\t<li class="duration" id="duration">{{changeLang[\'duration\']}}</li>\n\t\t\t\t\t\t\t\t\t<li class="format" id="formatMusic">{{changeLang[\'formatMusic\']}}</li>\n\t\t\t\t\t\t\t\t\t<li class="size" id="