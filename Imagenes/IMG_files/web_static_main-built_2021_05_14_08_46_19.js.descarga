    module.onMusicUpload = function(files) {
        var self = musicModuleController;

        var newMusic = [];

        for (var i = 0; i < files.length; i++) {
            var type = files[i].name.substring(files[i].name.lastIndexOf("."));
            if (Config.music[type]) {
                newMusic.push(files[i]);
            }
        }

        if (newMusic.length == 0) {
            //alert("请选择正确的格式!");
            // if(!event["dragFile"])
            tools.tipMsg(appLanguage["changeLang"]["onlyMusic"]);
            return;
        }
        self.queue.push(newMusic);

        if (self.totalLen == 0) self.disLen = 0;

        self.totalLen = self.totalLen + newMusic.length;
        self.disLen = self.disLen + newMusic.length;
        //console.log(self.disLen , newMusic.length , self.totalLen + newMusic.length , self.totalLen)
        console.log("self.leftLen:", self.leftLen);
        if (self.leftLen <= 0) {
            module.startUpload();
        }
    };

