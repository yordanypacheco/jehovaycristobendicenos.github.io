        vm.onFileOperateStatus = function(data) {
            var self = fileModuleController;

            module.clearStatus();

            var createOp = function(status) {
                tools.logs("new operation status = " + status);
                if (status == "1") {
                    module.createItem.path = module.createItem.path + "/" + module.createItem.name;

                    self.currentPathFiles.splice(0, 1, module.createItem);
                    listTree.pathFolderList[self.currentPath].splice(0, 0, module.createItem);
                    listTree.pathFileList[self.currentPath] = self.currentPathFiles.slice(0);

                    listTree.addLeaf(self.currentPath, module.createItem);
                    self.selectedIndex.splice(0, self.selectedIndex.length);

                    $($(".file_item").get(0)).removeClass("file_status_createFolder");

                    module.clearStatus();
                } else {
                    self.currentPathFiles.splice(0, 1);
                    self.fileItemStatus = "normal";
                    tools.tipMsg(appLanguage["changeLang"]["fileHasExist"]);
                }
            };

            var deleteOp = function(status) {
                tools.logs("delete operation status = " + status);
                var rootPath = self.currentPath.substr(0, cTree.titlePath.length);

                if (status == "1") {
                    tools.tipMsg(appLanguage["changeLang"]["deleteSuc"]);

                    /** 
                                                                 *  
                                                                if(cTree.titlePath === rootPath){
                                                                	module.deleteCategoryHistory(data.path);
                                                                }else{
                                                                	module.deleteListHistory(data.path);
                                                                }
                                                                 */
                } else {
                    tools.tipMsg(appLanguage["changeLang"]["deleteFailed"] + ' "' + data.path + '" '); //data.path.substr(data.path.lastIndexOf('/') + 1));
                    // 
                    //					module.rollBackList(data.path);
                }
            };

            var renameOp = function(status) {
                tools.logs("rename operation status = " + status);
                var folders = listTree.pathFolderList[listTree.pathHistory["rename"][data.path].path].slice(0);
                var files = listTree.pathFileList[listTree.pathHistory["rename"][data.path].path].slice(0);

                if (status == "1") {
                    // rename
                    if (listTree.pathHistory["rename"][data.path]) {
                        if (module.renameItem.isFile == "true") {
                            if (listTree.pathFileList[module.renameItem.path]) {
                                delete listTree.pathFileList[module.renameItem.path + "/" + module.renameItem.oldName];
                                delete listTree.pathFolderList[module.renameItem.path + "/" + module.renameItem.oldName];
                            }
                        } else {
                            if (listTree.pathFolderList[module.renameItem.path]) {
                                delete listTree.pathFolderList[module.renameItem.path + "/" + module.renameItem.oldName];
                            }
                        }

                        delete listTree.pathHistory["rename"][data.path];
                    }
                } else {
                    tools.tipMsg(appLanguage["changeLang"]["renameFailed"]);
                    for (var i = 0; i < files.length; i++) {
                        if (files[i].name === listTree.pathHistory["rename"][data.path].newName) {
                            files[i].name = listTree.pathHistory["rename"][data.path].oldName;
                            files[i].path =
                                listTree.pathHistory["rename"][data.path].path + "/" + listTree.pathHistory["rename"][data.path].oldName;

                            listTree.pathFileList[listTree.pathHistory["rename"][data.path].path] = files;

                            if (files[i].isFile == "false") {
                                var oldListName = $(
                                    "#" +
                                    listTree.getPath(
                                        listTree.pathHistory["rename"][data.path].path + "/" + listTree.pathHistory["rename"][data.path].newName
                                    ) +
                                    " div:first .tree_folder_name"
                                );
                                oldListName[0].innerHTML = listTree.pathHistory["rename"][data.path].oldName;

                                for (var i = 0; i < folders.length; i++) {
                                    if (folders[i].name === listTree.pathHistory["rename"][data.path].newName) {
                                        folders[i].name = listTree.pathHistory["rename"][data.path].oldName;
                                        folders[i].path =
                                            listTree.pathHistory["rename"][data.path].path + "/" + listTree.pathHistory["rename"][data.path].oldName;

                                        listTree.pathFolderList[listTree.pathHistory["rename"][data.path].path] = folders;
                                    }
                                }
                            }

                            if (listTree.pathHistory["rename"][data.path]) {
                                delete listTree.pathHistory["rename"][data.path];
                            }
                        }
                    }
                }
            };
            var pasteOp = function(status) {
                tools.logs("module.pasteCount = " + module.pasteCount);
                tools.logs("paste operation status = " + status);
                module.pasteCount++;
                if (status == "1") {
                    tools.logs("paste status = " + status, "self.clipBoard.isDelete = " + self.clipBoard.isDelete);
                    if (self.clipBoard.isDelete == 0) {
                        var insertNewPath = function() {
                            for (var i = 0; i < self.clipBoard.paths.length; i++) {
                                var f = $.extend(true, {}, self.clipBoard.files[i]);

                                if (self.clipBoard.paths[i] == data.path) {
                                    f.path = self.clipBoard.newPath + "/" + self.clipBoard.files[i].name;
                                    tools.logs("self.clipBoard.newPath = " + self.clipBoard.newPath, "data.path = " + data.path);
                                    if (self.clipBoard.files[i].isFile == "false") {
                                        listTree.pathFolderList[self.clipBoard.newPath].push(f);
                                        listTree.pathFileList[self.clipBoard.newPath].push(f);
                                        listTree.addLeaf(self.clipBoard.newPath, f);
                                    } else {
                                        listTree.pathFileList[self.clipBoard.newPath].push(f);
                                        //									listTree.pathFileList[self.clipBoard.newPath].splice(0, 0, f);
                                        //									listTree.pathFileList[self.clipBoard.newPath].unshift(self.clipBoard.files[i]);
                                    }
                                }
                            }

                            self.currentPathFiles = listTree.pathFileList[self.clipBoard.newPath];
                        };
                        insertNewPath();
                    } else {
                        var clearClip = function() {
                            if (self.clipBoard.count === self.clipBoard.paths.length) {
                                module.clearClipBoard();
                                self.selectedIndex.splice(0, self.selectedIndex.length);
                                Config.Runtime.eventBus.emit("clearSelected", {
                                    clearAll: true
                                });
                            }
                        };

                        var deleteAndInsert = function() {
                            for (var j = 0; j < self.clipBoard.paths.length; j++) {
                                tools.logs(" self.clipBoard.paths.length = " + self.clipBoard.paths.length);
                                var f = $.extend(true, {}, self.clipBoard.files[j]);

                                if (self.clipBoard.paths[j] == data.path) {
                                    f.path = self.clipBoard.newPath + "/" + self.clipBoard.files[j].name;
                                    tools.logs("self.clipBoard.newPath = " + self.clipBoard.newPath, "data.path = " + data.path);
                                    if (self.clipBoard.files[j].isFile == "false") {
                                        // insert
                                        listTree.pathFolderList[self.clipBoard.newPath].push(f);
                                        listTree.pathFileList[self.clipBoard.newPath].push(f);
                                        listTree.addLeaf(self.clipBoard.newPath, f);
                                        // remove leaf
                                        listTree.removeLeaf(data.path);
                                        // delete folders
                                        var oldFolders = listTree.pathFolderList[self.clipBoard.fromPath].slice(0);
                                        for (var i = 0; i < oldFolders.length; i++) {
                                            if (oldFolders[i].path === data.path) {
                                                oldFolders.splice(i, 1);
                                            }
                                        }
                                        listTree.pathFolderList[self.clipBoard.fromPath] = oldFolders;
                                        // delete files
                                        var oldFiles = listTree.pathFileList[self.clipBoard.fromPath].slice(0);
                                        for (var i = 0; i < oldFiles.length; i++) {
                                            if (oldFiles[i].path === data.path) {
                                                oldFiles.splice(i, 1);
                                            }
                                        }
                                        listTree.pathFileList[self.clipBoard.fromPath] = oldFiles;
                                    } else {
                                        // insert
                                        listTree.pathFileList[self.clipBoard.newPath].push(f);
                                        // delete
                                        var oldArray = listTree.pathFileList[self.clipBoard.fromPath].slice(0);
                                        for (var i = 0; i < oldArray.length; i++) {
                                            if (oldArray[i].path === data.path) {
                                                oldArray.splice(i, 1);
                                            }
                                        }
                                        listTree.pathFileList[self.clipBoard.fromPath] = oldArray;
                                    }
                                    //  paths
                                    tools.logs("delete paths[j] = " + j);
                                    self.clipBoard.count++;
                                }
                            }

                            if (self.currentPath === self.clipBoard.newPath) {
                                self.currentPathFiles = listTree.pathFileList[self.clipBoard.newPath];
                            }

                            clearClip();
                        };
                        deleteAndInsert();
                    }
                    module.setCrrentPath();
                } else {
                    tools.logs("paste error");
                }
            };

            switch (data.type) {
                case "new":
                    createOp(data.status);
                    break;
                case "delete":
                    deleteOp(data.status);
                    break;
                case "rename":
                    renameOp(data.status);
                    break;
                case "paste":
                    pasteOp(data.status);
                    break;
                default:
                    tools.logs("operation error");
                    return;
            }
        };

