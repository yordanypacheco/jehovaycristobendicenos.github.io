define('slider',["tools",
        "drag"], 
        
    function(tools,
    		drag){
	
	//var soliderMove = new drag()
	//var slider = tools.hqxClone(drag);
	
	var soliderMrg = {
			sliderMove:null,
			init:function(id , min , max , width , change){
				//this.sliderMove = tools.hqxClone(new drag(id));
				this.min = min;
				this.max = max;
				this.sliderWidth = width;
				this.change = change;
				this.wheelOldValue = 0;
				this.step = parseInt(width/max , 10);
				this.sliderMove = new drag(id , this.mousedown , null , this.mouseMove);
			//	console.log("this.soliderMove:" , this.sliderMove);
			},
			
			mousewheel: function(value){
				//console.log("mousewheel:" , e);
				this.setSliderValue(value);
				this.wheelOldValue = value;
			},
			
			mousedown:function(){
				this.flag = true;
				this.el.style.cursor ="move";
				this._start_x = parseInt(this.el.style.left , 10);
				this._start_y = parseInt(this.el.style.top , 10);
			},
			
			setSliderValue: function(value){
				
				var oldLeft = parseInt(this.sliderMove.el.style.left , 10) ||0;
				
				var left = oldLeft + value;
				if(value > 0 && oldLeft >= 427) return;
				if(value < 0 && oldLeft <= -13) return;
				//console.log(this.sliderMove.jqEl.get(0))
				//console.log("oldLeft:" , this.sliderMove.jqEl.get(0).style.left)
				this.sliderMove.el.style.left = left + "px";

			},
			
			mouseMove:function(e){
				
				if(document.getElementById("myImage").style.display != "block") return;
				if(this.flag){
					//console.log("this clientLeft" , this.clientLeft)
					var x = e.pageX -this._end_x;
	//				var y = e.pageY - this._end_y ;
					if(x<0 && parseInt(this.el.style.left , 10) < -13) return;
					
					if(x>0 && parseInt(this.el.style.left , 10) >= 427) return;
					if(!this.el.style.left) this.el.style.left="0px";
					this.el.style.left = (parseInt(this.el.style.left,10) + x) + "px";
					if(soliderMrg.change){
						//var flag = x >0 
						soliderMrg.change(parseInt(parseInt(this.el.style.left , 10)/soliderMrg.step , 10) , x>0 ? 1 : 0);
						
					} 
					//console.log("this:" , x , this.el.style , this.flag);
				}else{
					//this.style.cursor ="url(srcimg/ic_hand2.gif),auto";
				}
				this._end_x = e.pageX;
				this._end_y = e.pageY;
				this.el.style.cursor="move";
			}
		}
	
	return soliderMrg;
	
});

