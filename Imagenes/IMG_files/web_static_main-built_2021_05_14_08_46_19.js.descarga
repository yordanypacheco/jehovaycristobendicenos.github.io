				 }
				var movT = top+height * scan/2;
				var movL = left + width * scan/2;
				
				if(e && self.container("bigShowImgClick" , {x:e.pageX || e.clientX ,y:e.pageY || e.clientY})){
					
					var EX = e.pageX || e.clientX;
					var EY = e.pageY || e.clientY;
					movT = top + (EY - top)*scan;
					movL = left + (EX - left)*scan;
				}
			
				
				$("#bigShowImgClick").css({width:(width - width * scan),height:height - height * scan,top:movT , left:movL})
