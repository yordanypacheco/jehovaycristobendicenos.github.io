            fadeInOut: function(id, time) {

                var dom = document.getElementById(id);
                dom.style.opacity = 0;
                //console.log(document.getElementById(id))

                var animframe = this.animationFrame();
                var i = time / 10;
                var opacity = 1 / i;
                var j = 0;
                var runAnim = function(time) {
                    //console.log()
                    op = opacity * j;
                    if (j < i) {
