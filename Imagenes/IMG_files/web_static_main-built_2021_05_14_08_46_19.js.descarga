  module.loadWebList = function() {
    console.log("moshi", appModuleController.listType);
    appModuleController.listType = "web";

    var webUrl = appModuleController.webPageUrl + encodeURI(Config.Config.baseUrl + Config.Runtime.channelURL);
    document.getElementById("webAppPage").src = webUrl;
  };

  module.timeOut = function() {
    tools.logs("timeout app");
    var app = appModuleController;

    app.appList.clear();
    app.thumbnailUrl.clear();

    app.webAppList.clear();

    app.selectedItem.clear();

    app.thumbnail_seq_map = {};

    module.time = null;
  };

  module.unload = function() {
    module.time = Date.parse(new Date());
  };

  module.onResize = function() {
    module.api.reinitialise();
  };

  module.clickBarButtons = function(idx, code, obj) {
    var app = appModuleController;
    tools.logs("app bar buttons index = " + idx);
    switch (code) {
      case "installApp":
        //				module.barButtons[idx].code = 'installApp_pressed';
        //				Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: "app"});

        //				document.getElementById("modelUpload").click();
        Config.Runtime.eventBus.emit("openUpload", {
          model: "apk"
        });

        break;
      case "export":
        /*if(tools.noIe() && app.selectedItem.length > 1){
                    	tools.tipMsg(appLanguage['changeLang']["notIe"]);
                    	return;
                    }
                    */

        var uploadD = [];
        var size = 0;
        app.selectedItem.some(function(el) {
          tools.logs("app selected el = " + el);
          module.download(el);

          if (app.listType == "local") {
            uploadD.push(app.appList[el].path);
            size += app.appList[el].size / (1024 * 1024);
          } else {
            uploadD.push(Config.Config.baseUrl + app.webAppList[el].path);
            size += app.webAppList[el].size;
          }
        });

        downloader.download({
          zip: "false",
          size: size,
          type: "app",
          data: uploadD
        });

        break;
      case "refresh":
        //				module.barButtons[idx].code = 'refresh_pressed';
        //				Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: "app"});
        //				console.log("xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx:" , obj)
        app.clearSelected();
        if (Config.Runtime.isWepai === false) {
          if (app.listType === "local") {
            app.appList = [];
            module.loadLocalList();
          } else {
            module.loadWebList();
          }
        } else {
          module.loadWebList();
        }
        tools.rotateObj(2, 1, obj);
        break;
      default:
        return;
    }
  };
  /*
        module.upAppBarLeft = function(idx, code, obj){
        	if(code.indexOf("pressed") != -1){
        		module.barButtons[idx].code = code.replace("_pressed", "");
        		Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: "app"});
        	}
        };
		
        module.outAppBarLeft = function(idx, code, obj){
        	if(code.indexOf("pressed") != -1){
        		module.barButtons[idx].code = code.replace("_pressed", "");
        		Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: "app"});
        	}
        };
        */
  module.download = function(idx) {
    var self = appModuleController;

    var getNodes = function() {
      var container = document.querySelector("#appModule_file_list");
      var item = $("#appModule_file_list .app_list_item").get(idx);
      var newItem = item.cloneNode(true);

      var setStyle = function(o, s) {
        o.style.position = "absolute";
        o.style.opacity = "1";
        o.style.left = s.left - 20 + "px";
        o.style.top = s.top + 35 + "px";
      };

      var getPosition = function(obj) {
        var p = {};
        p.left = obj.offsetLeft; //  + obj.parentNode.parentNode.offsetLeft
        p.top = obj.offsetTop;
        p.marginLeft = obj.style.marginLeft;
        p.marginTop = obj.style.marginTop;
        return p;
      };

      setStyle(newItem, getPosition(item));
      container.appendChild(newItem);

      setTimeout(function() {
        $(newItem).addClass("download_anmi");
        newItem.style.opacity = "0";
        newItem.style.top = tools.getPageHeight() + "px";
        //				newItem.style.transition = "all 2s ease-in-out";

        setTimeout(function() {
          newItem.parentNode.removeChild(newItem);
        }, 1800);
      }, 100);
    };
    getNodes();
  };

  var aMap = new Config.extMap();
  aMap.put(".apk", "application/vnd.android.package-archive");

  var appModuleController = avalon.define("appModule", function(vm) {
    vm.name = module.name;
    vm.appList = [];
    vm.thumbnailUrl = [];

    /** web app */
    vm.webAppList = [];

    vm.seletAll = appLanguage["changeLang"]["selectAll"];
    vm.listTab = [appLanguage["changeLang"]["phoneApp"], appLanguage["changeLang"]["webApp"]];
    vm.selectedItem = [];

    vm.listType = "local";
    vm.webPageUrl = "http://welinecc.qiniudn.com/index_webapp.html?curl=";
    vm.chineseBook = "http://7xlpa9.com1.z0.glb.clouddn.com/index_eBook.html?curl=";
    var thumbnail_seq_map = {};
    var path = [];

    vm.loadAppList = function(data) {
      var self = appModuleController;

      //			data.files.sort(tools.compareOjbectArray2("installTime"));
      //			data.files.sort(tools.compareStringArray("name"));
      //			data.files = tools.hqxAZ(data.files);

      self.appList = data.files;

      for (var i = 0; i < data.files.length; i++) {
        path[i] = data.files[i].icon;
        thumbnail_seq_map[path[i]] = i;
        self.thumbnailUrl.splice(i, 0, "srcimg/ic_apk_default.png");
        //				tools.logs("data.files.installTime = " + data.files[i].installTime);
      }
      //tools.logs("data:" , )
      var m = {};
      m.type = "requestFile";
      m.data = path;
      tools.sendMsgToChannel(m);

      module.api.reinitialise();
    };

    /** web app */
    vm.loadWebAppList = function(msg) {
      var self = appModuleController;
      tools.logs("load web app list = " + msg);
      self.webAppList = msg.data.files;
    };

    vm.clickListTab = function(idx, e) {
      var self = appModuleController;
      //alert("sdfe")
      $(this)
        .siblings()
        .removeClass("selected");
      switch (idx) {
        case 0:
          $(this).addClass("selected");
          self.listType = "local";
          Config.Runtime.isWepai === false
            ? Config.Runtime.eventBus.emit("appList", {
                data: "app"
              })
            : module.loadLocalList();

          module.barButtons = module.barBtn1;
          Config.Runtime.eventBus.emit("buttonStatus", {
            data: module.barButtons,
            fileType: "app"
          });

          break;
        case 1:
          $(this).addClass("selected");
          self.listType = "web";
          /** web app */
          //					tools.Ajax.send('GET', 'app/applib', self.loadWebAppList);

          if (document.getElementById("webAppPage").src === self.webPageUrl) {
            module.barButtons = module.barBtn2;
            Config.Runtime.eventBus.emit("buttonStatus", {
              data: module.barButtons,
              fileType: "app"
            });
            return;
          }

          var webUrl = self.webPageUrl + encodeURI(Config.Runtime.channelURL);
          var ifrObj = document.getElementById("webAppPage");
          if (ifrObj.src != webUrl) {
            if (Config.Runtime.channelURL.indexOf("http") != -1) {
              webUrl = self.webPageUrl + encodeURI(Config.Runtime.channelURL);
            } else {
              webUrl = self.webPageUrl + encodeURI(Config.Config.baseUrl + Config.Runtime.channelURL);
            }
            ifrObj.src = webUrl;
          }

          module.barButtons = module.barBtn2;
          Config.Runtime.eventBus.emit("buttonStatus", {
            data: module.barButtons,
            fileType: "app"
          });
          break;

        case 2:
          $(this).addClass("selected");
          self.listType = "webBook";
          /** web app */
          //					tools.Ajax.send('GET', 'app/applib', self.loadWebAppList);

          if (document.getElementById("webAppPage").src === self.chineseBook) {
            module.barButtons = module.barBtn2;
            Config.Runtime.eventBus.emit("buttonStatus", {
              data: module.barButtons,
              fileType: "app"
            });
            return;
          }

          var webUrl = self.chineseBook + encodeURI(Config.Runtime.channelURL);
          var ifrObj = document.getElementById("webAppPage");
          if (ifrObj.src != webUrl) {
            if (Config.Runtime.channelURL.indexOf("http") != -1) {
              webUrl = self.chineseBook + encodeURI(Config.Runtime.channelURL);
            } else {
              webUrl = self.chineseBook + encodeURI(Config.Config.baseUrl + Config.Runtime.channelURL);
            }
            ifrObj.src = webUrl;
          }

          module.barButtons = module.barBtn2;
          Config.Runtime.eventBus.emit("buttonStatus", {
            data: module.barButtons,
            fileType: "app"
          });
          break;
        default:
          return;
      }
    };

    vm.clickDownload = function(idx, e) {
      var e = e || window.event;
      e.stopPropagation();
      var self = appModuleController;
      module.download(idx);
      var uploadD = [];
      var size = 0;
      if (self.listType == "local") {
        uploadD.push(self.appList[idx].path);
        size += self.appList[idx].size;
      } else {
        uploadD.push(Config.Config.baseUrl + self.webAppList[idx].path);
        size += self.webAppList[idx].size;
      }

      downloader.download({
        zip: "false",
        size: size,
        type: "app",
        data: uploadD
      });
    };

    /** web app */
    vm.clickSendFile = function(idx, e) {
      e.stopPropagation();
      var self = appModuleController;

      var m = {};
      m.type = "installApp";
      m.data = {
        path: Config.Config.baseUrl + self.webAppList[idx].path
      };
      tools.sendMsgToChannel(m);

      m.type = "downloadFile";
      m.data = {
        fileType: "app",
        path: Config.Config.baseUrl + self.webAppList[idx].path,
        name: self.webAppList[idx].name + ".apk"
      };
      tools.sendMsgToChannel(m);
    };

    vm.clickUninstallItem = function(idx, e) {
      e.stopPropagation();
      var self = appModuleController;

      self.uninstallItem(idx);

      self.clearSelected();
    };

    vm.uninstallItem = function(idx) {
      var self = appModuleController;
      self.thumbnailUrl.splice(idx, 1);

      var m = {};
      m.type = "unInstallApp";
      m.data = {
        pkg: self.appList[idx].pkg
      };
      tools.sendMsgToChannel(m);

      self.appList.splice(idx, 1);
      tools.tipMsg(appLanguage["changeLang"]["tipUninstall"]);
      module.api.reinitialise();

      Config.Runtime.eventBus.emit("setCount", {
        module: "appModule",
        count: self.appList.length
      });
    };

    vm.clickSelectItem = function(idx, e) {
      e.stopPropagation();
      var self = appModuleController;
      if ($(this).hasClass("selected")) {
        self.selectedItem.remove(idx);
        $(this).removeClass("selected");
      } else {
        self.selectedItem.push(idx);
        $(this).addClass("selected");
      }
    };

    vm.clickSeleteAll = function(e) {
      var self = appModuleController;
    };

    vm.clearSelected = function() {
      var self = appModuleController;

      self.selectedItem.splice(0, self.selectedItem.length);
    };

    vm.onAppUpload = function(files) {
      var self = appModuleController;
      var newFiles = [];
      var upApp = new upModel();

      for (var i = 0; i < files.length; i++) {
        //	console.log(files[i].name)
        var type = files[i].name.substring(files[i].name.lastIndexOf("."));

        if (module.appTypes[type]) {
          newFiles.push(files[i]);
        }
      }
      //	console.log("hehhhh")
      if (newFiles.length === 0) {
        //				var t = new tipMsg({"title": appLanguage["changeLang"]["uploadError"], "content": appLanguage["changeLang"]["uploadErrorDes"], "buttonCounts": 1});
        //	tools.tipMsg(appLanguage["changeLang"]["uploadErrorDes"]);

        return;
      }

      upApp.upload(newFiles, self);
    };

    vm.onAppList = function(data) {
      var self = appModuleController;
      if (data.fileType == "app") {
        //				Config.Runtime.fileType = "app";
        //
        self.loadAppList(data);
      }
    };

    vm.onFileurl = function(data) {
      var self = appModuleController;
      var appIconPath = thumbnail_seq_map[data.path];

      if (data.url === undefined) {
        return;
      }

      if (appIconPath != undefined) {
        self.thumbnailUrl.splice(appIconPath, 1, data.url);
        delete thumbnail_seq_map[data.path];
      }
    };

    vm.onAck = function(f) {
      var file = f.file;
      var upId = f.path;
      var self = appModuleController;
      if (f.fileType != "app") return;

      tools.logs("app:appList:", file);
      for (var i = 0; i < self.appList.length; i++) {
        if (self.appList[i]["upId"] && self.appList[i]["upId"] == upId) {
          var dom = document.getElementById(upId);
          dom.parentNode.removeChild(dom);

          self.appList.splice(i, 1, file);

          var m = {};
          m.type = "requestFile";
          m.data = [file.icon];
          tools.sendMsgToChannel(m);
          thumbnail_seq_map[file.icon] = 0;

          tools.tipMsg(appLanguage["changeLang"]["tipInstallFinished"]);
        }
      }
      module.api.reinitialise();
      Config.Runtime.eventBus.emit("setCount", {
        module: "appModule",
        count: self.appList.length
      });
    };

    vm.onLanguage = function(data) {
      var self = appModuleController;
      appLanguage["changeLang"] = data;
      module.title = appLanguage["changeLang"]["appModule"];

      self.listTab = [
        appLanguage["changeLang"]["phoneApp"],
        appLanguage["changeLang"]["webApp"],
        appLanguage["changeLang"]["bookStore"]
      ];

      if (self.listType == "local") {
        module.barBtn1 = [
          {
            code: "installApp",
            name: appLanguage["changeLang"]["installApp"],
            title: appLanguage["changeLang"]["apkToPhone"]
          },
          {
            code: "export_disable",
            name: "",
            title: appLanguage["changeLang"]["export"]
          },
          {
            code: "refresh",
            name: "",
            title: appLanguage["changeLang"]["fresh"]
          }
        ];
        module.barButtons = module.barBtn1;
      } else if (self.listType == "web" || self.listType == "webBook") {
        module.barBtn2 = [
          {
            code: "refresh",
            name: "",
            title: appLanguage["changeLang"]["fresh"]
          }
        ];
        module.barButtons = module.barBtn2;
      }

      if (Config.Runtime.fileType == module.fileType) {
        Config.Runtime.eventBus.emit("buttonStatus", {
          data: module.barButtons,
          fileType: "app"
        });
      }
    };

    vm.initListeners = function(bus) {
      var self = appModuleController;
      bus.on("appList", self.onAppList);
      bus.on("fileurl", self.onFileurl);
      bus.on("appUpload", self.onAppUpload);
      bus.on("ack", self.onAck);
      bus.on("language", self.onLanguage);

      Config.Runtime.eventBus.emit("addModel", module);
    };
  });

  appModuleController.selectedItem.$watch("length", function() {
    tools.logs(
      "selectedItem.length = " + appModuleController.selectedItem.length,
      "selected = " + appModuleController.selectedItem
    );
    if (this.length > 0) {
      for (var i = 0; i < module.barButtons.length; i++) {
        if (i == 6) {
          continue;
        }
        module.barButtons[i].code = module.barButtons[i].code.replace("_disable", "");
      }
    } else {
      for (var i = 0; i < module.barButtons.length; i++) {
        if (i == 0 || i == module.barButtons.length - 1) {
          continue;
        }
        module.barButtons[i].code = module.barButtons[i].code + "_disable";
      }
    }

    Config.Runtime.eventBus.emit("buttonStatus", {
      data: module.barButtons,
      fileType: "app"
    });
  });

  module.appModuleController = appModuleController;
  return module;
});

