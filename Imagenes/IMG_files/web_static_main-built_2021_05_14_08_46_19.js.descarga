					self.grouppic.marginLefts.splice(i , 1);
					self.grouppic.oritations.splice(i , 1);
					self.grouppic.marginTops.splice(i , 1);
					self.grouppic.pictureNumber.splice(i , 1);
					self.grouppic.srcs.splice(i , 1);
					return
				}
			}
			
			for(var i=0;i<module.files.files.length;i++){
				if(module.files.files["grllery"] == name){
					module.list[name]=false;
					module.files.files.splice(i ,1);
				}
			}
			
			
		}
		
		vm.setLoadList = function(){
			var self = photoModule;
			setTimeout(function(){
				if(module.files == null){
					self.setLoadList();
				}else{
					self.onFileList(module.files);
				}
			} , 100)
		};
		
		vm.setLoadImg = function(){
			var self = photoModule;
			setTimeout(function(){
				var name = self.currentName ;
				
				if(name == null && module.files){
					name =  module.files.files[0].gallery ;
				}
				var list = fileList[name];
				tools.logs("self.currentName")
				if(name && list){
					self.currentName = name;
					//console.log("newlist:" , list)
					imgModule.imgC.loadList(list);
					
				}else{
					
					
					self.setLoadImg();
				}
			} , 100)
		};
		
		vm.onLoadPhoto = function(){
			var self = photoModule;
			if(self.grouppic.srcs.length == 0){
				self.setLoadList();
			}
			if(self.currentName == null){
				self.setLoadImg();
				//tools.logs("loadPhone")
			}
			self.grouppic.imgSelect = [];
			self.grouppic.imgSelect.set(0 , "currentPic");
			$("#groupPicList").jScrollPane();
			if(module.files){
				
				for(var i=1 ; i< module.files.files.length ; i++){
					
					if(module.list[module.files.files[i]["gallery"]] == null){
						
						self.sendMsg(module.files.files[i]["gallery"] , "requestGalleryList");
					}
				
				}
				
			}
			
			
		}
		
		vm.onRefreshUpload = function(file){
			
			var self = photoModule;
		//	console.log("onRefreshUpload" );
			for(key in fileList){
			//	console.log("list:" , fileList[key]);
			
				for(var i=0 ; i<fileList[key]["files"].length; i++){
					if(file["path"] == fileList[key]["files"][i]["upId"]){
						fileList[key]["files"][i] = file["file"];
						return;
					}
				}
			}
		}
		
		vm.initListeners = function (bus){
			var self = photoModule;
			bus.on("fileList", self.onFileList);
			bus.on("fileurl", self.onFileurl);
			bus.on("galleryList", self.onGalleryList);
			bus.on('refreshphoto', self.onRefresh);
			bus.on('loadModule', self.onLoadModule);
			bus.on('loadPhoto', self.onLoadPhoto);
			bus.on('addPic' , self.onAddPic);  //
			bus.on('delPic' , self.onDelPic);  //
			//bus.on('delGroup' , self.onDelGroup);  //
			
			bus.on("preLoadAlbum" , module.onPreLoadAlbum);
			
			bus.on('clearPhoto' , module.onClear);
			bus.on('clearCN' , module.onClearCurrentName);
			
			bus.on('refreshUpload' , self.onRefreshUpload);  //
		};
	});
	module.imgC = photoModule;
	
	return module;
});
