    module.sendRequestFile = function(index) {
        var m = musicModuleController;
        var r = {};

        m.curPath = m.musicList[index].path;
        m.curIdx = index;

        if (!module.music_sendPath[m.curPath]) {
            r.type = "requestFile";
            //			console.log("index = " + index);
            r.data = [m.musicList[index].path];
            tools.sendMsgToChannel(r);

            module.music_sendPath[m.curPath] = true;
            //			console.log(">>> module.music_sendPath = " + m.musicList[index].path);
        } else {
            //			console.log("*** " + m.curPath + "***");
        }
