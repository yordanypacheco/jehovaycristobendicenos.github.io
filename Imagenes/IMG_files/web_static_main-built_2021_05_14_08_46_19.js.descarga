define('fileModule',[
    "avalon",
    "tools",
    "config",
    "listTree",
    "cTree",
    "tipMsg",
    "downloader",
    "upModel",
    "rectSelect",
    "appLanguage",
    "fileUpFlag",
    "text!../template/file-module.html"
], function(
    avalon,
    tools,
    Config,
    listTree,
    cTree,
    tipMsg,
    downloader,
    upModel,
    rectSelect,
    appLanguage,
    fileUpFlag,
    tpl_fileModule
) {
    var module = {};
    module.name = "fileModule";
    module.title = appLanguage["changeLang"]["fileModule"];
    module.fileType = "file";
    module.apiTree = null;
    module.apiList = null;
    module.isDbl = false;
    module.barButtons = [];
    module.time = null;
    module.timeFn = null;
    module.pasteCount = 0;
    module.clickCatetoryFn = null;

    module.init = function(container) {
        container.innerHTML = tpl_fileModule;
        module.setLanguage();

        module.apiTree = tools.scrollInitNotAuto("file_tree_list");

        var rs = new rectSelect(document.getElementById("file_item_container"), "file_item");

        // 因为有可能出现大量数据，所以使用原始滚动条
        //		module.apiList = tools.scrollInit("file_icon_list");
    };

    module.setLanguage = function() {
        var f = fileModuleController;

        module.barButtons = module.getButtons();
        f.rootCategory = cTree.getRoot();
        cTree.init();
        module.title = appLanguage["changeLang"]["fileModule"];
        if (Config.Runtime.fileType == module.fileType) {
            Config.Runtime.eventBus.emit("buttonStatus", {
                data: module.barButtons,
                fileType: "file"
            });
        }
    };

    module.getButtons = function() {
        return [{
                code: "file_add",
                name: appLanguage["changeLang"]["addFile"],
                title: appLanguage["changeLang"]["docToPhone"]
            },
            {
                code: "export_disable",
                name: "",
                title: appLanguage["changeLang"]["export"]
            },
            {
                code: "new_folder",
                name: "",
                title: appLanguage["changeLang"]["newFolder"]
            },
            {
                code: "rename_disable",
                name: "",
                title: appLanguage["changeLang"]["rename"]
            },
            {
                code: "cut_disable",
                name: "",
                title: appLanguage["changeLang"]["cut"]
            },
            {
                code: "copy_disable",
                name: "",
                title: appLanguage["changeLang"]["copy"]
            },
            {
                code: "paste_disable",
                name: "",
                title: appLanguage["changeLang"]["paste"]
            },
            {
                code: "delete_disable",
                name: "",
                title: appLanguage["changeLang"]["delete"]
            },
            {
                code: "refresh",
                name: "",
                title: appLanguage["changeLang"]["fresh"]
            }
        ];
    };

    module.load = function() {
        module.time = null;
        if (fileModuleController.devcieStorageInfo.length > 0) {
            //			module.apiTree.reinitialise();
            //			module.apiList.reinitialise();
            return;
        } else {
            var m = {};
            m.type = "getDeviceStorageInfo";
            m.data = "file";
            tools.sendMsgToChannel(m);

            cTree.sendReq("getLastedFileList");

            module.setLanguage();

            fileModuleController.bindEvt();
        }
    };

    module.refresh = function() {
        var fileC = fileModuleController;
        var rootPath = fileC.currentPath.substr(0, cTree.titlePath.length);

        if (rootPath == cTree.titlePath) {
            fileC.currentPathFiles = [];

            listTree.deleteLists(fileC.currentPath);

            switch (fileC.currentPath) {
                case fileC.rootCategory[0].path:
                    cTree.sendReq("getLastedFileList");
                    break;
                case fileC.rootCategory[1].path:
                    cTree.sendReq("getBigFileList");
                    break;
                case fileC.rootCategory[2].path:
                    cTree.sendReq("getZipFileList");
                    break;
                case fileC.rootCategory[3].path:
                    cTree.sendReq("getAPKFileList");
                    break;
                default:
                    fileC.currentPathFiles = [];
                    cTree.openCloseLeaf(fileC.currentPath, "refresh", fileC);
                    break;
            }

            module.clearSelected();

            return;
        }
        // 刷新后删除当前目录级其子目录的映射
        listTree.deleteLists(fileC.currentPath);

        fileC.currentPathFiles = [];
        listTree.openCloseLeaf(fileC.currentPath, "refresh", fileC);
        module.clearClipBoard();
        module.clearSelected();
    };

    module.timeOut = function() {
        var fileC = fileModuleController;
        tools.logs("timeout file");
        // normal, rename, create
        fileC.fileItemStatus = "normal";

        fileC.fileCategoryPath = cTree.titlePath;
        fileC.rootCategory = cTree.getRoot();

        fileC.devcieStorageInfo.clear();
        // 监控对象不能直接清空， 否则会出错。
        //		fileC.currentItem = {};
        //		fileC.clipBoard = {};

        fileC.currentPathArr.clear();

        fileC.selectedIndex.clear();

        // 右侧列表
        fileC.currentPathFiles.clear();

        fileC.isEmpty = false;

        fileC.clipBoardFiles.clear();

        listTree.pathFileList = [];
        listTree.pathFolderList = [];
        listTree.pathFolderStatus = {};
        listTree.pathHistory = {
            dir: [],
            rename: {}
        };

        module.time = null;
    };

    module.unload = function() {
        module.time = Date.parse(new Date());
    };

    module.onResize = function() {
        module.apiTree.reinitialise();
        //		module.apiList.reinitialise();
    };

    module.clickBarButtons = function(idx, code, obj) {
        var fileC = fileModuleController;

        switch (code) {
            case "file_add":
                //				module.barButtons[idx].code = 'file_add_pressed';
                //				Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: "file"});

                Config.Runtime.eventBus.emit("openUpload", {
                    model: "file"
                });

                break;
            case "export":
                //				module.barButtons[idx].code = 'export_pressed';
                //				Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: "file"});
                if (fileC.fileItemStatus !== "normal") {
                    return;
                }
                /*
                                                    for(var i=0 ; i<fileC.selectedIndex.length ; i++){
                                                    	if(fileC.currentPathFiles[fileC.selectedIndex[i]].isFile == "false"){
                                                    		tools.tipMsg(appLanguage["changeLang"]["fileExportError"]);
                                                    		return;
                                                    	}
                                                    }
                                                    */
                module.exportFiles();
                break;
            case "new_folder":
                //				module.barButtons[idx].code = 'new_folder_pressed';
                //				Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: "file"});

                module.opCreateFolder();
                break;
            case "rename":
                //				module.barButtons[idx].code = 'rename_pressed';
                //				Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: "file"});
                if (fileC.fileItemStatus !== "normal") {
                    return;
                }
                module.opRename();
                break;
            case "cut":
                //				module.barButtons[idx].code = 'cut_pressed';
                //				Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: "file"});
                if (fileC.fileItemStatus !== "normal") {
                    return;
                }
                module.opCut();
                break;
            case "copy":
                //				module.barButtons[idx].code = 'copy_pressed';
                //				Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: "file"});
                if (fileC.fileItemStatus !== "normal") {
                    return;
                }
                module.opCopy();
                break;
            case "paste":
                //				module.barButtons[idx].code = 'paste_pressed';
                //				Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: "file"});

                module.opPaste();
                break;
            case "delete":
                //				module.barButtons[idx].code = 'delete_pressed';
                //				Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: "file"});
                if (fileC.fileItemStatus !== "normal") {
                    return;
                }
                var t = new tipMsg({
                    title: appLanguage["changeLang"]["deleteFile"],
                    content: appLanguage["changeLang"]["deleteFileDes"],
                    fn: module.opDelete,
                    tipTheme: "red_the"
                });

                module.barButtons[idx].code = module.barButtons[idx].code.replace("_pressed", "");
                //				Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: "file"});

                break;
            case "refresh":
                //				module.barButtons[idx].code = 'refresh_pressed';
                //				Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: "file"});

                tools.rotateObj(2, 1, obj);

                module.refresh();
                break;
            default:
                return;
        }

        if (fileC.fileItemStatus !== "normal") {
            for (var i = 0; i < module.barButtons.length; i++) {
                module.barButtons[i].code = module.barButtons[i].code.replace("_disable", "");
                if (module.barButtons[i].code.indexOf("add") != -1 || module.barButtons[i].code.indexOf("refresh") != -1) {
                    continue;
                }

                module.barButtons[i].code = module.barButtons[i].code + "_disable";
            }
            Config.Runtime.eventBus.emit("buttonStatus", {
                data: module.barButtons,
                fileType: "file"
            });
            return;
        }
    };
    /*
        module.upAppBarLeft = function(idx, code, obj){
        	if(code.indexOf("pressed") != -1){
        		module.barButtons[idx].code = code.replace("_pressed", "");
        		Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: "file"});
        	}
        };
		
        module.outAppBarLeft = function(idx, code, obj){
        	if(code.indexOf("pressed") != -1){
        		module.barButtons[idx].code = code.replace("_pressed", "");
        		Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: "file"});
        	}
        };
        */

    // 0: 名字没重复， 2: 名字与其它文件名重复
    module.hasName = function(name) {
        var fileC = fileModuleController;

        for (var i = 0; i < listTree.pathFolderList[fileC.currentPath].length; i++) {
            if (listTree.pathFolderList[fileC.currentPath][i].name == name) {
                return 2;
            }
        }

        return 0;
    };

    (module.checkFileName = function(name) {
        var maxLen = 30;
        var reg = new RegExp('^[^/:*?"<>|]+$');
        if (name.length > maxLen) {
            tools.tipMsg("File name too long");
            return false;
        } else {
            if (reg.test(name)) {
                return true;
            } else {
                tools.tipMsg("File name is illegal characters");
            }
        }
    }),
    (module.exportFiles = function() {
        var fileC = fileModuleController;
        if (tools.noIe() && fileC.selectedIndex.length > 1) {
            tools.tipMsg(appLanguage["changeLang"]["notIe"]);
            return;
        }

        var getNodes = function() {
            var container = document.querySelector("#file_item_container");
            var items = document.querySelectorAll("#file_item_container .selected");
            var copyArr = [];
            var positionArr = [];

            if (items.length < 1) {
                return;
            }

            var setStyle = function(o, s) {
                o.style.position = "absolute";
                o.style.opacity = "1";
                o.style.left = s.left - 10 + "px";
                o.style.top = s.top - 5 + "px";
            };

            var getPosition = function(obj) {
                var p = {};
                p.left = obj.offsetLeft; //  + obj.parentNode.parentNode.offsetLeft
                p.top = obj.offsetTop;
                p.marginLeft = obj.style.marginLeft;
                p.marginTop = obj.style.marginTop;
                return p;
            };

            for (var i = 0; i < items.length; i++) {
                positionArr[i] = getPosition(items[i]);
                copyArr[i] = items[i].cloneNode(true);

                setStyle(copyArr[i], positionArr[i]);

                container.appendChild(copyArr[i]);
            }

            var move = tools.getPageHeight();
            setTimeout(function() {
                for (var i = 0; i < copyArr.length; i++) {
                    $(copyArr[i]).addClass("download_anmi");
                    copyArr[i].style.opacity = "0";
                    copyArr[i].style.top = move + "px";
                    //					copyArr[i].style.transition = "all 2s ease-in-out";
                }

                setTimeout(function() {
                    for (var i = 0; i < copyArr.length; i++) {
                        copyArr[i].parentNode.removeChild(copyArr[i]);
                    }
                }, 1800);
            }, 100);
        };

        getNodes();
        var downS = [];

        var zip = false;
        var size = 0;

        for (var i = 0; i < fileC.selectedIndex.length; i++) {
            //downloader.download(fileC.currentPathFiles[fileC.selectedIndex[i]].path);
            if (!Config.Runtime.isDirect == true && fileC.currentPathFiles[fileC.selectedIndex[i]]["isFile"] == "false") {
                new tipMsg({
                    title: appLanguage["changeLang"]["tipTitle"],
                    content: appLanguage["changeLang"]["singFileSize"],
                    fn: ""
                });
            } else {
                downS.push(fileC.currentPathFiles[fileC.selectedIndex[i]].path);
                size += fileC.currentPathFiles[fileC.selectedIndex[i]].size;
                if (fileC.currentPathFiles[fileC.selectedIndex[i]]["isFile"] == "false") zip = true;
            }
        }

        downloader.download({
            zip: zip,
            size: size,
            data: downS
        });
    });
    // 添加 list history
    module.addListHistory = function(path, file) {
        var fileC = fileModuleController;
        var f = {};
        f.name = file.name;
        f.size = file.size;
        f.path = file.path;
        f.time = file.time;
        f.isFile = file.isFile ? file.isFile : true;
        //		var f = file;
        f.folder = fileC.currentPath;

        listTree.pathHistory["delete"]["treeList"][path] = f;
        //		console.log('add list history path = ' + listTree.pathHistory['delete']['treeList'][path], "path = " + path);
    };
    // 添加 category history
    module.addCategoryHistory = function(path, file) {
        var fileC = fileModuleController;
        var f = {};
        f.name = file.name;
        f.size = file.size;
        f.path = file.path;
        f.time = file.time;
        f.isFile = file.isFile ? file.isFile : true;
        //		var f = file;
        f.folder = fileC.currentPath;

        listTree.pathHistory["delete"]["cateList"][path] = f;
        //		console.log('add category history path = ' + listTree.pathHistory['delete']['cateList'][path], "path = " + path);
    };
    // 删除 list history
    module.deleteListHistory = function(path) {
        //		console.log('delete list before path = ' + listTree.pathHistory['delete']['treeList'][path]);
        delete listTree.pathHistory["delete"]["treeList"][path];
        //		console.log('delete list end path = ' + listTree.pathHistory['delete']['treeList'][path]);
    };
    // 删除 category history
    module.deleteCategoryHistory = function(path) {
        //		console.log('delete category before path = ' + listTree.pathHistory['delete']['cateList'][path]);
        delete listTree.pathHistory["delete"]["cateList"][path];
        //		console.log('delete category end path = ' + listTree.pathHistory['delete']['cateList'][path]);
    };

    module.rollBackList = function(path) {
        var fileC = fileModuleController;
        var rootPath = fileC.currentPath.substr(0, cTree.titlePath.length);

        if (cTree.titlePath === rootPath) {
            cTree.categoryList[listTree.pathHistory["delete"]["cateList"][path]["folder"]].push(
                listTree.pathHistory["delete"]["cateList"][path]
            );

            if (listTree.pathHistory["delete"]["cateList"][path]["folder"] === fileC.currentPath) {
                fileC.currentPathFiles.splice(0, 0, listTree.pathHistory["delete"]["cateList"][path]);
            }

            module.deleteCategoryHistory(path);
        } else {
            if (listTree.pathHistory["delete"]["treeList"][path]["isFile"]) {
                listTree.pathFileList[listTree.pathHistory["delete"]["treeList"][path]["folder"]].push(
                    listTree.pathHistory["delete"]["treeList"][path]
                );
            } else {
                listTree.pathFileList[listTree.pathHistory["delete"]["treeList"][path]["folder"]].push(
                    listTree.pathHistory["delete"]["treeList"][path]
                );
                listTree.pathFolderList[listTree.pathHistory["delete"]["treeList"][path]["folder"]].push(
                    listTree.pathHistory["delete"]["treeList"][path]
                );
            }

            if (listTree.pathHistory["delete"]["treeList"][path]["folder"] === fileC.currentPath) {
                fileC.currentPathFiles.splice(0, 0, listTree.pathHistory["delete"]["treeList"][path]);
            }

            module.deleteListHistory(path);
        }
    };

    module.opDelete = function() {
        var fileC = fileModuleController;
        var selectedItems = [];
        var rootPath = fileC.currentPath.substr(0, cTree.titlePath.length);

        // 删除时要排序，从后往前删
        fileC.selectedIndex.sort(function(a, b) {
            return b - a;
        });

        for (var i = 0; i < fileC.selectedIndex.length; i++) {
            var idx = fileC.selectedIndex[i];
            var path = fileC.currentPathFiles[idx].path;

            tools.logs("fileC.currentPathFiles[idx].path = " + path);

            selectedItems.push(path);

            var o = document.getElementById(path);
            if (o) {
                o.parentNode.removeChild(o);
            }

            if (cTree.titlePath === rootPath) {
                module.addCategoryHistory(path, fileC.currentPathFiles[idx]);
                fileC.currentPathFiles.splice(idx, 1);
                cTree.deleteListItem(path, fileC.currentPath);
            } else {
                module.addListHistory(path, fileC.currentPathFiles[idx]);
                fileC.currentPathFiles.splice(idx, 1);
                listTree.deleteListItem(path, fileC.currentPath);
            }

            // 删除clipBoard中对应的项目
            module.deleteClipItem(path);
        }
        fileC.selectedIndex.splice(0, fileC.selectedIndex.length);
        fileC.selectedIndex.length = 0;
        fileC.selectedIndex.push(0);
        // 清除选中
        module.clearSelected();

        //		listTree.pathFileList[fileC.currentPath] = fileC.currentPathFiles.slice(0);

        var m = {};
        m.type = "fileDelete";
