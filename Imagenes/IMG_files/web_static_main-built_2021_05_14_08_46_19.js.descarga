define('smsCenter',["avalon", 
        "config", 
        "tools", 
        "debug", 
        "newSMS",
        "text!../template/sms-center.html", 
        "appLanguage",
        "tipMsg"], 
function(avalon, 
		Config, 
		tools, 
		debug, 
		newSMS,
		tpl_smsCenter,
		appLanguage,
		tipMsg){
	
	var module = {};
	module.api = null;
	module.recordsApi = null;
	module.time = null;
	module.barButtons = [
	                     {"code": "sendSMS", "name": appLanguage["changeLang"]["sendSMS"], "title": ""}, 
	                     {"code": "delete_disable", "name": "", "title": appLanguage["changeLang"]["delete"]},
	                     {"code": "refresh", "name": "", "title": appLanguage["changeLang"]["fresh"]}
	                    ];
	module.isVisible = false;
	module.fileType = 'sms';
	module.hasReplay = false;

	module.init = function(container){
		container.innerHTML = tpl_smsCenter;
		newSMS.init(document.getElementById("sms_window"));
		module.setLanguage();
		
		module.api = tools.scrollInitNotAuto("smsModule_file_list");
		
		module.recordsApi = tools.scrollInitNotAuto("tel_sms_list");
		
	};
	
	module.setLanguage = function(){
		module.barButtons = module.getButtons();
		
		if(module.isVisible && module.fileType == Config.Runtime.subModule){
			Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: "sms"});
		}
	};
	
	module.getButtons = function(){
		return [
                {"code": "sendSMS", "name": appLanguage["changeLang"]["sendSMS"], "title": ""}, 
                {"code": "delete_disable", "name": "", "title": appLanguage["changeLang"]["delete"]},
                {"code": "refresh", "name": "", "title": appLanguage["changeLang"]["fresh"]}
               ];
	};

	module.load = function(){
		module.time = null;
		
		if(smsCenterController.smsList.length > 0 && !smsCenterController.hasNewSMS){
//			Config.Runtime.eventBus.emit('SMSList',{'data':'0'});
			module.api.reinitialise();
			module.recordsApi.reinitialise();
		}else{
			module.isVisible = true;
			var m = {};
			m.type = 'requestSMSList';
			m.data = 'SMS';
			tools.sendMsgToChannel(m);
		}
		Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: "sms"});
		Config.Runtime.eventBus.emit('smsIsEmpty', false);
	};
	
	module.timeOut = function(){
		tools.logs("timeout sms center");
		var sc = smsCenterController;
		sc.smsList.clear();
		sc.SMSRecordsList.clear();
		
		sc.phoneNum = "";
		sc.threadId = 0;
		sc.phoneTitle = "";
		sc.imMessage = "";
		sc.newNum = "";
		sc.newMessage = "";
		module.time = null;
	};
	
	module.unload = function(){                                                                                    
		module.time = Date.parse(new Date());
	};
	
	module.refresh = function(){
		smsCenterController.clearSelected();
		smsCenterController.smsList = [];
		smsCenterController.SMSRecordsList = [];
		module.load();
	};
	
	module.onResize = function(){
		module.api.reinitialise();
		module.recordsApi.reinitialise();
	};
	
	var smsCenterController = avalon.define("smsCenter", function(vm){
		vm.smsList = [];
		
//		 是否有新信息
		vm.hasNewSMS = false;
		
		// 短信总数与未读数
		vm.totalCount = 0;
		vm.unreadCount = 0;
		vm.unread_seq = {};
		
		// 被选中的短信会话列表
		vm.selectedItem = [];
		vm.selectedClass = [];
		vm.seletAll = appLanguage["changeLang"]["selectAll"];
		
		// 显示即时聊天记录
		vm.isShowList = false;
		vm.isShowRecords = false;
		vm.SMSRecordsList = [];
		
		// 即时聊天记录的信息
		vm.phoneNum = "";
		// 当前会话的thread id
		vm.threadId = 0;
		vm.phoneTitle = "";
		vm.imMessage = "";
		
		// 当前会话的index
		vm.currentSession = 0;
		
		vm.clickItemCheckbox = function(e, idx){
			var self = smsCenterController;
			if(e){
				var evt = e.eventElement || e.src;
				e.stopPropagation();
			}
			
			if(self.selectedClass[idx] == 'selected'){
				self.selectedClass.set(idx, null);
				self.selectedItem.remove(idx);
			}else{
				self.selectedClass.set(idx, 'selected');
				self.selectedItem.push(idx);
			}
			
			if(self.selectedItem.length === self.smsList.length && self.smsList.length > 0){
				Config.Runtime.eventBus.emit('selectAll',{"type": 'smsModule', "code": "1"});
			}else{
				Config.Runtime.eventBus.emit('selectAll',{"type": 'smsModule', "code": "0"});
			}
			
		}
		
		vm.clickSMSListItem = function(idx, e){
			var self = smsCenterController;
			if(e){
				var evt = e.eventElement || e.src;
				e.stopPropagation();
			}
			
//			self.selectedItem.remove(module.lastSelectedItem); 
			
			if(self.selectedClass[idx] == 'selected'){
				self.currentSession = -1;
				self.selectedItem.remove(idx);
				self.selectedClass.splice(idx, 1, null);
				return;
			}else{
				self.currentSession = idx;
				
				self.loadCurrentSession();
				
				self.selectedItem = [];
				self.selectedItem.push(idx);
				
				tools.logs("self.selectedClass = " + self.selectedClass, "module.lastSelectedItem = " + module.lastSelectedItem, "idx= " + idx);
				
				self.selectedClass = [];
				self.selectedClass.set(idx, 'selected');
				
				module.lastSelectedItem = idx;
				
			}
			
			if(self.selectedItem.length === self.smsList.length && self.smsList.length > 0){
				Config.Runtime.eventBus.emit('selectAll',{"type": 'smsModule', "code": "1"});
			}else{
				Config.Runtime.eventBus.emit('selectAll',{"type": 'smsModule', "code": "0"});
			}
			
			if(self.unreadCount > 0 && self.unread_seq[self.smsList[idx].thread_id] != undefined){
				tools.logs("self.unreadCount = " + self.unreadCount);
				self.unreadCount --;
				self.hasNewSMS = true;
				Config.Runtime.eventBus.emit('hasNewSMS',{'data':'0'});
			}
			if(self.unreadCount == 0){
				self.unreadCount = 0;
				self.hasNewSMS = false;
				Config.Runtime.eventBus.emit('hasNewSMS',{'data':'1'});
			}
			
			self.smsList[idx].read = '1';
			
		};
		
		vm.clearSelected = function(){
			var self = smsCenterController;
			
			self.selectedItem.splice(0, self.selectedItem.length);
			self.selectedClass.splice(0, self.selectedClass.length);
		};
		
//		发送即时信息
		vm.clickReplySMS = function(e){
			var self = smsCenterController;
			
			if(self.imMessage != ""){
				module.replaySMS(self.threadId, self.imMessage);
				module.clearImMessage();
			}else{
//				var t = new tipMsg({"title": appLanguage["changeLang"]["errorTip"], "content": appLanguage["changeLang"]["errorTipDes"], "buttonCounts": 1});
				tools.tipMsg(appLanguage["changeLang"]["errorTipDes"]);
			}
		};
		
		vm.pressReplaySMS = function(e){
			var self = smsCenterController;
			var e = e || window.event;
			var keyCode = e.keyCode || e.which || e.charCode;
			
			if(keyCode == 13){
				if(self.imMessage != ""){
					module.replaySMS(self.threadId, self.imMessage);
					module.clearImMessage();
				}else{
//					var t = new tipMsg({"title": appLanguage["changeLang"]["errorTip"], "content": appLanguage["changeLang"]["errorTipDes"], "buttonCounts": 1});
					tools.tipMsg(appLanguage["changeLang"]["errorTipDes"]);
				}
			}
		};
		
		vm.onSMSSendStatus = function(data){
			tools.logs("SMS send status = " + data);
			var self = smsCenterController;
			if(data == "1"){
				// 当前会话的threadId 与 当前发送信息的thread_id 是否相同
				if(self.threadId == module.curData.thread_id){
					var oneDay = 24 * 60 * 60 * 1000;
					var today = new Date(tools.getDate("yyyy/MM/dd 00:00:00")).getTime();
					
					var lastTime = new Date(module.rawDate[0].date).getTime();
					var curTime = new Date(module.curData.date).getTime();
					// 5分钟
					var dur = 5 * 60 * 1000;
					
//					self.SMSRecordsList.push(module.curData);
					
					var cd = $.extend(true, {}, module.curData);
					module.rawDate.unshift(cd);
					
					var newDate = module.curData.date.split(" ")[1];
					if(lastTime > today){
						if(curTime - lastTime > dur){
							module.curData.date = newDate;
							self.SMSRecordsList.push(module.curData);
						}else{
							module.curData.date = newDate;
							self.SMSRecordsList.push(module.curData);
						}
					}else{
						self.SMSRecordsList.push(module.curData);
					}
					
					module.dateArr.unshift(module.curData);
					tools.tipMsg(appLanguage["changeLang"]["sendSMSSuccessful"]);
					module.hasReplay = true;
				}else{
					for(var i = 0; i < self.smsList.length; i++){
						if(self.smsList[i].tel == module.curData.tel){
							for(j in module.curData){
								self.smsList[i][j] = module.curData[j];
							}
							self.clickSMSListItem(i);
							return;
						}
					}
					
					module.refresh();
//					self.smsList.unshift(module.curData);
				}
				module.api.reinitialise();
				module.recordsApi.reinitialise();
			}else if(data == '-1'){
				tools.tipMsg(appLanguage["changeLang"]["sendSMSFailed2"]);
			}else{
				tools.tipMsg(appLanguage["changeLang"]["sendSMSFailed1"]);
			}
		};
		
		vm.onSMSList = function(data){
			var self = smsCenterController;
			var list = [];
			var len = 20;
			
			self.smsList = [];
			
			debug.clogs("get sms before = " + new Date().getSeconds() + " : " + new Date().getUTCMilliseconds());
			
			list = tools.parseJson(data.showSMSList);
			
			if(list.length < 1){
//				Config.Runtime.eventBus.emit('smsIsEmpty', true);
				self.isShowList = false;
				return;
			}
			self.isShowList = true;
//			Config.Runtime.eventBus.emit('smsIsEmpty', false);
			
			tools.fadeInOut("smsModule_file_list", 300);
			
			self.unreadCount = 0;
			
			var hasLoaded = false;
			var addData = function(index){
				var begin = index * len;
				var end = len * (index + 1) > list.length ? list.length : len * (index + 1);
				
				var partList = list.slice(begin, end);
				self.smsList.splice.apply(self.smsList, [begin, end].concat(partList))
				debug.clogs("get sms processing = " + new Date().getSeconds() + " : " + new Date().getUTCMilliseconds());
				
				for(var i = begin; i < end; i++){
					if(list[i].read == '0'){
						self.unread_seq[list[i].thread_id] = i;
						self.unreadCount ++;
					}
				}
				
				if(self.unreadCount > 0){
					self.hasNewSMS = true;
					Config.Runtime.eventBus.emit('hasNewSMS',{'data':'0'});
				}else{
					self.hasNewSMS = false;
					Config.Runtime.eventBus.emit('hasNewSMS',{'data':'1'});
				}
				
				if(self.currentSession >= begin && self.currentSession <= end){
					if(module.hasReplay){
						self.loadCurrentSession(0);
						module.hasReplay = false;
					}else{
						self.loadCurrentSession();
					}
					hasLoaded = true;
				}else if(!hasLoaded){
					var listLen = list.length - 1;
					if(self.currentSession > listLen){
						self.loadCurrentSession(listLen);
					}
				}
			};
			
			var listLength = list.length;
			var indexArr = [];
			for(var i = 0; i < list.length / len; i ++){
				indexArr.push(i);
			}
			
			var index = 0;
			var setList = function(){
				
				if(listLength <= len){
					addData(indexArr[0]);
					return;
				}
				
				addData(indexArr[index++]);
				module.api.reinitialise();
				
				var s = setTimeout(setList, 300);
				if(index * len >= listLength){
					debug.clogs("get sms after = " + new Date().getSeconds() + " : " + new Date().getUTCMilliseconds());
					clearTimeout(s);
//					module.api.reinitialise();
				}
			};
			
			setList();
			
		};
		
		vm.loadCurrentSession = function(idx){
			var self = smsCenterController;
			
			if(self.smsList.length < 1){
				self.clearSelected();
				return;
			}
			
			if(idx != undefined){
				self.currentSession = idx;
			}
			
			// 加载当前session内容
			self.threadId = self.smsList[self.currentSession].thread_id;
			self.phoneNum = self.smsList[self.currentSession].tel;
			self.phoneTitle = self.smsList[self.currentSession].title;
			
			// 设置为已读
			self.smsList[self.currentSession].read = 1;
			
			module.sendSessionThreadId(self.threadId);
//			self.clickSMSListItem(self.currentSession);
//			$("#sms_list li").removeClass("selected");
//			$("#sms_list li").eq(self.currentSession).addClass("selected");
			
			self.selectedItem = [];
			self.selectedItem.push(self.currentSession);
			
			self.selectedClass = [];
			self.selectedClass.set(self.currentSession, 'selected');
		};
		
		vm.onTelSMSList = function(data){
			module.showTelSMSList(tools.parseJson(data.SMSList));
		};
		
		vm.onSMSNew = function(data){
			var self = smsCenterController;
			
			self.hasNewSMS = true;
			Config.Runtime.eventBus.emit('hasNewSMS',{'data':'0'});
			
			var d = data;
			// 当前会话有新信息
			if(self.threadId == d.thread_id){
				self.SMSRecordsList.push(d);
				// 信息来自当前会话，标记为已读
//				d.read = 1;
				for(var i=0; i<self.smsList.length; i++){
					if(self.smsList[i].thread_id == d.thread_id && i == 0){
						tools.logs("当前为第一条会话记录！");
						self.smsList.splice(0, 1, d);
						return;
					}
					
					if(self.smsList[i].thread_id == d.thread_id){
						self.smsList.splice(i, 1);
						self.smsList.unshift(d);
						return;
					}
				}
			}else{
				for(var i=0; i<self.smsList.length; i++){
					if(self.smsList[i].thread_id == d.thread_id){
						self.smsList.splice(i, 1);
						self.smsList.unshift(d);
						return;
					}
				}
				
				self.smsList.unshift(d);
			}
			
			module.api.reinitialise();
		};
		
		vm.onCurrentSMS = function(data){
			module.curData = data.data;
		};
		
		vm.onSMSDeleteStatus = function(data){
			var self = smsCenterController;
			
			if(data == "1"){
				tools.tipMsg(appLanguage["changeLang"]["deleteSuccessDes"]);
			}else{
				tools.tipMsg(appLanguage["changeLang"]["deleteErrorDes"]);
			}
		};
		
		vm.onLoadMessage = function(data){
			if(data.data == 'sms'){
				tools.logs("load sms");
				module.load();
			}
		};
		
		vm.onSelectAll = function(data){
			var self = smsCenterController;
			
			if(data.type == 'sms'){
				self.clearSelected();
				if(data.code == '1'){
					var items = [];
					var sc = [];
					for(var i=0; i<self.smsList.length; i++){
						items.push(i);
						sc.push("selected")
					}
					self.selectedItem = items;
					self.selectedClass.splice.apply(self.selectedClass, [0, self.selectedClass.length].concat(sc));
				}else{
					self.selectedClass = [];
				}
			}
		};
		
		vm.onLanguage = function(data){
			appLanguage["changeLang"] = data;
			module.setLanguage();
		};
		
		vm.initListeners = function(bus){
			var self = smsCenterController;
			bus.on("SMSList", self.onSMSList);
			bus.on("SMSSendStatus", self.onSMSSendStatus);
			bus.on("SMSDeleteStatus", self.onSMSDeleteStatus);
			bus.on("TelSMSList", self.onTelSMSList);
			bus.on("SMSNew", self.onSMSNew);
			bus.on("currentSMS", self.onCurrentSMS);
			bus.on("loadMessage", self.onLoadMessage);
			bus.on("selectAll", self.onSelectAll);
			bus.on("language", self.onLanguage);
			
			Config.Runtime.eventBus.emit('addModel',module);
		};
	});
	
	smsCenterController.selectedItem.$watch("length", function(s){
		var self = smsCenterController;
		if(self.selectedItem.length == 0){
			self.SMSRecordsList = [];
			self.isShowRecords = false;
			
			module.barButtons[1].code = module.barButtons[1].code + "_disable";
			Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: "sms"});
			Config.Runtime.eventBus.emit('selectAll',{"type": 'smsModule', "code": "0"});
			return;
		}
		
		if(self.selectedItem.length < self.smsList.length){
			Config.Runtime.eventBus.emit('selectAll',{"type": 'smsModule', "code": "0"});
		}else if(self.selectedItem.length === self.smsList.length && self.selectedItem.length > 0){
			Config.Runtime.eventBus.emit('selectAll',{"type": 'smsModule', "code": "1"});
		}
		
		if(this.length > 0){
			for(var i = 0; i < module.barButtons.length; i++){
				 module.barButtons[i].code = module.barButtons[i].code.replace("_disable", "");
			}
		}else{
			for(var i = 0; i < module.barButtons.length; i++){
				if(i == 0 || i == module.barButtons.length-1){
					continue;
				}
				module.barButtons[i].code = module.barButtons[i].code + "_disable";
			}
		}
		
		Config.Runtime.eventBus.emit('buttonStatus',{data: module.barButtons, fileType: "sms"});
	});
	
	smsCenterController.SMSRecordsList.$watch("length", function(s){
		module.recordsApi.reinitialise();
		module.recordsApi.scrollToBottom();
	});
	
	/** SMS */
	module.lastSelectedItem = -1;
	module.curData = {};
	module.rawDate = []; 	// 处理前的数据
	module.dateArr = [];		// 处理后的数据
	module.replaySMS = function(threadId, imMessage){
		var m = {};
		m.type = "SMSReply";
		m.data = {"thread_id":threadId, "content":imMessage, "date":tools.getDateTime(), "read":"1", "type":"2"};
		tools.sendMsgToChannel(m);
		
		module.curData = m.data;
	};
	
	module.clearImMessage = function(){
		smsCenterController.imMessage = "";
	};
	
	module.sendSessionThreadId = function(threadId){
		var m = {};
		m.type = "requestTelSMSList";
		m.data = {"thread_id":threadId};
		tools.sendMsgToChannel(m);
	};
	
	module.showTelSMSList = function(data){
		var sc = smsCenterController;
		
		if(data == "" || data.length < 1){
			sc.isShowRecords = false;
			return;
		}
		
		sc.SMSRecordsList = [];
		module.dateArr = [];
		module.rawDate = [];
		sc.isShowRecords = true;
		debug.clogs("sms session begin = " + new Date().getSeconds() + " : " + new Date().getUTCMilliseconds());
		
		var processDate = function(){
			// 86400000
			var oneDay = 86400000;
			var today = new Date(tools.getDate("yyyy/MM/dd 00:00:00")).getTime();
			tools.logs("today = " + today);
			
			// 得到每条信息的0点0时0分毫秒值
			var dayArr = [];
			for(var i=0; i<data.length; i++){
				data[i].date = data[i].date.replace(/-/g, "/");
				var rd = $.extend(true, {}, data[i]);
				module.rawDate.push(rd);
				
				var d = data[i].date.split(" ")[0];
				dayArr.push(new Date(d + " 00:00:00").getTime());
			}
			
			debug.clogs("sms session processing = " + new Date().getSeconds() + " : " + new Date().getUTCMilliseconds());

			var cd = dayArr[0];
			for(var i = 1; i < dayArr.length; i++){
				if(today - dayArr[i] < oneDay){
					var n = data[i - 1];
					n.date = data[i - 1].date.split(" ")[1];
					module.dateArr.push(n);
					
					cd = dayArr[i - 1];
					continue;
				}
				
				if((cd - dayArr[i]) >= oneDay){
					cd = dayArr[i];
					
					var n = data[i - 1];
					module.dateArr.push(n);
				}else{
					data[i - 1].date = "";
					module.dateArr.push(data[i - 1]);	
				}
			}
		
			module.dateArr.push(data[data.length - 1]);
		}
		
		processDate();

		/**
		 * 延时加载
		 * 
		 */
		var len = 15;
		var begin = 0;
		var end = 0;
		var partList = [];
		var all = [];
		var rev = [];
		var bottom = 0;
		var count = 1;
		var setData = function(){
			end = end + len;
			tools.logs("module.dateArr[0] = " + module.dateArr[0].date, "module.dateArr[module.dateArr.length - 1] = " + module.dateArr[module.dateArr.length - 1].date);
			partList = module.dateArr.slice(begin, end);
			
			partList.reverse();		//把最新的信息放到最后
			
			sc.SMSRecordsList = partList;
			
		};
		
		setData();
		
//		setTimeout(function(){module.recordsApi.reinitialise();module.recordsApi.scrollToBottom();}, 500);
		
		var y = 0;
		$("#tel_sms_list").unbind('jsp-scroll-y').bind('jsp-scroll-y', function(event, scrollPositionY, isAtTop, isAtBottom){
			tools.logs("scroll position ..." + y, "isAtTop = " + isAtTop, "isAtBottom = " + isAtBottom);
			
			if(module.dateArr.length <= sc.SMSRecordsList.length)
				return;
			
			if(isAtBottom){
				bottom = scrollPositionY;
			}
			
			if(isAtTop && y > 0 && y < 100 && !isAtBottom){
				tools.logs("goooooooooooooooooooooo");
				setData();
				count++;
				module.scrPos = bottom / count;
				tools.logs("module.scrPos = " + module.scrPos);
				module.recordsApi.scrollTo(0, module.scrPos);
//				y = scrollPositionY;
			}else{
				y = scrollPositionY;
				return;
			}
		});
		
		/**
		 * 全部加载
		 */
//		sc.SMSRecordsList = module.dateArr.reverse();
		debug.clogs("sms session end = " + new Date().getSeconds() + " : " + new Date().getUTCMilliseconds());
		
	};
	
	module.deleteSMS = function(){
		var sms = smsCenterController;
		var threadIds = '';
		
		// 没有选中任何信息
		if(sms.selectedItem.length <= 0){
			tools.logs("self.selectedItem.length <= 0");
			return;
		}
		
		tools.logs("self.selectedItem init = " + sms.selectedItem);
		
		var sendDeleteMsg = function(){
			// 合并thread_id成字符串
			for(var i = 0; i < sms.selectedItem.length; i++){
				threadIds += sms.smsList[sms.selectedItem[i]].thread_id + ",";
			}
			
			threadIds = threadIds.substring(0, threadIds.length-1);
			
			tools.logs("threadIds = " + threadIds);
			
			// 信息中包含非法的thread_id
			if(threadIds.indexOf(undefined) != -1)
				return;
			
			// 发送删除命令
			var m = {};
			m.type = 'SMSDelete';
			m.data = {"thread_id": threadIds};
			tools.sendMsgToChannel(m);
		};
		
		sendDeleteMsg();
		
		var deleteFromList = function(){
			var slist = sms.smsList.slice(0);
			// 重新排序
			sms.selectedItem = sms.selectedItem.sort(module.compare);
			tools.logs("sms.selectedItem compare = " + sms.selectedItem);
			
			for(var j = 0; j < sms.selectedItem.length; j++){
				
				if(sms.currentSession == sms.selectedItem[j]){
					sms.currentSession = 0;
				}
				
				slist.splice(sms.selectedItem[j], 1);
			}
			
			sms.smsList = slist;
			setTimeout(function(){module.api.reinitialise();}, 200);
			
			if(sms.smsList.length === 0){
				sms.currentSession = -1;
				sms.isShowList = false;
			}
			
			sms.clearSelected();
			
			if(sms.currentSession > slist.length - 1){
				sms.currentSession = slist.length - 1;
			}else if(sms.currentSession < 0){
				sms.currentSession = 0;
			}
			
			sms.loadCurrentSession();
		};
		
		var finished = false;
		$("#sms_list li.selected").slideUp(300, function(){
			if(!finished){
				deleteFromList();
				finished = true;
			}
		});
		
	};
	module.compare = function(v1, v2){
		if(v1 > v2){
			return -1;
		}
		if(v2 > v1){
			return 1;
		}else{
			return 0;
		}
	};
	
	return module;
});

