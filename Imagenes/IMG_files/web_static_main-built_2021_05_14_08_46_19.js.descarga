                bus.on("ack", self.onAck);
                //	bus.on("phoneInfo", module.onPhoneInfo);
                bus.on("introInfo", module.onIntroInfo);

                Config.Runtime.eventBus.emit('addModel', module);

                self.setMeelFun() // 

            };
        });
        module.imgC = imgModuleController;

        return module;
    });

define('text!../template/video-module.html',[],function () { return '<!-- video model -->\n<div id="videoModule" class="videoModule modules" ms-controller="videoModule" onselectstart="return false">\n    <div class="tab_line" ms-class="{{selecteClass}}">\n        <b class="check_box" ms-hover="hover" ms-click="clickSelectGroupImg()"></b>\n        <span class="select_all_text" style="display: block;">{{changeLang[\'selectAll\']}}</span>\n        <ul class="img_list_tab list_tab only_one">\n            <li class="current" id="phoneVideo">{{changeLang[\'phoneVideo\']}}</li>\n        </ul>\n    </div>\n    <div class="bottom_line"></div>\n\n    <div id="videoModule_file_list" class="img_list" style="width:100%" ms-css-height="rightListHeight">\n        <div>\n\n            <div class="icon_list_item img_list_item" ms-class="{{posterUrls.css[$index] ? posterUrls.css[$index] : \'\'}}" ms-repeat="posterUrls.srcs" ms-hover="hover" style="width:180px;height:180px" ms-click="clickAddClass($index , $event)" ms-mouseover="mouseOverName($index)"\n                ms-mouseout="mouseOutName($index)">\n                <div class="img_container">\n                    <img onerror="this.src=\'srcimg/ic_apk_default.png\'" ms-src="{{posterUrls.srcs[$index]}}" ms-css-width="{{posterUrls.widths[$index]}}" ms-css-height="{{posterUrls.heights[$index]}}" ms-css-margin-left="{{posterUrls.marginLefts[$index]}}" ms-css-margin-top="{{posterUrls.marginTops[$index]}}"\n                        ms-mouseover="mouseOverImg($index)" ms-mouseout="mouseOutImg($index)" />\n                </div>\n\n                <div class="video_item_big_btn" ms-click="dblclickVideo($index, $event)" style="display:none"></div>\n\n                <div class="icon_checkbox" ms-hover="icon_checkbox_hover"></div>\n\n                <div class="icon_btn_bar_bg"></div>\n                <div class="videoName" ms-visible="posterUrls.videoNameVisible[$index]">\n                    <p>{{posterUrls.name[$index]}}</p>\n                </div>\n                <dl class="icon_btn_bar">\n                    <dd class="download" ms-click="clickDownload($index, $event)"></dd>\n                    <dd class="delete_item" ms-visible="noneIos!=\'weline_ios_xxx\'" ms-click="deleteSingleVideo($index, $event)"></dd>\n                </dl>\n            </div>\n\n        </div>\n\n    </div>\n    <div class="empty_list" ms-visible="!isShowVideo" style="top:25px ;">\n        <div id="img_video" class="empty_img empty_message" style="background:url(img/btn_empty_video.png) no-repeat"></div>\n    </div>\n</div>';});


