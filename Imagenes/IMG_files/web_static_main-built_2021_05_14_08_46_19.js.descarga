	var TipMsg = function(s){
		this.title = appLanguage['changeLang']["tipTitle"];
		this.content = appLanguage['changeLang']["tipContent"];
		this.buttonCounts = 2;
		this.tWidth = 400;
		this.tHeight = 'auto';
		this.tipTheme = 'green_the'; // #8dca35: green_the, #da3610: red_the
		this.settings = {};
		this.init(s);
		this.tip();
	};
	
	/**
	 * settings = {title, content, buttonCounts, fn()}
	 */
	TipMsg.prototype.init = function(settings){

		var self = this;
		
		if(settings != undefined){
			this.settings = settings;
		}

		this.fnParam = this.settings.fnParam ? this.settings.fnParam : null;
		this.title = this.settings.title ? this.settings.title : this.title;
		this.content = this.settings.content ? this.settings.content : this.content;
		self.comfirm = this.settings.fn ? this.settings.fn : this.comfirm;
		this.tWidth = this.settings.tWidth ? this.settings.tWidth : this.tWidth;
		this.tHeight = this.settings.tHeight ? this.settings.tHeight : this.tHeight;
		this.buttonCounts = this.settings.buttonCounts == undefined ? this.buttonCounts : this.settings.buttonCounts;
		this.tipTheme = this.settings.tipTheme == undefined ? this.tipTheme : this.settings.tipTheme;
		
		if(document.getElementById("tipMsg")){
			self.close();
		}
		
		var block = document.createElement("div");
		block.id = "blockTip";
		block.className = "blockTip";
		block.style.width = tools.getPageWidth();
		block.style.height = tools.getPageHeight();
		
		var tc = document.createElement("div");
		tc.id = "tipMsg";
