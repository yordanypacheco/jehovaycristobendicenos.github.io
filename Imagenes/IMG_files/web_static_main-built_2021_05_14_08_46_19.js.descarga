        vm.deleteSelectMusic = function() {
            var self = musicModuleController;
            var dataFile = [];
            var deleteListening = false; // 删除的是否是当前播放的音乐
            var data = [];
            var deleteItems = [];

            var deleteMulti = function() {
                self.selectedItem.sort(function(x, y) {
                    if (x > y) {
                        return -1;
                    } else if (x < y) {
                        return 1;
                    } else {
                        return 0;
                    }
                });
                tools.logs("self.selectedItem:", self.selectedItem);
                for (var i = 0; i < self.selectedItem.length; i++) {
                    var index = self.selectedItem[i];
                    var path = self.musicList[index].path;

                    deleteItems.push(path);

                    if (index == self.curIdx) {
                        deleteListening = true;
                    }
                    tools.logs("index index index :", index);
                    data.push(path);

                    self.musicList.splice(index, 1);
                }
                self.clearSelected();

                if (self.musicList.length === 0) {
                    module.stopMusic();
                    self.isEmpty = true;
                    module.api.reinitialise();
                    self.selectedItem = [];
                    self.curIdx = -1;
                    self.curPath = "";

                    return;
                }
                self.isEmpty = false;
                Config.Runtime.eventBus.emit("setCount", {
                    module: "musicModule",
                    count: self.musicList.length
                });
                if (self.isAudition) {
                    if (deleteListening) {
                        if (self.musicList.length >= self.curIdx) {
                            self.curIdx = 0;
                            module.sendRequestFile(self.curIdx);
                        } else {
                            self.isAudition = false;

                            if (self.musicList.length <= 0) {
                                self.curIdx = -1;
                                self.curPath = "";
                                $("#jquery_jplayer_1").jPlayer("clearMedia");
                            } else {
                                self.curIdx = self.musicList.length - 1;
                                $("#jquery_jplayer_1").jPlayer("stop");
                            }
                        }
                    }
                } else {
                    if (deleteListening) {
                        if (self.musicList.length > self.curIdx) {
                            module.sendRequestFile(self.curIdx);
                        } else {
                            if (self.musicList.length <= 0) {
                                self.curIdx = -1;
                                self.curPath = "";
                                $("#jquery_jplayer_1").jPlayer("clearMedia");
                            } else {
                                self.curIdx = self.musicList.length - 1;
                            }
                        }
                    }
                }
            };

            var finished = false;
            $("#list_body ul.selected").slideUp(300, function() {
                if (!finished) {
                    deleteMulti();
                    self.delMusicSendMesg(deleteItems);
                    module.api.reinitialise();
                    self.selectedItem = [];
                    finished = true;
                }
            });
        };

        vm.clickSort = function(v, e) {
            var self = musicModuleController;

            var e = e || window.event;
            var es = e.target || e.srcElement;
            var evt = $(es).closest("li");

            var selectedPaths = [];

            for (var i = 0; i < self.selectedItem.length; i++) {
                selectedPaths.push(self.musicList[self.selectedItem[i]].path);
            }

            var sortSelected = function(musArr) {
                self.selectedClass = [];
                var index = 0;
                for (var j = 0; j < selectedPaths.length; j++) {
                    for (var i = 0; i < musArr.length; i++) {
                        if (musArr[i].path == selectedPaths[j]) {
                            self.selectedItem[index] = i;
                            self.selectedClass.set(i, "selected");
                            index++;
                        }
                    }
                }
            };

            if ($(evt).hasClass("pressed")) {
                $(evt).removeClass("pressed");
                $(evt).addClass("pressed");
            } else {
                $(evt).addClass("pressed");
            }

            var musicArray = self.musicList.slice(0);
            if (v == "name") {
                self.musicSort = 1;
            } else {
                self.musicSort = 2;
            }
            if (self.sortKey[v] != undefined) {
                if (self.sortKey[v] == "des") {
                    self.sortKey[v] = "asc";

                    musicArray.sort(function(a, b) {
                        return a[v].localeCompare(b[v]);
                    });
                    $(evt)
                        .find("span")
                        .last()
                        .removeClass()
                        .addClass("order_up");
                } else {
                    self.sortKey[v] = "des";
                    musicArray.sort(function(a, b) {
                        return b[v].localeCompare(a[v]);
                    });
                    $(evt)
                        .find("span")
                        .last()
                        .removeClass()
                        .addClass("order_down");
                }
            } else {
                self.sortKey[v] = "asc";
                musicArray.sort(function(a, b) {
                    return a[v].localeCompare(b[v]);
                });
                $(evt)
                    .find("span")
                    .last()
                    .removeClass()
                    .addClass("order_up");
            }

            self.musicList = musicArray;
            sortSelected(musicArray);

            for (var i = 0; i < self.musicList.length; i++) {
                if (self.curPath == self.musicList[i].path) {
                    self.curIdx = i;
                }
            }
        };

