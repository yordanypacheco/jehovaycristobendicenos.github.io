define('drawCircle',['tools', 'config'], function(tools, Config){
	Config.Runtime.drawList = {};
	var DrawCircle = function(parentId, color){
		this.x = 51;
		this.y = 50;
		this.st = null;
		this.dur = 10;
		this.lineWidth = 2;
		this.deg = 0;
		this.lastDeg = 0;
		this.cav = null;
		this.cavCxt = null;
		this.pid = document.getElementById(parentId);
		this.color = color ? color : 'rgba(40, 160, 255, 1)';
		this.createCav();
		this.timeout = null;
	};
	
	DrawCircle.prototype.createCav = function(){
		var d = this;
		d.cav = document.createElement('canvas');
		d.cav.id = "circle_" + new Date().getTime();
		d.cav.width = 100;  // 102
		d.cav.height = 100; // 130
			
		d.pid.appendChild(d.cav);
		d.cavCxt = this.cav.getContext('2d');
		
		d.pid.style.display = "block";
	};
	
	DrawCircle.prototype.draw = function(){
		var d = this;
		
		d.cavCxt.beginPath();
		d.cavCxt.arc(d.x, d.y, 49, Math.PI * 1 / 180, Math.PI * 1 / 180);
		d.cavCxt.lineWidth = d.lineWidth;
		d.cavCxt.lineCap = '';//'round';
		d.cavCxt.strokeStyle = d.color;
		d.cavCxt.stroke();	
		d.cavCxt.closePath();
	};
	
	DrawCircle.prototype.drawArc = function(){
		var d = this;
		
		d.cavCxt.beginPath();
		d.cavCxt.arc(d.x, d.y, 49, Math.PI * 45 / 180, Math.PI * 135 / 180);
		d.cavCxt.lineWidth = d.lineWidth;
		d.cavCxt.lineCap = '';//'round';
		d.cavCxt.strokeStyle = d.color;
		d.cavCxt.stroke();	
		d.cavCxt.closePath();
	};
	
	DrawCircle.prototype.addDegree = function(lastDeg, degree){
		var d = this;
		
		d.cavCxt.beginPath();
		d.cavCxt.arc(d.x, d.y, 49, Math.PI * lastDeg / 180, Math.PI * degree / 180);
		d.cavCxt.lineWidth = d.lineWidth;
		d.cavCxt.lineCap = '';//'round';
		d.cavCxt.strokeStyle = d.color;
		d.cavCxt.stroke();
		d.cavCxt.closePath();
	};
	
	DrawCircle.prototype.clearDegree = function(lastDeg){
		var d = this;
		
		d.cavCxt.beginPath();
		d.cavCxt.arc(d.x, d.y, 49, Math.PI * (lastDeg-d.dur*9) / 180, Math.PI * (lastDeg-d.dur*8) / 180);
		d.cavCxt.lineWidth = d.lineWidth + 2;
		d.cavCxt.lineCap = '';//'round';
		d.cavCxt.strokeStyle = 'rgba(255, 255, 255, 1)';
		d.cavCxt.stroke();
		
		d.cavCxt.beginPath();
		d.cavCxt.arc(d.x, d.y, 50, Math.PI * (lastDeg-d.dur*9) / 180, Math.PI * (lastDeg-d.dur*8) / 180);
		d.cavCxt.lineWidth = 1;
		d.cavCxt.lineCap = '';//'round';
		d.cavCxt.strokeStyle = 'rgba(211, 221, 230, 1)';
		d.cavCxt.stroke();
	};
	
	DrawCircle.prototype.rotate = function(){
		var d = this;
		d.cav.className = "backupRecoveryRotate";
	};
	
	DrawCircle.prototype.rotate2 = function(){
		var d = this;
		var deg = 0;
		
		var rotateObj = function(speed, circle, obj){
			
			var dg = deg * speed;
			
			if(dg / 360 > circle){
				deg = 0;
				return;
			}
			
			deg += 1;
			
			$(obj).css({
				"transform":"rotate(" + dg + "deg)",
				"-ms-transform":"rotate(" + dg + "deg)",
				"-moz-transform":"rotate(" + dg + "deg)",
				"-webkit-transform":"rotate(" + dg + "deg)",
				"-o-transform":"rotate(" + dg + "deg)"
			});
			tools.logs("degree = " + dg);
			d.timeout = setTimeout(function(){rotateObj(speed, circle, obj)}, 30);
		};
		
		rotateObj(8, 1000, d.cav);
	};
	
	DrawCircle.prototype.run = function(){
		
		var d = this;
		
		var cssOrJs = function(){
			d.draw();
			d.drawArc();
			
			/**
			 * canvas 
			var r = function(){

				d.lastDeg = d.deg;
				d.deg += d.dur;
				d.addDegree(d.lastDeg, d.deg);
				d.clearDegree(d.lastDeg);
				
				d.st = setTimeout(r, 20);
			};
			r();
			*/
			
			// css3
			d.rotate();
		};
		
		var cssAnJs = function(){
			d.draw();
			d.drawArc();
			d.rotate2();
		};
		
		if(navigator.userAgent.indexOf('MSIE') > 0){
			var bv = navigator.userAgent.substring(navigator.userAgent.indexOf('MSIE')+5, navigator.userAgent.indexOf('MSIE')+7);
			if(9 >= bv && bv < 10){
				tools.logs(9 >= bv);
				tools.logs(bv < 10);
				cssAnJs();
			}else if(bv >= 10){
				cssOrJs();
			}else{
				return;
			}
		}else{
			cssOrJs();
		}
		
	};
	
	DrawCircle.prototype.stop = function(){
		tools.logs("stop draw");
		var d = this;
		/**
		clearInterval(d.st);
		*/
		d.cav.parentNode.removeChild(d.cav);
		this.pid.style.display = "none";
		clearTimeout(d.timeout);
	};
	
	return DrawCircle;
});
