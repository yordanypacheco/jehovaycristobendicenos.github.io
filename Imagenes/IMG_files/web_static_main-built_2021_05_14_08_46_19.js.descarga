            vm.setGroupPicSelect = function(index, that, flag, e) {
                if (e) {
                    e.stopPropagation();
                }
                self = imgModuleController;
                var arr = [];
                var dom = e.target.parentNode.parentNode;
                var domChild = dom.children;

                var start = self.albums[index].start;

                if (flag == 0) {
                    //self.albums[index].css=[];
                    var back = [];
                    for (var i = 0; i < self.albums[index].$src.length; i++) {
                        back.push("animScaleBack");

                        for (var j = 0; j < self.currentSelect.length; j++) {
                            if (start + i == self.currentSelect[j].index) {
                                self.currentSelect.splice(j, 1);
                                break;
                            }
                        }
                    }
                    self.albums[index].css = back;
                } else {
                    for (var i = 0; i < self.albums[index].$src.length; i++) {
                        arr.push("selected animScale");
                        var f = true;
                        for (var j = 0; j < self.currentSelect.length; j++) {
                            if (start + i == self.currentSelect[j].index) { f = false; break; };
                        }
                        var t = {
                            index: start + i,
                            src: self.albums[index].srcs[i]
                        }
                        if (domChild[i + 1]) {

                            t.left = domChild[i + 1].offsetLeft || 0;
                            t.top = domChild[i + 1].offsetTop || 0;
                            t.offSettop = domChild[i + 1].offsetTop || 0;
                            t.width = domChild[i + 1].clientWidth || 0;
                            t.height = domChild[i + 1].clientHeight || 0;

                        } else {
                            t.left = 0;
                            t.top = 0;
                            t.offSettop = 0;
                            t.width = 0;
                            t.height = 0;
                        }
                        //if(f) self.currentSelect.push(index + i);
                        //console.log("t:" ,t)
                        if (f) self.currentSelect.push(t);
                    }
                    self.albums[index].css = arr;
                }

            }

